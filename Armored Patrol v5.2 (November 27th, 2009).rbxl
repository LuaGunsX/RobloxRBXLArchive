Staff.Touched:connect(onTouched)
Flag.Touched:connect(onTouched)


local CLAIM_INTERVAL = 60
local INTERVAL = .5


while true do

	wait(INTERVAL)

	if claimer then

		if checkForPlayer(claimer) then

			claimTimer = claimTimer + INTERVAL

			if claimTimer &lt; CLAIM_INTERVAL then	
				
				if Flag.BrickColor == FLAG_NEUTRAL then
					Flag.BrickColor = claimer
				else
					Flag.BrickColor = FLAG_NEUTRAL
				end

				local offset = ((claimTimer/CLAIM_INTERVAL) * (FLAG_OFFSET_HIGH-FLAG_OFFSET_LOW)) + FLAG_OFFSET_LOW
				FlagMesh.Offset = Vector3.new(-2, offset, 0)

			-- Else claim timer has expired, give hill to new owners
			else

				if _G.pegCapture then

					-- If there was a previous owner then deduct the hill value from them
					if owner ~= TEAM_NEUTRAL then
						_G.pegCapture(owner, -hillValue(owner))
					end

					-- Add the hill value to the new owners
					_G.pegCapture(claimer, hillValue(claimer))
				end

				owner = claimer
				claimer = nil
				claimTime = 0
				Flag.BrickColor = owner
				FlagMesh.Offset = Vector3.new(-2,FLAG_OFFSET_HIGH,0)
			end

		-- Players team is not still on hill, stop claiming process, revert flag to original owner color
		else
			claimer = nil
			claimTime = 0
			Flag.BrickColor = owner
			FlagMesh.Offset = Vector3.new(-2,FLAG_OFFSET_HIGH,0)
		end
	end
end
