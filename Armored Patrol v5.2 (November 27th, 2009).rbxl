--------------------------------------------------------------------------------
-- For the side incline off of a main incline whose angle is mainAng, 
-- calculate the shallowest angle that the side incline can be and still be
-- inside of the adj main incline whose angle is adjAng.  Plus 15 degrees
-- to make interrior more open.

-- The mainAng and adjAng are in radians, result in  radians.

local function calcSideAngle( mainAng, adjAng)

	return math.atan(math.tan(adjAng)/math.cos(mainAng)) + math.rad(15)

end


-------------------------------------------------------------------------------------
local function calcOneCornerAttr(ca, sideAngle, adjSideAngle, height)

	ca.angle = calcSideAngle(sideAngle, adjSideAngle)


	local ls = 10

	local ws = 0

	ca.ws = ws
	ca.ls = ls
end

-----------------------------------------------------------------------------------

local function calcCornerAttr(ho)

	height = ho.height

	calcOneCornerAttr(ho.nnw, ho.n.angle, ho.w.angle, height)
	calcOneCornerAttr(ho.nne, ho.n.angle, ho.e.angle, height)
	calcOneCornerAttr(ho.ene, ho.e.angle, ho.n.angle, height)
	calcOneCornerAttr(ho.ese, ho.e.angle, ho.s.angle, height)
	calcOneCornerAttr(ho.sse, ho.s.angle, ho.e.angle, height)
	calcOneCornerAttr(ho.ssw, ho.s.angle, ho.w.angle, height)
	calcOneCornerAttr(ho.wsw, ho.w.angle, ho.s.angle, height)
	calcOneCornerAttr(ho.wnw, ho.w.angle, ho.n.angle, height)
end

