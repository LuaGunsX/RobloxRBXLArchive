							<string name="Source">----------------------------------------------------------------------------------------------------
--2008/10/18 Attach vehicle to Player at Seat weld time
-- 2008/11/17 Enforce one vehicle per player rule
--2009/4/12 Separate driver and gunner seats, Do not attach vehicle to Player 
--2009/4/28 Do attached vehicle to Player, record Player in CntrDriver CntrGunner
--2009/5/6 Change to CntrUser convention
--2009/5/20 Implement CntrOwner and CntrOwnerSeq
--2009/5/27 CntrGunner not set by seat weld but by tool selection

-- Features:

	-- Enforce first come - first seated for the seats

	-- Record the Player in the CntrUser values (not in CntrGunner) on attach

	-- Remove Player from CntrUser and CntrGunner on detach

	-- Move vehicle under character when seatweld to driver seat, move back
	--	to game.Workspace when detached

	-- On attached record Player in CntrOwner




local Vehicle = script.Parent
local CntrOwner = Vehicle.CntrOwner
local CntrOwnerSeq = Vehicle.CntrOwnerSeq
local CntrUser = Vehicle.CntrUser
local CntrGunner = Vehicle.CntrGunner
local SeatDriver = Vehicle.SeatDriver
local SeatGunner = Vehicle.SeatGunner

local driverAttached = nil
local gunnerAttached = nil


----------------------------------------------------------------------------------------------------

function onChildAdded(seat, part)

	if part.className == &quot;Weld&quot; then

		local torso = part.Part1

		if not torso then
			return
		end

		local character = torso.Parent

		if not character then
			return
		end

		local Player = game.Players:playerFromCharacter(character)

		if not Player then
			return
		end

		if (seat.Name == &quot;SeatDriver&quot;) then

			-- If someone already welded to seat then remove this new seat weld.
			if  driverAttached then
				part.Parent = nil
				return
			end

			driverAttached = torso
			CntrUser.Value = Player
			CntrOwner.Value = Player

			-- Assign latest owner seq number to determine order of ownership
			if _G.assignOwnerSeq then
				_G.assignOwnerSeq(CntrOwnerSeq)
			end

			-- Move the vehicle under the character
	
			Vehicle.Parent = character

		elseif (seat.Name == &quot;SeatGunner&quot;) then

			-- If someone already welded to seat then remove this new seat weld.
			if gunnerAttached then
				part.Parent = nil
				return
			end

			gunnerAttached = torso
		end
	end
end

----------------------------------------------------------------------------------------------------

function onChildRemoved(seat, part)

	if part.className == &quot;Weld&quot; then

		local torso = part.Part1
	
		if (seat.Name == &quot;SeatDriver&quot;) and (driverAttached == torso) then
			driverAttached = nil
			CntrUser.Value = nil	-- Signal tool that driver detached

			Vehicle.Parent = game.Workspace

		elseif (seat.Name == &quot;SeatGunner&quot;) and (gunnerAttached == torso) then
			gunnerAttached = nil
			CntrGunner.Value = nil	-- Signal tool that gunner detached
		end
	end
end


----------------------------------------------------------------------------------------------------
SeatDriver.ChildAdded:connect(function(part) onChildAdded(SeatDriver, part) end)
SeatDriver.ChildRemoved:connect(function(part) onChildRemoved(SeatDriver, part) end)

SeatGunner.ChildAdded:connect(function(part) onChildAdded(SeatGunner, part) end)
SeatGunner.ChildRemoved:connect(function(part) onChildRemoved(SeatGunner, part) end)



while true do

	wait(15)


end
