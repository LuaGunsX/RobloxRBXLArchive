local function onAncestryChanged(child, parent)

	--print(&quot;ancestry changed child &quot;, child, &quot; parent &quot;, parent)

	if parent == nil then
		if outstandingBullets &gt; 0 then
			outstandingBullets = outstandingBullets - 1
		end
	end
end


