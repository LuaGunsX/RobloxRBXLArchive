local function inters( sa,ta, sizeah, sb, tb, offsetb) 

	local a = sa - ((sa-ta) * ((sb-offsetb)/(sb-tb))) 
	
	if a &gt;sizeah  then 

		return false,sizeah, a-sizeah

	elseif a &lt; -sizeah then
		return false, -sizeah, -sizeah - a
	end 

	return true,a,0
end

