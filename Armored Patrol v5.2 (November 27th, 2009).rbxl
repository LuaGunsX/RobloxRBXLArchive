					<string name="Source">--------------------------------------------------------------------------------------------------------------
-- 2009/5/3 Initial versionbased on ArmorGunnerScript-v1.2
--2009/5/6 Change to CntrUser terminology
--2009/5/7 Add arcing trajectory fire
--2009/5/10 Flush CntrDrive, CntrCmd,CntrAim values to nil values at Equip time, and after each use
--2009/5/10 Check for closeness to Howitzer when attaching
--2009/5/21 Change Hint
--2009/6/6 Paint howitzer to conform to Player
--2009/6/10 Change blue team to Cyan
--2009/6/11 Change CntrOwner then player takes control of it
--2009/6/25 Mark every part with team color number to assist TK detection
--2009/8/22 Add auto gen of Howitzer
--2009/8/22 Add check for near captured hill
--2009/8/24 Change: allow reg if near a bunch of fortification slabs
--2009/8/25 Change to request Howitzer gen with mouse click, also indicates position, change CameraType to Watch
--2009/8/25 Add 30 sec delay between last use of Howitzer and eligibility to gen a new one
--2009/8/25 Add delay after genning of Howitzer before it can be used, also remove need to deselect and reselect tool
--2009/9/7 Make v command allow for only 2 camera modes
--2009/11/10 v3.7 Use GUI hint

-- Features:

	-- Sends fire command only to Howitzer player is near

	-- Firing commands sent via CntrCmd, aiming info via CntrAim

	-- Player must be close to Howitzer to attach to it but then may
	--   travel any distance from it and still control it as long as he does
	--   not deselect the Howitzer tool.


local Tool = script.Parent

-- This runs while the script is still in game.Players/&lt;playername&gt;.
-- Record the object now so we can use it to send player specific
-- hints after this tool is equipped.
local Player = Tool.Parent.Parent

local Torso
local Mouse

-- Vehicle parts and parameters
local Vehicle = nil
local CntrUser

local CntrAim  -- to provide aiming info
local CntrCmd --to fire weapon

-- Values that represent nil for the CntrCmd and CntrAim values (because
-- Lua nil is not allowed to be assigned).
local NIL_CMD = &quot;-&quot;
local NIL_AIM = Vector3.new(0,0,0)

-- How close must be to Howitzer to attach
local MAX_ATTACH_DISTANCE = 20

local TEAM_RED = BrickColor.new(&quot;Bright red&quot;)
local TEAM_BLUE = BrickColor.new(&quot;Cyan&quot;)
local VEH_RED = BrickColor.new(&quot;Sand red&quot;)
local VEH_BLUE = BrickColor.new(&quot;Sand blue&quot;)
local VEH_DEFAULT = BrickColor.new(&quot;Dark stone grey&quot;)

local GEN_SLABS_MIN = 20		-- Number of nearby slabs needed to gen a Howitzer
local GEN_SLAB_RADIUS = 50		-- How close the slabs need to be to qualify as &quot;nearby&quot;

local genTime

local POST_GEN_DELAY = 15	-- Amount of time that must elapsed after a Howitzer is 
						-- gened before it may be used.




----------------------------------------------------------------------------------------------------------------
-- Search all and only Howizters that are in the specified mod (presumably game.Workspace,
-- this will insure we only find independent Howitzers instead of the ones still control by a Jeep).
-- Find the nearest, idle Howitzer.  Returns the unit and distance to it or nil.  

function findHowitzer(mod, torso)

	local closest = nil
	local closestDist = 10000000

	for k,v in pairs(mod:GetChildren()) do

		if v.className == &quot;Model&quot;  and string.match(v.Name, &quot;Howitzer&quot;) then

			local CntrUser = v:FindFirstChild(&quot;CntrUser&quot;)

			if CntrUser and ((CntrUser.Value == nil)  or (CntrUser.Value == Player)) then

				local d = (v.Chassis.Position - torso.Position).magnitude
				if d &lt; closestDist then
					closestDist = d
					closest = v
				end
			end
		end
	end

	return closest, closestDist
end

-------------------------------------------------------------------------------------------------------
-- Gen Howitzer centered on genPos, horizontal and facing in the xz direction from
-- torso to genPos
local function genHowitzer( torso, mousePos )

	local mainModel = game.Workspace.RemGenHowitzer
	local genPadProto = mainModel.ProtoGenPad
	local genPad = genPadProto:Clone()
	genPad.Name = &quot;GenPad&quot;

	-- The genPos is the mousePos elevated 2 units up
	local genPos = mousePos + Vector3.new(0,2,0)

	-- Get facing direction vector from torso to genPos in the xz plane
	local facingVec = Vector3.new(genPos.x - torso.Position.x, 0, genPos.z - torso.Position.z)
 
	-- Put genPad into the world at some innocuous place, but oriented to match player

	genPad.CFrame = CFrame.new(Vector3.new(0,100,0), Vector3.new(0,100,0) + facingVec)
	genPad.Parent = mainModel

	-- Place genPad in desired position by changing .Position.  Then check to see if it stays there.
	-- If not, this means there was some obstruction.

	genPad.Position = genPos

	if (genPad.Position - genPos).magnitude &gt; 1 then
		genPad:Remove()
		return false
	end

	-- genPad positions OK, remove from world, attach script, and add to world again

	genPad.Parent = nil

	local scriptGenPad
	for k,v in pairs(mainModel:GetChildren()) do
		if string.match(v.Name, &quot;^ScriptRemGen%-&quot;) then
			scriptGenPad = v:Clone()
			break
		end
	end

	scriptGenPad.Disabled = false
	scriptGenPad.Parent = genPad

	genPad.CntrOwner.Value = Player

	genPad.Parent = mainModel

	return true

end

-------------------------------------------------------------------------------------------------------
-- Go thru all Slabs in Fortifications that are within GEN_SLAB_RADIUS of position
-- (do not bother to count beyond GEN_SLABS_MIN).

local function countSlabs( tpos )

	local fortifications = game.Workspace:FindFirstChild(&quot;Fortifications&quot;)

	if not fortifications then
		return GEN_SLABS_MIN
	end

	local count = 0

	for k,s in pairs(fortifications:GetChildren()) do

		if (s.Name == &quot;Slab&quot;) and ((s.Position-tpos).magnitude &lt; GEN_SLAB_RADIUS) then

			count = count + 1

			if count &gt;= GEN_SLABS_MIN then
				return count
			end
		end
	end

	return count
end

-----------------------------------------------------------------------------------------------------------
function toggleCamera()

	if game.Workspace.Camera.CameraType == Enum.CameraType.Custom then
		game.Workspace.Camera.CameraType = Enum.CameraType.Track

	else
		game.Workspace.Camera.CameraType = Enum.CameraType.Custom
	end

	--print(&quot;Camera &quot;, game.Workspace.Camera.CameraType)

end

------------------------------------------------------------------------------------------------------------
-- If a weapon command it is sent to vehicle in the CntrCmd value.  Aiming info is also
-- sent in CntrAim.  The &quot;+&quot; is suffixed on the CntrCmd code to tell the receiver to
-- expect aiming info.  To guard against duplicate commands or aiming info
-- not being sent (because it will not be seen as a &quot;change&quot;) we force
-- the CntrCmd and CntrAim to the, by convention, &quot;nil&quot; values after sending the command.
-- We wait one clock tick between sending the command and sending the nils.  (Is this 
-- enough, can the the nils get out of sequence with the actual commands?)


function onKeyDown(key)

	if Vehicle == nil then
		return
	end

	if ((key == &quot;f&quot;) or (key == &quot;t&quot;) or  (key == &quot;g&quot;) or (key == &quot;b&quot;)) and CntrAim then

		-- If we are still in post gen time then do not allow commands
		if genTime and ((time() - genTime) &lt; POST_GEN_DELAY) then
			local timeLeft = math.ceil(POST_GEN_DELAY - (time() - genTime))
			_G.displayHint(&quot;Howitzer useable in &quot;.. timeLeft .. &quot; secs&quot;, timeLeft)
			return

		else
			_G.displayHint(&quot;missile: f or left mouse button, shell: t,g,b, view: v&quot;,0)
		end

		local a = Mouse.Hit.p
		CntrAim.Value = a

		-- To synchronize the aim and cmd Values we append &quot;+&quot; to the command to 
		-- tell receiver that this command has an accompanying aim component.

		CntrCmd.Value = key .. &quot;+&quot;

		wait()
	
		CntrAim.Value = NIL_AIM
		CntrCmd.Value = NIL_CMD

		-- Note this as the most recent time Howitzer was used
		lastUseTime = time()

		return

	elseif key == &quot;v&quot; then
		toggleCamera()
	end
end


-------------------------------------------------------------------------------------------------------
-- Recursively go thru every part in model and mark it with the players team color number.
-- If the part is one of the vehicle colors then
-- paint it to the new color (i.e. do not paint the non team specific colored parts).

local function paintVehicle(m, newColor,teamColorNumber)

	for k,p in pairs(m:GetChildren()) do

		if p.className == &quot;Model&quot; then
			paintVehicle(p, newColor, teamColorNumber)

		elseif (p.className == &quot;Part&quot;)  then
		
			p.BackParamA = teamColorNumber

			if (p.BrickColor == VEH_RED) or 
				(p.BrickColor == VEH_BLUE) or 
				(p.BrickColor == VEH_DEFAULT) then

				p.BrickColor = newColor
			end
		end
	end
end
-------------------------------------------------------------------------------------------------------
-- Use the current color of the chassis to determine if this vehicle matches the 
-- team of the player.  If not, paint it.

local function checkVehicleTeamColor(v, player)

	if not player.TeamColor then
		return
	end

	local vcolor = v.Chassis.BrickColor

	if (player.TeamColor == TEAM_RED) and (vcolor ~= VEH_RED) then
		paintVehicle(v, VEH_RED, player.TeamColor.Number)

	elseif (player.TeamColor == TEAM_BLUE) and (vcolor ~= VEH_BLUE) then
		paintVehicle(v, VEH_BLUE, player.TeamColor.Number)
	end
end

-------------------------------------------------------------------------------------------------------
local function tryAttachHowitzer()

	local v,d = findHowitzer(game.Workspace, Torso)

	-- If no Howitzer is attachable
	if (not v) or (d &gt; MAX_ATTACH_DISTANCE) then

		local count = countSlabs( Torso.Position )

		-- If we are near enough Slabs to gen a howitzer, set cursor and tell player that

		if count &gt;= GEN_SLABS_MIN then
			Mouse.Icon = &quot;rbxasset://textures\\CloneCursor.png&quot;
			_G.displayHint(&quot;Click on ground nearby to gen a Howitzer&quot;)

		else
			_G.displayHint(&quot;Stand near unhitched, idle Howitzer then select tool&quot;)
		end

		return

	-- Else Howitzer is attachable
	else

		checkVehicleTeamColor(v, Player)

		CntrOwner = v.CntrOwner
		CntrUser = v.CntrUser

		CntrAim = v.CntrAim
		CntrCmd = v.CntrCmd

		-- Force these to a known state
		CntrCmd.Value = NIL_CMD
		CntrAim.Value = NIL_AIM

		_G.displayHint(&quot;missile: f or left mouse button, shell: t,g,b, view: v&quot;,0)

		Vehicle = v

		Mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;

		CntrOwner.Value = Player
		CntrUser.Value = Player
	end

end

-------------------------------------------------------------------------------------------------------
local function tryGenHowitzer( pos )

	local count = countSlabs( pos )

	if count &lt; GEN_SLABS_MIN then
		_G.displayHint(&quot;To gen Howitzer must be near &quot;..GEN_SLABS_MIN..&quot; solid blocks, currently: &quot;..count)
		return
	end

	if (Torso.Position - pos).magnitude &gt; MAX_ATTACH_DISTANCE then
		_G.displayHint(&quot;Howitzer must be gened close to you&quot;)
		return

	end

	if not genHowitzer(Torso, pos) then
		_G.displayHint(&quot;To gen Howitzer area around mouse position must be free of obstructions&quot;)
		return
	end
	
	-- Howitzer generation requested,  wait a few secs then try to attach to it.

	genTime = time()

	_G.displayHint(&quot;Howtizer will be useable in &quot;..POST_GEN_DELAY..&quot; secs&quot;)

	wait(3)

	-- If we did not attach to a vehicle while we were asleep then try to now
	if Vehicle == nil then
		tryAttachHowitzer()
	end
end


-------------------------------------------------------------------------------------------------------------
function onButton1Down(mouse)
	-- print(&quot;down: &quot;, mouse)

	if Vehicle == nil then
		tryGenHowitzer(mouse.Hit.p)
	
	elseif CntrAim  then
		onKeyDown(&quot;f&quot;)
	end
end




-------------------------------------------------------------------------------------------------------
function onEquipped(mouse)

	Torso = script.Parent.Parent.Torso

	if Torso.Position.y &gt; 300 then
		return
	end

	Mouse = mouse

	game.Workspace.Camera.CameraType = Enum.CameraType.Track

	-- Define even if no Howitzer to attach, to give user option to gen one

	Mouse.Button1Down:connect(function() onButton1Down(mouse) end)
	Mouse.KeyDown:connect(onKeyDown)

	tryAttachHowitzer()

end

-------------------------------------------------------------------------------------------------------
function onUnequipped()

	if Vehicle and CntrUser.Value == Player then
		CntrUser.Value = nil
	end

	_G.clearHint()

	Vehicle = nil

	game.Workspace.Camera.CameraType = Enum.CameraType.Custom

end
---------------------------------------------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)

local elapsed = 0
local CONTROL_INTERVAL = .1
local CONTROL_CHECK_INTERVAL = 5 

while (true) do

	if Vehicle then

		elapsed = elapsed + CONTROL_INTERVAL

		if elapsed &gt; CONTROL_CHECK_INTERVAL then

			if CntrUser.Value ~= Player then
				Vehicle = nil
			end

			elapsed = 0

		end
	end

	wait(CONTROL_INTERVAL)

     --print (&quot;looping&quot;)
end

