-- 2008/10/20 JackTool
-- 2008/11/17 Add restriction that player must not be moving to use, restrict max number of jacks
--2009/5/19 Increase tool distance
--2009/5/23 Disable in skybox
--2009/5/28 Remove upon touch of player (so players cannot use it to fly)
--2009/11/10 v1.4 Use GUI hint

-- Create a Jack object at the position indicated by the mouse


local Tool=script.Parent
local Torso
local Mouse

-- This runs while the script is still in game.Players/&lt;playername&gt;.
local Player = Tool.Parent.Parent


local MAX_JACK_TOOL_DISTANCE = 15
local MAX_JACKS = 5

local numJacks = 0

local MAX_VELOCITY = 2

--------------------------------------------------------------------------------------------------------------
-- If we touch a player then remove the jack.  The goal is to keep the jack from being
-- used by players to fly.

local function onTouched(jack, part)

	if part.Parent:FindFirstChild(&quot;Torso&quot;) then
		jack:Remove()
	end

end

------------------------------------------------------------------------------------------------------------
function onButton1Down(mouse)
	-- print(&quot;down: &quot;, mouse)

	if numJacks &gt;= MAX_JACKS then
		return
	end

	local torso = Tool.Parent.Torso

	if torso.Velocity.magnitude &gt; MAX_VELOCITY then
		return
	end

	local pos = Mouse.Hit.p

	if (pos - Torso.Position).magnitude &gt; MAX_JACK_TOOL_DISTANCE then
		return
	end

	numJacks = numJacks + 1

	local j = Instance.new(&quot;Part&quot;)
	j.formFactor = 2
	j.Size = Vector3.new(2,.4,2)
	j.Friction = 1
	j.Position = pos
	j.Parent = game.Workspace
	
	local bg = Instance.new(&quot;BodyGyro&quot;)
	bg.D = 110
	bg.P = 100
	bg.maxTorque = Vector3.new(1000000,0,1000000)
	bg.Parent = j

	local bp = Instance.new(&quot;BodyPosition&quot;)
	bp.D = 6200
	bp.P = 6000
	bp.maxForce = Vector3.new(10000,6000000,10000)
	bp.position = Vector3.new(j.Position.x, j.Position.y+20, j.Position.z)
	bp.Parent = j

	j.Touched:connect(function(part) onTouched(j,part) end)

	wait(5)

	j:Remove()

	numJacks = numJacks - 1

end

------------------------------------------------------------------------------------------------------------
function onButton1Up(mouse)
	-- print(&quot;up: &quot;, mouse)

end



-------------------------------------------------------------------------------------------------------
function onEquipped(mouse)

	Torso = script.Parent.Parent.Torso

	if Torso.Position.y &gt; 300 then
		return
	end

	Mouse = mouse

        mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;

	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
        mouse.Button1Up:connect(function() onButton1Up(mouse) end)

	_G.displayHint(&quot;Click on ground nearby and near edge of vehicle&quot;,0)

end

-------------------------------------------------------------------------------------------------------
function onUnequipped()

	_G.clearHint()

end
---------------------------------------------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)



while (true) do


     wait(10)

     --print (&quot;looping&quot;)
end

