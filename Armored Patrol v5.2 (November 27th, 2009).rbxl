								<string name="Source">-----------------------------------------------------------------------------------------------------------
--2009/5/3 Initial version based on AsvTurretScript-v3.1
--2009/5/5 Make weld parent game.Workspace
--2009/5/9 Fix repeated command encoding
--2009/5/10 Assume tool will flush special nil values to CntrCmd and CntrAim


local Windshield = script.Parent
local Chassis = Windshield.Parent.Chassis

local Mount = Windshield.Mount

-- Insure glass is connected to Mount via surface connection.
Mount:MakeJoints()

local CntrCmd = Instance.new(&quot;StringValue&quot;)	-- to receive commands from Tool
CntrCmd.Name = &quot;CntrCmd&quot;
CntrCmd.Parent = Windshield

local CntrAim = Instance.new(&quot;Vector3Value&quot;)		-- unused, just make Tool happy
CntrAim.Name = &quot;CntrAim&quot;
CntrAim.Parent = Windshield

-- Values that represent nil for the CntrCmd and CntrAim values (because
-- Lua nil is not allowed to be assigned).
local NIL_CMD = &quot;-&quot;
local NIL_AIM = Vector3.new(0,0,0)


-- Weld the Mount to the Chassis

local Mweld = Instance.new(&quot;Weld&quot;) 
local cf = Chassis.CFrame:toObjectSpace(Mount.CFrame)
local windshieldUpCf = CFrame.new()
Mweld.Name = &quot;Weld&quot;
Mweld.Parent = game.Workspace
Mweld.Part0 = Chassis --&gt; part0 has to be the parent 
Mweld.Part1 = Mount 
Mweld.C0 = cf
Mweld.C1 = windshieldUpCf

local windshieldDownCf = windshieldUpCf * CFrame.fromAxisAngle(Vector3.new(0,0,1), math.rad(95))


-- WindShield status variables

local windshieldUp = true -- current windshield orientation

---------------------------------------------------------------------------------------------
function handleCmd()

	-- If we have not gotten the command portion yet, do not handle yet.
	if (CntrCmd.Value == nil) or  (CntrCmd.Value == NIL_CMD) then
		return
	end

	local cmd = string.sub(CntrCmd.Value, 1,1)	-- Strip out just command char

	-- Adjust windshield
	if (cmd==&quot;f&quot;) or (cmd == &quot;g&quot;)  or (cmd == &quot;t&quot;) or (cmd == &quot;b&quot;) then

		if windshieldUp then
			Mweld.C1 = windshieldDownCf
			windshieldUp = false

		else
			Mweld.C1 = windshieldUpCf
			windshieldUp = true
		end
	end
end


----------------------------------------------------------------------------------------------------------

CntrCmd.Changed:connect(handleCmd)

while true do

	wait(5)


end
		
