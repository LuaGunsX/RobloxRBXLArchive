-- 2009/5/22 Initial version based on Galleon game PlayerControlScript-v2.2
--2009/6/10 Change blue team to Cyan
--2009/7/4 Add hill capture leaderboard columns
--2009/7/12 Fixed KO, WO
--2009/11/10 v2.7 Add GUI Hint



--[[
	This script handles Players entering game and respawns.   
	For each player after each game, the player is assigned
	the green team (neutral) and health=0 and goes to the green spawn pad in the skybox.  

	Each player may select red or blue team (via the touch pads) as long as there is still the possibility
	of fair teams.  If there is no possibility of fair teams then the choice is ignored.

	After choosing the player is Health=0 so he can respawn at his teams spawn pad.

	Also when a player enters the game, his Player is assigned the leaderstats IntValue for the 
	scoreboard.

--]]

local GameControl = script.Parent

local teamRed = BrickColor.new(&quot;Bright red&quot;)
local teamBlue = BrickColor.new(&quot;Cyan&quot;)
local teamGreen = BrickColor.new(&quot;Medium green&quot;)

local ChooseRed = game.Workspace.SkySpawnBox.ChooseRed
local ChooseBlue = game.Workspace.SkySpawnBox.ChooseBlue

local CurCapRed = 0
local TotCapRed = 0 
local CurCapBlue = 0
local TotCapBlue = 0 

-----------------------------------------------------------------------------------------
-- Count how many players are on each team and neutral except 
-- do not count the specified player.

local function countTeams( player )

	local rteam = 0
	local bteam = 0
	local neutral = 0

	for k,v in pairs(game.Players:GetChildren()) do

		if v ~= player then

			if v.Neutral then
				neutral = neutral + 1

			elseif v.TeamColor == teamRed then
				rteam = rteam + 1

			elseif v.TeamColor == teamBlue then
				bteam = bteam + 1

			else
				neutral = neutral + 1

			end
		end
	end

	return rteam, bteam, neutral

end

--------------------------------------------------------------------------------------------
local function respawnPlayer(player)

	local character = player.Character

	if character then
		local humanoid = character.Humanoid

		if humanoid then
			humanoid.Health = 0
		end
	end

end

-------------------------------------------------------------------------------------------
-- Find the hint TextBox in the players gui and display the text.
-- The TextBox x size is set since the Tool hint displays may change this.

local function displayHint( player, text )

	if not player:FindFirstChild(&quot;PlayerGui&quot;) then
		return
	end

	if not player.PlayerGui:FindFirstChild(&quot;ScreenGui&quot;) then
		return
	end

	local hint = player.PlayerGui.ScreenGui:FindFirstChild(&quot;Hint&quot;)

	if not hint then
		return
	end

	local hintHeight = hint.AbsoluteSize.y

	hint.Size = UDim2.new(0,300, 0,hintHeight)
	hint.Text = text
	hint.Visible = true
end

-------------------------------------------------------------------------------------------
local function clearHint(player)

	if not player:FindFirstChild(&quot;PlayerGui&quot;) then
		return
	end

	if not player.PlayerGui:FindFirstChild(&quot;ScreenGui&quot;) then
		return
	end

	local hint = player.PlayerGui.ScreenGui:FindFirstChild(&quot;Hint&quot;)

	if not hint then
		return
	end

	hint.Visible = false
end

-------------------------------------------------------------------------------------------
local function chose(player, selectedTeam)

	-- If player is not neutral then this is an old event, ignore

	if (not player.Neutral) and  ((player.TeamColor == teamRed) or
		(player.TeamColor == teamBlue)) then
		
		return
	end

	-- Count current teams (except our player)
	local rteam, bteam, neutral = countTeams(player)

	-- Add in our player to the neutral count
	neutral = neutral + 1

	local total = rteam + bteam + neutral

	--print(&quot;chose: rteam &quot;, rteam, &quot; bteam &quot;, bteam, &quot; neutral &quot;, neutral)

	if selectedTeam == teamRed then

		--print(&quot;red&quot;)

		if rteam &lt; bteam + neutral then	
			player.Neutral = false
			player.TeamColor = teamRed

			respawnPlayer( player )

		else
			displayHint(player, &quot;Teams unbalanced!  Must choose Blue&quot;)
			wait(3)
			clearHint(player)
			return
		end
	
	elseif selectedTeam == teamBlue then

		--print(&quot;blue&quot;)

		if bteam &lt; rteam + neutral then	
			player.Neutral = false
			player.TeamColor = teamBlue

			respawnPlayer( player )

		else
			displayHint(player, &quot;Teams unbalanced!  Must choose Red&quot;)
			wait(3)
			clearHint(player)
			return
		end
	end
end
-----------------------------------------------------------------------------------------

function onTouchRed(part)

	--print(&quot;touched red &quot;, part.Name)

	local player = game.Players:playerFromCharacter(part.Parent)

	if player then

		chose(player, teamRed)
	end
end

function onTouchBlue(part)

	--print(&quot;touched blue &quot;, part.Name)

	local player = game.Players:playerFromCharacter(part.Parent)

	if player then

		chose(player, teamBlue)
	end
end



----------------------------------------------------------------------------------------
function onPlayerEntered(newPlayer)

	newPlayer.Neutral = false
	newPlayer.TeamColor = teamGreen

	local stats = Instance.new(&quot;IntValue&quot;)
	stats.Name = &quot;leaderstats&quot;

	local kills = Instance.new(&quot;IntValue&quot;)
	kills.Name = &quot;KO&quot;
	kills.Value = 0

	local deaths = Instance.new(&quot;IntValue&quot;)
	deaths.Name = &quot;WO&quot;
	deaths.Value = 0

	kills.Parent = stats
	deaths.Parent = stats

	local curCap = Instance.new(&quot;IntValue&quot;)
	curCap.Name = &quot;CCap&quot;
	curCap.Value = 0
	curCap.Parent = stats

	local totCap = Instance.new(&quot;IntValue&quot;)
	totCap.Name = &quot;TCap&quot;
	totCap.Value = 0
	totCap.Parent = stats


	stats.Parent = newPlayer
end



----------------------------------------------------------------------------------------------------
-- Global functions to peg KOs and Wipeouts

local function pegWipeouts( player, count)

	--print(&quot;pegWipeouts called&quot;)

	local leaderstats = player:FindFirstChild(&quot;leaderstats&quot;)

	if leaderstats then
		local Wipeouts = leaderstats:FindFirstChild(&quot;WO&quot;)
	
		if Wipeouts then
			--print(&quot;Pegging&quot;)
			Wipeouts.Value = Wipeouts.Value + 1
		end
	end
end

--------------------------------------------------

local function pegKOs( player, count)

	--print(&quot;pegKOs called&quot;)
	
	local leaderstats = player:FindFirstChild(&quot;leaderstats&quot;)

	if leaderstats then
		local KOs = leaderstats:FindFirstChild(&quot;KO&quot;)
	
		if KOs then
			--print(&quot;Pegging&quot;)
			KOs.Value = KOs.Value + 1
		end
	end

end

------------------------------------------------
_G.pegWipeouts = pegWipeouts
_G.pegKOs = pegKOs

--------------------------------------------------

local function pegCapture( teamColor, count)

	--print(&quot;pegKOs called&quot;)

	if teamColor == teamRed then
		CurCapRed = CurCapRed + count
		if CurCapRed &lt; 0 then
			CurCapRed = 0
		end

	elseif teamColor == teamBlue then
		CurCapBlue = CurCapBlue + count
		if CurCapBlue &lt; 0 then
			CurCapBlue = 0
		end
	end
end

------------------------------------------------
_G.pegCapture = pegCapture

-------------------------------------------------------
local function resetCapture()

	CurCapRed = 0
	TotCapRed = 0
	CurCapBlue = 0
	TotCapBlue = 0
end

_G.resetCapture = resetCapture

-------------------------------------------------------
local function getCapture()

	return TotCapRed,TotCapBlue
end

_G.getCapture = getCapture

-----------------------------------------------------------------------------------------------------
-- Distribute the specified points among the specified team players

local function adjustCapturePoints(teamColor, curCap, totCap)

	-- Count how many players on a team

	local teamCount = 0

	for k,v in pairs(game.Players:GetChildren()) do

		if (not v.Neutral) and  (v.TeamColor == teamColor) then
			teamCount = teamCount + 1
		end
	end

	if teamCount &lt;= 0 then
		return
	end

	-- Distribute the points evenly to the team
	
	local curCapPerPlayer = curCap / teamCount
	local totCapPerPlayer = totCap / teamCount

	for k,v in pairs(game.Players:GetChildren()) do

		if (not v.Neutral) and  (v.TeamColor == teamColor) then
				
			if v.leaderstats then
				if v.leaderstats.CCap then
					v.leaderstats.CCap.Value = curCapPerPlayer
				end

				if v.leaderstats.TCap then
					v.leaderstats.TCap.Value = totCapPerPlayer
				end
			end
		end
	end
end



-----------------------------------------------------------------------------------------------------


--game.Players.ChildAdded:connect(onPlayerEntered)

ChooseRed.Touched:connect(onTouchRed)
ChooseBlue.Touched:connect(onTouchBlue)

game.Players.PlayerAdded:connect(onPlayerEntered)



local UPDATE_INTERVAL = 60
local DISPLAY_INTERVAL = 5

while true do

	wait(DISPLAY_INTERVAL)

	TotCapRed = TotCapRed + (CurCapRed*(DISPLAY_INTERVAL/UPDATE_INTERVAL))
	assert(pcall(adjustCapturePoints,teamRed, math.floor(CurCapRed), math.floor(TotCapRed)))

	TotCapBlue = TotCapBlue + (CurCapBlue*(DISPLAY_INTERVAL/UPDATE_INTERVAL))
	assert(pcall(adjustCapturePoints,teamBlue, math.floor(CurCapBlue), math.floor(TotCapBlue)))
end


