findGenStations(game.Workspace)

local INTERVAL = 10
local MAX_GAME_TIME = 30 * 60	-- 30 minutes  

local timer = 0

while true do

	if timer &gt; 0 then
		timer = timer - INTERVAL
		GameTime.Value = GameTime.Value + INTERVAL

	else
		emptySwitchList()		-- Allow players to change teams
		GameTime.Value = 0

		restartGame()

		timer = MAX_GAME_TIME

	end

	wait(INTERVAL)

end
