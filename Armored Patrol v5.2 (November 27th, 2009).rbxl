	s.Disabled = false
	s.Parent = probe

	-- Copy for later
	local probe2 = probe:Clone()

	-- Put the probe into the world then move it to the desired
	-- position.  If it will not stay there this means there must be something in between
	-- the blast pos and the player, so the player is protected. (I know,	
	-- extremely hokey ray trace.)

	probe.Parent = game.Workspace
	probe.Position = p

	if (probe.Position - p).magnitude &gt; .1 then
	
		-- Probe moved, keep probe in whatever position it moved to and make it visible red.
		probe.Transparency = .5
		probe.BrickColor = BrickColor.Red()
		probe.CanCollide = false

		-- Move probe2 to the position we tried to place probe and make it visible blue.
		local scf = probe2.CFrame
		probe2.CFrame = scf + (p - scf.p)
		probe2.Transparency = .5
		probe2.BrickColor = BrickColor.Blue()
		probe2.CanCollide = false
		probe2.Parent = game.Workspace

		-- Create a phantom of the limb we probed to

		print(&quot;probeLimb &quot;, probeLimb)

		local plimb = Instance.new(&quot;Part&quot;)
		plimb.Name = &quot;Limb&quot;
		plimb.Shape = probeLimb.Shape
		plimb.formFactor = probeLimb.formFactor
		plimb.Size = probeLimb.Size
		plimb.Anchored = true
		plimb.CFrame = probeLimb.CFrame
		plimb.BrickColor = probeLimb.BrickColor
		plimb.CanCollide = false
		plimb.Transparency = .5
		local s = game.Workspace.GlobalScripts.ScriptRemoveMe:Clone()
		s.Disabled = false
		s.Parent = plimb
		plimb.Parent = game.Workspace

		CntrDebugText.Value = CntrDebugText.Value .. &quot;Player protected by blockage#&quot;
	
		return false
	end

	CntrDebugText.Value = CntrDebugText.Value .. &quot;Player hit (b)#&quot;
	
	probe:Remove()

	return true
end

--------------------------------------------------------------------------------------------------------------------
local function fly(part, blastPos)

	if part then

		local v = (part.Position - blastPos).unit

		part.Velocity = (v * 30) + Vector3.new(0,70,0)
		part.RotVelocity = Vector3.new(math.random(10), math.random(10), math.random(10))

	end

end


-------------------------------------------------------------------------------------------------------------------
local function blastPlayer(torso, blastPos, bolt)

	-- Check for TK

	local targetPlayer = game.Players:getPlayerFromCharacter(torso.Parent)
	if not targetPlayer then
		return
	end

	local creatorPlayer = bolt.Creator.Value

	if creatorPlayer then
		if creatorPlayer.TeamColor == targetPlayer.TeamColor then
			local creatorChar = creatorPlayer.Character
			if creatorChar then
				creatorChar.torso:BreakJoints()
				return
			end
		end
	end

	-- See if any vehicle under player, if yes, move it back to game.Workspace so
	-- it is not blasted as well.

	local char = torso.Parent

	for k,v in pairs(char:GetChildren()) do

		if (v.className == &quot;Model&quot;) and v:FindFirstChild(&quot;CntrUser&quot;) then
			v.Parent = game.Workspace
			break
		end
	end

	if not char:FindFirstChild(&quot;ForceField&quot;) then

		torso:BreakJoints()

		fly(torso, blastPos)
		fly(char:FindFirstChild(&quot;Head&quot;), blastPos)
		fly(char:FindFirstChild(&quot;Left Arm&quot;), blastPos)
		fly(char:FindFirstChild(&quot;Right Arm&quot;), blastPos)
		fly(char:FindFirstChild(&quot;Left Leg&quot;), blastPos)
		fly(char:FindFirstChild(&quot;Right Leg&quot;), blastPos)

		if _G.pegWipeouts then
			_G.pegWipeouts(targetPlayer, 1)
		end

		if _G.pegKOs then
			_G.pegKOs(creatorPlayer, 1)
		end

	end
end

_G.glBlastPlayer = blastPlayer

-------------------------------------------------------------------------------------------------------------------
function _G.glAssessPlayers( blastPos, blastRadius, bolt)

	local debugProbe = false
	local inRange = false
	local closestDist = nil

	if CntrDebug.Value == 1 then
		debugProbe = isVip(bolt.Creator.Value.Name)

		-- Init the debug text
		if debugProbe then
			CntrDebugText.Value = &quot;&quot;
		end

	end

	for i,p in pairs(game.Players:GetPlayers()) do
	--for i,p in pairs(game.Workspace.FakePlayers:GetChildren()) do

		--print(&quot;player &quot;, p)

		local ch = p.Character

		if ch then
			--print(&quot;character&quot;)

			local t = ch:FindFirstChild(&quot;Torso&quot;)

			if t then
				--print(&quot;torso &quot;, t.Position, &quot; &quot;, blastPos, &quot; dist &quot;, (t.Position - blastPos).magnitude, &quot; br &quot;, blastRadius)
				--if (t.Position - blastPos).magnitude &lt; blastRadius then

				--print(&quot;player dist to blast: &quot;, (t.Position - blastPos).magnitude)

				local dist = (t.Position - blastPos).magnitude

				if  dist &lt; blastRadius then

					inRange = true

					if debugProbe then
						if playerHitDebug(t, blastPos) then
							blastPlayer(t, blastPos,bolt)
						end
					else
						if playerHit(t, blastPos) then
							blastPlayer(t, blastPos,bolt)
						end
					end
				else
					if (not closestDist) or (closestDist &gt; dist) then
						closestDist = dist
					end
				end
			end
		end
	end

	if debugProbe and (not inRange) then
		local s = string.format(&quot;No player w/i %.1f, closest: %.1f#&quot;, blastRadius,closestDist)
		CntrDebugText.Value = CntrDebugText.Value .. s
	end
end

