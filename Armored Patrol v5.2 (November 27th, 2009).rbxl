--2009/5/4 Change to CntrUser usage, and controllable sub-Models named &quot;Movable&quot;
--2009/5/9 Use y,x,h,n drive keys
--2009/5/9 Fix CntrUser usage.
--2009/5/9 Flush CntrDrive, CntrCmd,CntrAim values to nil values at Equip time, and after each use
--2009/5/21 Modify Hints
--2009/5/27 Make Hints more specific to type of vehicle
--2009/6/6 Paint vehicle to conform to Player
--2009/6/10 Change blue team to Cyan
--2009/6/18 Add p command to remove the gunner seat
--2009/6/25 Mark every part with team color number to assist TK detection
--2009/7/1 Add command to fire 50 cal
--2009/7/2 Add fire probe m command, allow only for VIP players
--2009/11/10 v2.10 Use GUI hint
--2009/11/26 v2.11 Add support for M109

-- Features:

	-- Sends commands to vehicle the player is seat welded to.

	-- Assumes Attach script will put vehicle under Character,
	-- 	auto sets view to Tracking.

	-- Differentiates Driver from Gunner seat.

	-- Firing commands sent via CntrCmd, aiming info via CntrAim

	-- Vehicle painted to match Player team if necessary

-- Concepts:

	-- A Player is recorded in the vehicles CntrUser value as long as he is
	--     welded to driver seat, regardless of any tool selection.

	-- A Player is recorded in the vehicles CntrGunner value if he is both
	--     welded to gunner seat and has selected gunner tool.

	-- The driver can operate the gun if there is no Player recorded
	--    in the CntrGunner value.


local Tool = script.Parent

-- This runs while the script is still in game.Players/&lt;playername&gt;.
-- Record the object now so we can use it to send player specific
-- hints after this tool is equipped.
local Player = Tool.Parent.Parent

local Torso
local Mouse

-- Vehicle parts and parameters
local Vehicle = nil
local CntrDrive
local CntrSteer
local CntrUser
local CntrGunner

local vip = false

local vehicleType = &quot;none&quot;

local CntrAim  -- to provide aiming info
local CntrCmd --to fire weapon

-- Values that represent nil for the CntrCmd and CntrAim values (because
-- Lua nil is not allowed to be assigned).
local NIL_CMD = &quot;-&quot;
local NIL_AIM = Vector3.new(0,0,0)

-- Vehicle status
local steer = false

local TEAM_RED = BrickColor.new(&quot;Bright red&quot;)
local TEAM_BLUE = BrickColor.new(&quot;Cyan&quot;)
local VEH_RED = BrickColor.new(&quot;Sand red&quot;)
local VEH_BLUE = BrickColor.new(&quot;Sand blue&quot;)
local VEH_DEFAULT = BrickColor.new(&quot;Dark stone grey&quot;)


----------------------------------------------------------------------------------------------------------------
-- Find the vehicle (if any) that we are welded to.  Set up parameters for this vehicle.
-- Assumes the Seat Parent is the vehicle.  Returns the vehicle or nil.

function findVehicle(mod, torso)

	for k,v in pairs(mod:GetChildren()) do

		if v.className == &quot;Model&quot; then
			local veh = findVehicle(v,torso) 
			if veh then
				return veh
			end

		elseif v.className == &quot;Seat&quot; then
			local sw = v:FindFirstChild(&quot;SeatWeld&quot;) 

			if sw and sw.Part1 == torso then

				if v.Name == &quot;SeatDriver&quot; then
					local CntrUser = v.Parent:FindFirstChild(&quot;CntrUser&quot;)

					if CntrUser and ((CntrUser.Value == nil)  or (CntrUser.Value == Player)) then
						return v.Parent
					end
				end
			end
		end
	end

	return nil
end


------------------------------------------------------------------------------------------------------------
function onButton1Down(mouse)
	-- print(&quot;down: &quot;, mouse)

	if Vehicle == nil then
		return
	end

	steer = true

	CntrSteer.Value = Mouse.Hit.p

end

------------------------------------------------------------------------------------------------------------
function onButton1Up(mouse)
	-- print(&quot;up: &quot;, mouse)
	steer = false
end


-----------------------------------------------------------------------------------------------------------
function toggleCamera()

	if game.Workspace.Camera.CameraType == Enum.CameraType.Fixed then
		game.Workspace.Camera.CameraType = Enum.CameraType.Attach

	elseif game.Workspace.Camera.CameraType == Enum.CameraType.Attach then
		game.Workspace.Camera.CameraType = Enum.CameraType.Watch

	elseif game.Workspace.Camera.CameraType == Enum.CameraType.Watch then
		game.Workspace.Camera.CameraType = Enum.CameraType.Track

	elseif game.Workspace.Camera.CameraType == Enum.CameraType.Track then
		game.Workspace.Camera.CameraType = Enum.CameraType.Follow

	elseif game.Workspace.Camera.CameraType == Enum.CameraType.Follow then
		game.Workspace.Camera.CameraType = Enum.CameraType.Custom

	elseif game.Workspace.Camera.CameraType == Enum.CameraType.Custom then
		game.Workspace.Camera.CameraType = Enum.CameraType.Fixed
	end

	--print(&quot;Camera &quot;, game.Workspace.Camera.CameraType)

end


------------------------------------------------------------------------------------------------------------
-- Analyse the command and send to vehicle if appropriate.  

-- If a driving command then it is alternated between upper and lower case to
-- insure it is a &quot;change&quot; from the last command in the case of duplicate commands.
-- It is sent to vehicle in the CntrDrive value.

-- If a weapon command it is sent to vehicle in the CntrCmd value.  Aiming info is also
-- sent in CntrAim.  The &quot;+&quot; is suffixed on the CntrCmd code to tell the receiver to
-- expect aiming info.  To guard against duplicate commands or aiming info
-- not being sent (because it will not be seen as a &quot;change&quot;) we force
-- the CntrCmd and CntrAim to the, by convention, &quot;nil&quot; values after sending the command.
-- We wait one clock tick between sending the command and sending the nils.  (Is this 
-- enough, can the the nils get out of sequence with the actual commands?)

--local count = 1

function onKeyDown(key)

	if Vehicle == nil then
		return
	end

	if key == &quot;v&quot; then
		toggleCamera()
		return
	end

	-- Check for special vip commands and block if not vip

	if ((key == &quot;m&quot;) or (key == &quot;n&quot;)) and (not vip) then
		return
	end

	-- Fire weapon if there is no gunner, if jeep this lowers windshield

	if ((key == &quot;f&quot;) or (key == &quot;t&quot;) or  (key == &quot;g&quot;) or (key == &quot;b&quot;) or (key == &quot;c&quot;) or (key == &quot;m&quot;)) and CntrAim  then
	
		if ((vehicleType == &quot;Tank&quot;) or (vehicleType == &quot;ASV&quot;) or (vehicleType == &quot;M109&quot;)) and CntrGunner.Value then
			_G.displayHint(&quot;Cannot fire gun: gunner is controlling it&quot;)
			return
		end

		-- Put aiming info into CntrAim

		local a = script.Parent.Parent.Humanoid.TargetPoint
		CntrAim.Value = a

		-- To synchronize the aim and cmd Values we append &quot;+&quot; to the command to 
		-- tell receiver that this command has an accompanying aim component.

		--local oldcmd = CntrCmd.Value

		CntrCmd.Value = key .. &quot;+&quot;

		--count = count + 1

		wait(.1)	-- This is to insure the previous command gets sent before we clear our the Cntr values
	
		CntrAim.Value = NIL_AIM
		CntrCmd.Value = NIL_CMD

		return

	-- If key if seat remove command

	elseif key == &quot;p&quot; then

		CntrCmd.Value = key

		wait(.1)	-- This is to insure the previous command gets sent before we clear our the Cntr values
	
		CntrAim.Value = NIL_AIM
		CntrCmd.Value = NIL_CMD

	-- Else must be a drive command
	else
		CntrDrive.Value = key

		wait()

		CntrDrive.Value = NIL_CMD
	end



end

-------------------------------------------------------------------------------------------------------
-- Recursively go thru every part in model and mark it with the players team color number.
-- If the part is one of the vehicle colors then
-- paint it to the new color (i.e. do not paint the non team specific colored parts).

local function paintVehicle(m, newColor,teamColorNumber)

	for k,p in pairs(m:GetChildren()) do

		if p.className == &quot;Model&quot; then
			paintVehicle(p, newColor, teamColorNumber)

		elseif (p.className == &quot;Part&quot;)  then
		
			p.BackParamA = teamColorNumber

			if (p.BrickColor == VEH_RED) or 
				(p.BrickColor == VEH_BLUE) or 
				(p.BrickColor == VEH_DEFAULT) then

				p.BrickColor = newColor
			end
		end
	end
end
-------------------------------------------------------------------------------------------------------
-- Use the current color of the chassis to determine if this vehicle matches the 
-- team of the player.  If not, paint it.

local function checkVehicleTeamColor(v, player)

	if not player.TeamColor then
		return
	end

	local vcolor = v.Chassis.BrickColor

	if (player.TeamColor == TEAM_RED) and (vcolor ~= VEH_RED) then
		paintVehicle(v, VEH_RED, player.TeamColor.Number)

	elseif (player.TeamColor == TEAM_BLUE) and (vcolor ~= VEH_BLUE) then
		paintVehicle(v, VEH_BLUE, player.TeamColor.Number)
	end
end

-------------------------------------------------------------------------------------------------------
function onEquipped(mouse)

	Torso = script.Parent.Parent.Torso

	local v = findVehicle(game.Workspace, Torso)
	if not v then
		_G.displayHint(&quot;First sit in driver (yellow) seat then select tool&quot;)
		return
	end

	CntrDrive = v.CntrDrive
	CntrSteer = v.CntrSteer
	CntrUser = v.CntrUser
	CntrGunner = v.CntrGunner

	local movable = v:FindFirstChild(&quot;Movable&quot;)
	if movable then
		CntrAim = movable.CntrAim
		CntrCmd = movable.CntrCmd

		-- Force these to a known state
		CntrCmd.Value = NIL_CMD
		CntrAim.Value = NIL_AIM

	end

	checkVehicleTeamColor(v, Player)

	if string.match(v.Name, &quot;Tank&quot;) then
		_G.displayHint(&quot;Accel=y,h, decel=j, brake=x,u, missile=f, 50cal=c, steer=LMB, remove gunner=p, change view=v&quot;,0)
		vehicleType = &quot;Tank&quot;

	elseif string.match(v.Name, &quot;Jeep&quot;) then
		_G.displayHint(&quot;Accel=y,h, decel=j, brake=x,u windshield=f, unhitch howitzer=r, steer=LMB, change view=v&quot;,0)
		vehicleType = &quot;Jeep&quot;

	elseif string.match(v.Name, &quot;M109&quot;) then
		_G.displayHint(&quot;Accel=y,h, decel=j, brake=x,u, shell=b,g,t, 50cal=c, steer=LMB, remove gunner=p, change view=v&quot;,0)
		vehicleType = &quot;M109&quot;

	else
		_G.displayHint(&quot;Accel=y,h, decel=j, brake=x,u, missile=f, shell=b,g,t, 50cal=c, steer=LMB, remove gunner=p, change view=v&quot;,0)
		vehicleType = &quot;ASV&quot;
	end

	-- Check for VIP players

	if (Player.Name == &quot;Wingman8&quot;) or (Player.Name == &quot;cormorant&quot;) then
		vip = true
	else
		vip = false
	end

	Vehicle = v

	Mouse = mouse

        mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;

	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
        mouse.Button1Up:connect(function() onButton1Up(mouse) end)

	--mouse.Move:connect(function() onMouseAny(mouse) end)
	mouse.KeyDown:connect(onKeyDown)

	--game.Workspace.Camera.CameraType = Enum.CameraType.Track
end

-------------------------------------------------------------------------------------------------------
-- Note we do not remove our Player from CntrUser.  A Player is considered the 
-- user iff he is seatwelded to the driver seat, regardless of tool selection.

function onUnequipped()

	CntrDrive = nil
	CntrSteer = nil
	CntrUser = nil
	CntrGunner = nil
	CntrAim = nil
	CntrCmd = nil

	game.Workspace.Camera.CameraType = Enum.CameraType.Custom

	Vehicle = nil
	vehicleType = &quot;none&quot;

	_G.clearHint()

end
---------------------------------------------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)


local lastTarget
local elapsed = 0
local CONTROL_INTERVAL = .1
local CONTROL_CHECK_INTERVAL = 5 

while (true) do

	if Vehicle then

		if steer and Mouse.Hit.p ~= lastTarget then
		
			CntrSteer.Value = Mouse.Hit.p
			lastTarget = Mouse.Hit.p

		end

		elapsed = elapsed + CONTROL_INTERVAL

		if elapsed &gt; CONTROL_CHECK_INTERVAL then

			if CntrUser.Value ~= Player then
				onUnequipped()
			end

			elapsed = 0

		end
	end

	wait(CONTROL_INTERVAL)

     --print (&quot;looping&quot;)
end

