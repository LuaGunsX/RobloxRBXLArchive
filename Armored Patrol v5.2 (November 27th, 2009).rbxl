local function genOnePos()

	while true do

		local x = math.random(MIN_X, MAX_X)
		local z = math.random(MIN_Z, MAX_Z)	

		local pos = Vector3.new(x,0,z)

		for k,zone in pairs(Zones:GetChildren()) do

			if (zone.TopParamB &gt; 0) and 
				(zone.Position - pos).magnitude &lt; zone.TopParamB then

				return pos
			end
		end
	end
end


