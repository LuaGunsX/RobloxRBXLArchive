local function placeRidges(ho)

	local top = ho.top

	--print(&quot;top.Position &quot;, top.Position)

	local wps = {
		{dir=&quot;se&quot;, p=top.CFrame:pointToWorldSpace(Vector3.new(top.Size.x/2,0,top.Size.z/2))},
		{dir=&quot;sw&quot;, p=top.CFrame:pointToWorldSpace(Vector3.new(-top.Size.x/2,0,top.Size.z/2))},
		{dir=&quot;ne&quot;, p=top.CFrame:pointToWorldSpace(Vector3.new(top.Size.x/2,0,-top.Size.z/2))},
		{dir=&quot;nw&quot;, p=top.CFrame:pointToWorldSpace(Vector3.new(-top.Size.x/2,0,-top.Size.z/2))}
	}

	buildRidge(ho, cfn, wps, HIGH_Z_EDGE, ho.height)
	buildRidge(ho, cfs, wps, HIGH_Z_EDGE, ho.height)
	buildRidge(ho, cfe, wps, HIGH_X_EDGE, ho.height)
	buildRidge(ho, cfw, wps, HIGH_X_EDGE, ho.height)


end

