-- Remove all entries from the global DamageHandler. 

local function emptyDamageHandler( )

	if _G.DamageHandler ~= nil then

		for k,v in pairs(_G.DamageHandler) do
			_G.DamageHandler[k] = nil
		end
	end
end



-----------------------------------------------------------------------------------------------------
-- Delete any vehicles found in game.Workspace and in player characters

local function deleteVehicles( m )

	for k,v in pairs(m:GetChildren()) do 

		if v.className == &quot;Model&quot; then

			-- If this Model has a CntrOwner then we consider it a vehicle
			if  v:FindFirstChild(&quot;CntrOwner&quot;) then
