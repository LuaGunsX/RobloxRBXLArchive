-- Make a floor under the ridge and its incline (of angle angle in radians)
-- which goes in the cfDir direction relative to ridge.

local function makeRidgeFloor(hill, ridge, cfDir, height, angle)

	local wid = ridge.Size.z
	local len = math.floor((height / math.tan(angle)) + ridge.Size.x)	-5
	local offs = len/2 - ridge.Size.x/2

	-- CFrame of untilted ridge floor, resting on ground
	local cf = (ridge.CFrame * cfDir) 
	cf = cf + (cf:pointToWorldSpace(Vector3.new(0,-height+FLOOR_THICKNESS+.4,-offs)) - cf.p)

print(&quot;len &quot;,len, &quot; offs &quot;, offs, &quot; cf &quot;, cf)

	-- Angle needed to tilt floor to make one end flush with ground
	local a = math.atan(FLOOR_THICKNESS/len)

	local rcf = CFrame.fromAxisAngle(Vector3.new(1,0,0),-a)

	local z = len / 2
	local y = -FLOOR_THICKNESS / 2

	local zp = z * math.cos(a) + y * math.sin(a)
	local yp = y * math.cos(a) - z * math.sin(a)

print(&quot;a &quot;, math.deg(a), &quot; zp-z &quot;, zp-z, &quot; yp-y &quot;, yp-y)

	local transVec = Vector3.new(0, yp-y, zp-z)

	local floor = Instance.new(&quot;Part&quot;)
	floor.Name = &quot;FloorTile&quot;
	floor.formFactor = 0
	floor.Anchored = true
	floor.BrickColor = COLOR_FLOOR
	floor.Material = Enum.Material.Slate
	floor.Size = Vector3.new(wid, FLOOR_THICKNESS, len)

	floor.CFrame = cf * rcf + transVec

print(&quot;floor.CFrame after &quot;, floor.CFrame)

	floor.Parent = hill
end

