						<string name="Source">------------------------------------------------------------------------------------------
--2009/5/19 Initial version
--2009/5/22 Paint the proto vehicle to match the Duct
--2009/6/25 Add team color number to each vehicle part BackParamA for vehicle anti-TK

-- The Duct in the containing model is the teams vehicle color.

-- Paint all parts of the proto vehicle that are currently the base color.


local GenStation = script.Parent
local RegenButton = GenStation.RegenButton
local GenPad = GenStation.GenPad

local RGColorIdle = RegenButton.BrickColor
local RGColorBusy = BrickColor.Black()

local BASE_COLOR = BrickColor.new(&quot;Dark stone grey&quot;)

local ProtoVehicle

local REGEN_CYCLE_TIME = 30
local REGEN_CYCLE_DISTANCE = 40

local busy = false

local TeamColor

local TEAM_RED = BrickColor.new(&quot;Bright red&quot;)
local TEAM_BLUE = BrickColor.new(&quot;Cyan&quot;)
local VEH_RED = BrickColor.new(&quot;Sand red&quot;)
local VEH_BLUE = BrickColor.new(&quot;Sand blue&quot;)
local VEH_DEFAULT = BrickColor.new(&quot;Dark stone grey&quot;)

------------------------------------------------------------------------------------------
-- See if any part of m (recursively) overlaps (in the xz dimensions) the genPad.  

local function obstructs( m, genPad )

	for k,v in pairs(m:GetChildren()) do

		if v.className == &quot;Model&quot; then

			if obstructs(v, genPad) then
				return true
			end

		elseif (v.className == &quot;Part&quot;) or (v.className == &quot;Seat&quot;) then

			local p = genPad.CFrame:pointToObjectSpace(v.Position)

			if (p.x &gt; -genPad.Size.x/2) and
				(p.x &lt; genPad.Size.x/2) and
				(p.z &gt; -genPad.Size.z/2) and
				(p.z &lt; genPad.Size.z/2) then

				return true
			end
		end
	end

	return false
end

------------------------------------------------------------------------------------------
-- Search the specified m, which on the first call is game.Workspace,
-- and any player character found for a model with CntrUser in it
-- (assumed to be a vehicle).  Remove any such vehicle to clear the genPad.

local function clearGenPad( m, genPad )

	for k,v in pairs(m:GetChildren()) do

		if v.className == &quot;Model&quot; then

			if v:FindFirstChild(&quot;CntrUser&quot;) then

				if obstructs(v, genPad) then
					v:Remove()
				end

			elseif v:FindFirstChild(&quot;Torso&quot;) then
				clearGenPad( v, genPad)
			end
		end
	end
end

----------------------------------------------------------------------------------------
-- Enable all the scripts in the specified model

local function enableScripts(m)

	for k,v in ipairs(m:GetChildren()) do

		if v.className == &quot;Script&quot; then
			v.Disabled = false

		elseif v.className == &quot;Model&quot; then
			enableScripts(v)
		end
	end
end

-----------------------------------------------------------------------------------------------
local function genVeh()

	--print(&quot;regening car &quot;, debounce)

	local curVeh = ProtoVehicle:Clone()
	enableScripts(curVeh)
	curVeh.Parent = game.Workspace

	curVeh:MakeJoints()

	return curVeh

end

-----------------------------------------------------------------------------------------------
-- Wait for the specified vehicle to be threshold distance away from the 
-- genPad or timeout, whichever comes first.

local function waitForStationReady(veh, genPad)

	local chassis = veh:FindFirstChild(&quot;Chassis&quot;)

	if not chassis then
		return
	end

	for t = 1, REGEN_CYCLE_TIME do

		wait(1)

		if not chassis.Parent then
			return
		end

		if (chassis.Position - genPad.Position).magnitude &gt; REGEN_CYCLE_DISTANCE then
			return
		end
	end
end


-----------------------------------------------------------------------------------------------
local function onTouch(part)
--print(&quot;touched regen button &quot;, debounce)

	if busy then
		return
	end

	local h = part.Parent:FindFirstChild(&quot;Humanoid&quot;)

	if not h then
		return
	end

	local Player = game.Players:playerFromCharacter(part.Parent)

	if not Player then
		return
	end

	-- Do not allow enemy to gen vehicles

	if Player.TeamColor ~= TeamColor then

		--print(&quot;PlayerTeamColor &quot;, Player.TeamColor, &quot; TeamColor &quot;, TeamColor)
		return
	end

	busy = true

	RegenButton.BrickColor = RGColorBusy

	clearGenPad( game.Workspace, GenPad )

	local veh = genVeh()

	local CntrOwner = veh:FindFirstChild(&quot;CntrOwner&quot;)
	if CntrOwner then
		CntrOwner.Value = Player

		-- Assign latest owner seq number to determine order of ownership
		if _G.assignOwnerSeq then
			_G.assignOwnerSeq(veh:FindFirstChild(&quot;CntrOwnerSeq&quot;))
		end

	end

	waitForStationReady(veh, GenPad)

	RegenButton.BrickColor = RGColorIdle
	busy = false

end

------------------------------------------------------------------------------------------
-- Change the color of all Parts of v that are baseColor to newColor 

local function paintVehicle( m, baseColor, newColor, teamColorNumber )

	for k,v in pairs(m:GetChildren()) do

		if v.className == &quot;Model&quot; then
			paintVehicle(v, baseColor, newColor, teamColorNumber)

		elseif (v.className == &quot;Part&quot;) then

			v.BackParamA = teamColorNumber

			if v.BrickColor == baseColor then
			--print(&quot;change colors&quot;, v.BrickColor,&quot; &quot;, baseColor)
				v.BrickColor = newColor
			end
		end
		--if (v.className == &quot;Part&quot;) then
			--print(&quot;colors&quot;, v.BrickColor,&quot; &quot;, baseColor)
		--end
	end
end


-----------------------------------------------------------------------------------------

for k,v in pairs(GenStation:GetChildren()) do

	if v.className == &quot;Model&quot; then

		if v:FindFirstChild(&quot;CntrOwner&quot;) then
			ProtoVehicle = v
			break
		end
	end
end

if ProtoVehicle then

	local duct = GenStation.Parent:FindFirstChild(&quot;Duct&quot;)

	-- Determine our team color from the duct color

	if duct.BrickColor == VEH_RED then
		TeamColor = TEAM_RED
	else
		TeamColor = TEAM_BLUE
	end

	if duct then
		paintVehicle( ProtoVehicle, BASE_COLOR, duct.BrickColor, TeamColor.Number)
	end

	ProtoVehicle.Parent = nil
	RegenButton.Touched:connect(onTouch)
else

	print(&quot;Could not find proto vehicle&quot;)
end
