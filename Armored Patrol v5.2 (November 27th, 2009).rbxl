-- Adjust barrel to specified direction and elevation relative to Chassis.
-- Check if requested angles are within range and reduce as needed.
-- Return true if barrel could be adjusted as requested and false if
-- at least one specification was outside of range.

local function adjustBarrel(da, ea)

	local ok = true

	if ea &gt; MaxUpAngle then
		ea = MaxUpAngle
		ok = false

	elseif ea &lt; MaxDownAngle then
		ea = MaxDownAngle
		ok = false
	end

	if da &gt; MaxRightAngle then
		da = MaxRightAngle
		ok = false

	elseif da &lt; MaxLeftAngle then
		da = MaxLeftAngle
		ok = false
	end

	local dircf = CFrame.fromAxisAngle(Vector3.new(0,1,0), da)
	local elevcf = CFrame.fromAxisAngle(Vector3.new(-1,0,0), ea)

	--refreshClient()

	--Mweld.C1 = CFrame.new(0, 0, 1.5) * dircf * elevcf

	Mweld.C1 = CFrame.new(0, 0, 1.5) * elevcf * dircf

	return ok

end


---------------------------------------------------------------------------------------------
local function launchBolt( target )

	alarm = false

	--refreshClient()

	-- Source pos near rear center of Chassis
	local sourcePos = Chassis.CFrame:pointToWorldSpace(Vector3.new(0,7,1.5))

	local vec = target - sourcePos

	--print(&quot;got aim&quot;, vec)

	local vru = Chassis.CFrame:vectorToObjectSpace(vec).unit 

	local ea = math.asin(vru.y)
	local da = math.atan2(vru.x,-vru.z)

	if not adjustBarrel(da, ea) then
		alarm = true
	end

	if (not reloading) and intact()then

		-- Launch pos is 5 &quot;above&quot; end of barrel with respect to its new aimed orientation.
		-- This is to insure the missile does not hit the Howitzer itself.

		local launchPos = Barrel.CFrame:pointToWorldSpace(Vector3.new( 0, -4.5,  5))

		-- Fire in direction Mount actually faces.  Thus if we are blasted enough 
		-- that manipulating the Mweld does not really point the barrel where we want we
		-- will fire innacurately.
		fire(launchPos, Mount.CFrame - Mount.Position)		
	end

	setStatus()

	-- Set wheels in (very slow) motion so the barrel aiming will be forced down to Clients
	wheelMotion(.00001)
end


--------------------------------------------------------------------------------------------------
local function computeLaunchSpeed(source, target, angle, gravity)

	local v = target - source
	local mxz = Vector3.new(v.x, 0, v.z).magnitude

	local r = (gravity * mxz * mxz) / ( 2 * math.cos(angle) * math.cos(angle) * ( (mxz * math.tan(angle)) - v.y ) )

	if r &lt; 0 then
		return nil
	end

	return math.sqrt(r)


end


-------------------------------------------------------------------------------------------------
local function fireShell(sourcePos, speed, launchVec, effectiveGrav, boltCode )

	local bolt = ProtoShell:Clone()
	bolt.Name = &quot;Shell&quot;

	bolt.FrontParamA = boltCode		-- ASV Shell parameter set
	bolt.FrontParamB = speed		-- Launch speed
	bolt.CFrame = CFrame.new(sourcePos, sourcePos + launchVec)

	-- Enable any script in the Shell
	for k,v in pairs(bolt:GetChildren()) do
		if v.className == &quot;Script&quot; then
			v.Disabled = false
		end
	end

	bolt.Creator.Value = CntrUser.Value
	bolt.BrickColor = CntrUser.Value.TeamColor

	bolt.Parent = game.Workspace

	reloading = true

	local co = coroutine.create(reload)
	assert(coroutine.resume(co, bolt))

	return bolt
end

---------------------------------------------------------------------------------------------
local function launchShell( target, boltCode )

	alarm = false

	-- Source pos near rear center of Chassis
	local sourcePos = Chassis.CFrame:pointToWorldSpace(Vector3.new(0,5,1.5))

	local angle = _G.glBoltParms[boltCode].parmAngle
	local maxSpeed = _G.glBoltParms[boltCode].parmSpeed
	local effectiveGrav = _G.glBoltParms[boltCode].parmGravity

	local speed = computeLaunchSpeed(sourcePos, target, math.rad(angle), effectiveGrav)

	--print(&quot;launch speed &quot;, speed)

	-- If we are not angled steeply enough to hit target then arbitrarity pick a speed
	if not speed then
		--print(&quot;Cannot hit target with specified elevation&quot;)	
		speed = maxSpeed/2
	end	

	if speed &gt; maxSpeed then
		speed = maxSpeed
	end

	-- v3 is the 3d world space launch vector
	local v = target - Chassis.Position
	local v2 = Vector3.new(v.x, 0, v.z).unit * math.cos(math.rad(angle))
	local v3 = Vector3.new(v2.x, math.sin(math.rad(angle)), v2.z)

	-- Direction in Chassis object space of launch vector
	local v3ou = Chassis.CFrame:vectorToObjectSpace(v3).unit 

	-- Direction angle in Chassis object space
	local da = math.atan2(v3ou.x,-v3ou.z)

	-- Elev angle in Chassis object space
	local ea = math.asin(v3ou.y)

	if not adjustBarrel(da, ea) then
		alarm = true
	end

	-- Use the actual vector Mount is facing as the launch vector
	-- since the computed vector might be beyond its limits.

	local vec = Mount.CFrame.lookVector

	if (not reloading) and intact() then

		-- Launch pos is 5 &quot;above&quot; end of barrel with respect to its new aimed orientation.
		-- This is to insure the missile does not hit the Howitzer itself.
		local launchPos = Barrel.CFrame:pointToWorldSpace(Vector3.new( 0, -4.5,  5))

		local shell = fireShell(launchPos, speed, vec, effectiveGrav, boltCode)
	end

	setStatus()

	-- Set wheels in (very slow) motion so the barrel aiming will be forced down to Clients
	wheelMotion(.00001)
end

---------------------------------------------------------------------------------------------
-- Handle changes to the CntrCmd and CntrAim value objects.  We wait for
-- a change to something non-nil in the CntrCmd and (if the command indicates
-- it has accompanying aim info) in the CntrAim.  Note that the cmd and aim info
-- can come in any order.  After processing the command the tool forces both CntrCmd and
-- CntrAim to nil in preparation for next usage (note, of course, this will
-- trigger another Changed event here but this is ignored since we only act on 
-- non-nil values).

function handleCmdAndAim()

	-- If we have not gotten the command portion yet, do not handle yet.
	if (CntrCmd.Value == nil) or  (CntrCmd.Value == NIL_CMD) then
		return
	end

	--If the command portion indicates there is accompanying aim info and
	-- it has not arrived yet then do not handle.
	if string.match(CntrCmd.Value, &quot;+&quot;) and 
	   ((CntrAim.Value == nil) or (CntrAim.Value == NIL_AIM)) then
		return
	end

	-- Else we have received complete command.

	--print(&quot;received complete command&quot;)

	local cmd = string.sub(CntrCmd.Value, 1,1)	-- Strip out just command char

	if cmd == &quot;f&quot; then
		launchBolt(CntrAim.Value)

	else
		local boltCode 

		if (cmd == &quot;b&quot;) then
			boltCode = BOLTG_CODE_30

		elseif (cmd == &quot;g&quot;)  then
			boltCode = BOLTG_CODE_45

		elseif (cmd == &quot;t&quot;) then
			boltCode = BOLTG_CODE_70

		end

		if boltCode then
			launchShell(CntrAim.Value, boltCode)
		end
	end

end


-----------------------------------------------------------------------------------------------------------
-- It has been determined we are no longer physically hitched to the parent Jeep.
-- At this time we move ourselves to game.Workspace to act as an independent unit.
-- Since we poll for unhitched status, there is a window between the time the 
-- Jeep unhitches and we detect it.  The Jeep could leave the game during this time.
-- However, we hold a reference to the Jeep established at init time that should 
-- continue to be valid.

local function unHitch()

	hitched = false

	Howitzer.Parent = game.Workspace

	local cntrOwner = Howitzer:FindFirstChild(&quot;CntrOwner&quot;)

	local jeepCntrOwner = Jeep:FindFirstChild(&quot;CntrOwner&quot;)

	-- Inherit the current Jeeps owner
	if cntrOwner and jeepCntrOwner then
		cntrOwner.Value = jeepCntrOwner.Value
		Howitzer.CntrOwnerSeq.Value = Jeep.CntrOwnerSeq.Value
	end
end

-----------------------------------------------------------------------------------------------------------
-- These routines monitor for when we are no longer hitched to Jeep.  Several checks are involved in this:

--	- If the parent Jeep is no longer in the world then we are unhitched.

--	- If Jeep.HitchBar is no longer in the world then we are unhitched

-- 	- If the Hitch part is no longer in the world then we are unhitched.

--	- If the Hitch part is no longer the same distance from the parent Jeep Chassis (compared to
--	  gen time) then we are unhitched
--

local origHitchDistance = 10000

if Jeep then
	origHitchDistance = (Jeep.Chassis.Position - Hitch.Position).magnitude

-- Else no Jeep part, we were gened outside of a parent Jeep.
else
	hitched = false
end

local function stillHitched()

	if not hitched then
		return false
	end

	if Hitch.Parent == nil then
		unHitch()
		return false
	end

	if (Jeep == nil) or (Jeep.Parent == nil) then
		unHitch()
		return false
	end

	if Jeep:FindFirstChild(&quot;HitchBar&quot;) == nil then
		unHitch()
		return false
	end

	local hd = (Jeep.Chassis.Position - Hitch.Position).magnitude 

	if math.abs(origHitchDistance - hd) &gt; 1 then
		unHitch()
		return false
	end

	return true


end
-----------------------------------------------------------------------------------------------------------

CntrUser.Changed:connect(setStatus)

--CntrCmd.Changed:connect(function() print(&quot;CntrCmd received: &quot;, CntrCmd.Value); handleCmdAndAim() end)
--CntrAim.Changed:connect(function() print(&quot;CntrAim received: &quot;, CntrAim.Value); handleCmdAndAim() end)
CntrCmd.Changed:connect(handleCmdAndAim)
CntrAim.Changed:connect(handleCmdAndAim)


if stillHitched() then

	-- Monitor to see when we are unhitchedfrom parent Jeep.

	while stillHitched() do
		wait(5)
	end

-- Else we must be gened outside of a Jeep.  Mark us as reloading during this time.
else

	reloading = true
	setStatus()

	wait(POST_GEN_DELAY)

	reloading = false
	setStatus()

end

-- Loop until we are no long intact

while intact() do

	wait(5)

	-- Periodically turn off wheel motion so unused Howitzer will not
	-- overly burden physics engine
	wheelMotion(0)

end

Howitzer:Remove()


