-- This is a little package of routines to handle touch events for Anchored objects.
-- This is needed because after the 2009/8/5 Roblox update moving object stopped
-- receiving most touch events from Anchored objects.  So we add a touch handler for
-- every Anchored object and have it call the touch handler registered by the 
-- moving object in the global _G.Projectiles table.

-- This package provides a global routine to add and remove a touch handler so
-- it can be called from outside this script.  Add added touch handlers are recorded
-- in the anchoredTouchConnections table (indexed by Anchored part and give the
-- touch handlers connection) so they can all be removed.

_G.Projectiles = {}

local anchoredTouchConnections = {}

----------------------------------------------------
-- This is the handler for an anchored part when it is touched by another part.

local function anchoredTouchHandler( anchoredPart, touchingPart )

	--print(&quot;us &quot;, anchoredPart, &quot; other: &quot;, touchingPart)

	if _G.Projectiles[touchingPart] then

		--print(&quot;Calling Projectiles handler&quot;)

		_G.Projectiles[touchingPart](anchoredPart)
	end
end


----------------------------------------------------
-- Create a touch handler for the specified part and record this connection 
-- in anchoredTouchConnections.  This routine is made global.

local function anchoredTouchCreate(anchoredPart)

	--print(&quot;Registering &quot;, anchoredPart)

	anchoredTouchConnections[anchoredPart] = 
		anchoredPart.Touched:connect(function(otherPart) anchoredTouchHandler(anchoredPart,otherPart) end)
end

_G.AnchoredTouchCreate = anchoredTouchCreate


----------------------------------------------------
-- Remove a touch handler for the specified part and remove the record of this connection 
-- from anchoredTouchConnections.  This routine is made global.

local function anchoredTouchRemove(anchoredPart)

		local connection = anchoredTouchConnections[anchoredPart]

		if connection then
			print(&quot;DEregistering &quot;, anchoredPart)
			connection:disconnect()
			anchoredTouchConnections[anchoredPart] = nil
		else 
				print(&quot;*********************deregistering: could not find&quot;, anchoredPart)
		end
end

_G.AnchoredTouchRemove = anchoredTouchRemove


	
----------------------------------------------------
-- Go thru the specified model recursively and create a touch handler for each part that is 
-- Anchored. 

local function anchoredTouchCreateAll()

	local l2 = {}

	local nconn = 0
	local oconn = 0

	local function rec( m )

		for k,v in pairs(m:GetChildren()) do
			if v.className == &quot;Part&quot; and v.Anchored then

				-- If we already have such a connection then just copy it from l2, and remove from l2
				if l2[v] then
					anchoredTouchConnections[v] = l2[v]
					l2[v] = nil

					oconn = oconn + 1

				-- Else do not already have, create
				else
					anchoredTouchCreate(v)

					nconn = nconn + 1
				end
				
			elseif v.className == &quot;Model&quot; then
				rec(v)
			end
		end
	end

	local dconn = 0

	l2 = anchoredTouchConnections
	anchoredTouchConnections = {}

	rec(game.Workspace)

	-- Now l2 should be left with those connections we do not need anymore because the 
	-- corresponding part is no longer in the world.

	for p,connection in pairs(l2) do

		if p.Parent then
			print(&quot;Huh?  unused connection but part is still in the world&quot;)

		else
			--connection:disconnect()
			p:Remove()
			l2[p] = nil
	
			dconn = dconn + 1
		end
	end

	print(&quot;new conn &quot;, nconn, &quot; kept conn &quot;, oconn, &quot; deleted conn &quot;, dconn)
	
end


-----------------------------------------------------
-- Remove all connections recorded in the anchoredTouchConnections table and empty that table.

local function anchoredTouchRemoveAll()

	for k,v in pairs(anchoredTouchConnections) do

		anchoredTouchRemove(k)
	end
end


