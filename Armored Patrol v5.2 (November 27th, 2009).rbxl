local function computeHillValue(hillPos, enemyHqPos)
	
	local d = (hillPos - enemyHqPos).magnitude 

	local v = ((1 - d/MAX_DIST) * (MAX_VALUE - MIN_VALUE)) + MIN_VALUE

	return v
end

local valueBlue = computeHillValue(Staff.Position, HqRedFlag.Position)
local valueRed = computeHillValue(Staff.Position, HqBlueFlag.Position)

local function hillValue( team )

	if team == TEAM_RED then
		return valueRed
	elseif team == TEAM_BLUE then
		return valueBlue
	else
		return 0
	end
end


------------------------------------------------------------------------------------------------

local function onTouched( part )

	--print(&quot;touched by &quot;, part.Name, &quot; color num &quot;, part.BrickColor.Number, &quot; BackParamA &quot;, part.BackParamA)
	
	local team

	local torso = part.Parent:FindFirstChild(&quot;Torso&quot;) or part.Parent.Parent:FindFirstChild(&quot;Torso&quot;)
		
	if torso then
		team = torso.BrickColor

	else

		if _G.DamageHandler and _G.DamageHandler[part]  then
	
			if part.BackParamA == TEAM_RED.Number then
				team = TEAM_RED
	
			elseif part.BackParamA == TEAM_BLUE.Number then
				team = TEAM_BLUE
			end
		end
	end

	-- If not touched by a player or a vehicle then ignore
	if not team then
		return
	end

	-- If touching team already owns hill then insure any claiming by other team is stopped
	if team == owner then
		claimer = nil
		claimTime = 0
		Flag.BrickColor = team
		FlagMesh.Offset = Vector3.new(-2,FLAG_OFFSET_HIGH,0)
		return
	end

	-- If touching team is already in the act of claiming then ignore

	if team == claimer then
		return
	end

	-- Toucher does not already own hill and is not currently claiming, start the claiming process

	claimer = team
	claimTimer = 0

end


---------------------------------------------------------------------------------------------------------
-- Does the position lie over the top

local function overlap( top, pos)

	local rpos = top.CFrame:pointToObjectSpace(pos)

	if pos.y &lt; Staff.Position.y then
		return false
	end

	if (rpos.x &gt; -SIZEXH) and (rpos.x &lt; SIZEXH) and (rpos.z &gt; -SIZEZH) and (rpos.z &lt; SIZEZH) then
		return true
	end

	return false
end


-------------------------------------------------------------------------------------------------------
-- Check to see if any player of the designated team is on top of the CTH hill

local function checkForPlayer( teamColor )

	for k,p in pairs(game.Players:GetChildren()) do

		if p.TeamColor == teamColor then

			local ch = p.Character

			if ch then
				print(&quot;character&quot;)

				local t = ch:FindFirstChild(&quot;Torso&quot;)

				if t then
					if overlap(Top, t.Position) then
						return true
					end
				end
			end
		end
	end

	return false
end



