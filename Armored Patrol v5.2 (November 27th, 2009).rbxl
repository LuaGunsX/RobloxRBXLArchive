--2009/3/23 Initial version
--2009/4/7 Unanchor all parts after done.
-- 2009/5/2 Weld all parts that are in BodyWeld
--2009/5/5 Make weld parent game.Workspace
--2009/5/6 Make welding part optional based on existence of BodyWeld sub-Model


-- This script creates a weld between the chassis and every part named WeldPart

-- It also unanchors all parts after a 3 second wait.


local Model = script.Parent
local BodyWeld = script.Parent:FindFirstChild(&quot;BodyWeld&quot;)
local Chassis = Model.Chassis


---------------------------------------------------------------------
local function weldOne( item )

	--print(&quot;Welding: &quot;, item.Name)

	local weld = Instance.new(&quot;Weld&quot;)

	local cf = Chassis.CFrame:toObjectSpace(item.CFrame)

	weld.Part0 = Chassis
	weld.Part1 = item
	weld.C0 = cf
	weld.Parent = game.Workspace

end

-------------------------------------------------------------------
local function weldAll( model )

	for k,v in pairs(model:GetChildren()) do

		if (v.className == &quot;Part&quot;) and string.match(v.Name,&quot;Weld&quot;) then
			weldOne(v)



		elseif v.className == &quot;Model&quot; then
			weldAll(v)

		end
	end
end

-------------------------------------------------------------------
local function unanchorAll( model )

	for k,v in pairs(model:GetChildren()) do

		if (v.className == &quot;Part&quot;) or (v.className == &quot;Seat&quot;) then
			v.Anchored = false

		elseif v.className == &quot;Model&quot; then
			unanchorAll(v)

		end
	end
end

------------------------------------------------------------------

if BodyWeld then
	weldAll(BodyWeld)
end

wait(3)

unanchorAll(Model)




