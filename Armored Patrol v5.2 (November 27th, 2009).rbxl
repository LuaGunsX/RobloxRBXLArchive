-- 2009/9/9 Initial version
-- 2009/9/9 Flight path decays after reaching range limit
--2009/9/10 Make NON_COLLIDE dist the maximum of 25 and the parmSlabIgnoreDist
--2009/9/11 Missile.CanCollide=false and &quot;hands on&quot; approach



--[[
	- The firing weapon clones ProtoMissile, aims it by setting its .CFrame, and enables this script.

	- ProtoMissile Part has this script,  Mesh,  and BodyForce.

	- BodyForce removes gravity.

	- Missile flies in straight line only by initial setting of .Velocity.

	- CanCollide set to false for whole flight.

	- Every wait() interval Missile.RotVelocity changed incrementally to insure we get all touch events.

	- Parameter set code selects one of a fixed set of parameter configurations


	Parameters based in the Bolt part:

		bolt.CFrame 		-- Launch vector
		bolt.Velocity 		-- Any initial velocity caused by mount movement (which is ignored)
		bolt.FrontParamA	-- Parameter set code

		This code selects one set of values for various Bolt parameters.

--]]


local Bolt = script.Parent

-- Get bolt parameters from the library based on the parm set code in FrontParamA
local Ps = _G.glBoltParms[Bolt.FrontParamA]

--print(&quot;Ps.parmSlabIgnoreDist &quot;, Ps.parmSlabIgnoreDist)

local InitalPosition
local connection
local explosion
local fireSound
local blastPosition

local parmHardRadius = Ps.parmDefHardRadius
local parmSoftRadius = Ps.parmDefSoftRadius
local parmDefForce = Ps.parmDefForce
local parmRange = Ps.parmRange
local parmSpeed = Ps.parmSpeed
local parmSlabIgnoreDist = Ps.parmSlabIgnoreDist


------------------------------------------------------------------------------------------------------------------

local function onHit(part,distance)

	--print(&quot;Expl at &quot;, explosion.Position,&quot; hit &quot;, part.Name, &quot; pos &quot;, part.Position, &quot; hit dist: &quot;)

	if _G.DamageHandler and _G.DamageHandler[part]  then

		--print(&quot;Found damage hndlr for &quot;,  part.Name, &quot; pos &quot;, part.Position, &quot; hit dist: &quot;, distance, &quot; parmHardRadius &quot;, parmHardRadius)

		if distance &lt; parmHardRadius then

			--print(&quot;Calling damage handler for part: &quot;, part.Name, &quot; dist &quot;, distance, &quot; force &quot;, parmDefForce, &quot; radius &quot;, parmHardRadius)

			_G.DamageHandler[part](part, distance, parmDefForce, parmHardRadius)	
		end
	end
end


------------------------------------------------------------------------------------------------------------------------

local firstTouch = true

function onTouch(part)

	--print(string.format(&quot;touch on %s %s BoltPos %.1f,%.1f,%.1f bolt collidable: %s&quot;, part.Name, part.BrickColor.Name, 
		--Bolt.Position.x, Bolt.Position.y, Bolt.Position.z, tostring(Bolt.CanCollide)))

	--do return end

	if firstTouch == false then

		--print(&quot;Bolt: received subsequent touch from &quot;, part.Name)
		return
	end

	-- Ignore touch of our barrel or others barrel
	if part.Name == &quot;Barrel&quot; then
		return
	end

	-- Ignore non-solid slabs
	if part.Name == &quot;SlabInProgress&quot; then
		return
	end

	local distTravelled = (part.Position - InitialPosition).magnitude

	-- The parmSlabIgnoreDist allows certain projectiles to go thru Slabs.  This is to
	-- allow the creation of bunker like fortification from which the Howitzer can shoot
	-- without having to try to make windows for the Bolt to go thru.

	if (part.Name == &quot;Slab&quot;) and (distTravelled &lt; parmSlabIgnoreDist) then
		return
	end

	-- If we hit a player close to launch point assume it was the player doing the 
	-- launching and ignore.

	local torso = part.Parent:FindFirstChild(&quot;Torso&quot;) or part.Parent.Parent:FindFirstChild(&quot;Torso&quot;)

	if  torso and (distTravelled &lt; 5) then
	
		--print(&quot;Ignore touch of: &quot;, part.Name)
		return
	end

	-- No more touch events for the Bolt

	firstTouch = false

	if connection then
		connection:disconnect()
		connection = nil
	end

	-- Derive a pseudo source and target  so we can call the findTp().  Take
	-- Current position and current Velocity and backup 2000 for source and
	-- go forward 2000 for target

	local source = Bolt.Position - (Bolt.Velocity.unit * 2000)
	local target = Bolt.Position + (Bolt.Velocity.unit * 2000)

	local direct,contactPoint,normal,code = _G.glFindTp(source,target, part)	

	-- Make blastPosition one unit above the contacting surface
	blastPosition = contactPoint + normal.lookVector

	--print(&quot;Bolt: contact point: &quot;, contactPoint, &quot; Bolt pos&quot;, Bolt.Position)

	-- Do not allow to blow up self.  Just let Bolt
	-- fall to ground for a couple seconds.  Let main loop remove.
	if (contactPoint - InitialPosition).magnitude &lt;= Ps.parmArmDist then

		--print(&quot;Disarmed by touch of &quot;, part.Name)

		Bolt.CFrame = Bolt.CFrame + (blastPosition - Bolt.Position)
		Bolt.Velocity = Vector3.new(0,0,0)
		Bolt.CanCollide = true
		fireSound:Stop()

		local misfire = Instance.new(&quot;Sound&quot;)
		misfire.SoundId = &quot;rbxasset://sounds\\Kerplunk.wav&quot;
		misfire.Parent = Bolt
		misfire.Volume = .5
		misfire.PlayOnRemove = false
		misfire:play()

		Bolt.BodyForce.force = Bolt.BodyForce.force * .1

		return
	end

	-- If a direct hit on a player then blast player
	if torso then
		_G.glBlastPlayer(torso, blastPosition, Bolt)

	-- Else determine if any players in range of softRadius
	-- Before doing the check move the bolt far away so it does not
	-- interfere with probing.  
	else
		Bolt.Position = Bolt.Position + Vector3.new(0,60,0)

		_G.glAssessPlayers(blastPosition, parmSoftRadius, Bolt)
	end

	-- Create explosion
	explosion = Instance.new(&quot;Explosion&quot;)
	explosion.Position = blastPosition
	explosion.BlastRadius = parmHardRadius
	explosion.BlastPressure = 0
	explosion.Parent = Bolt

	explosion.Hit:connect(onHit)

	local sound = Instance.new(&quot;Sound&quot;)
	sound.SoundId = &quot;rbxasset://sounds\\collide.wav&quot;
	sound.Parent = Bolt
	sound.Volume = 1
	sound.PlayOnRemove = false
	sound:play()

--[[
	local marker = Instance.new(&quot;Part&quot;)
	marker.formFactor = 0
	marker.Shape = 0
	marker.BrickColor = BrickColor.Green()
	marker.Anchored = true
	marker.Size = Vector3.new(1,1,1)
	marker.CanCollide = false
	marker.CFrame = CFrame.new(blastPosition)
	marker.Parent = game.Workspace
--]]

	-- Since we are waiting a little for sound to play and for Hit events to be collected
	-- we mark our Bolt as invisible so it does not seem to go thru things.

	Bolt.Transparency = 1

	wait(1)

	Bolt:remove()
end


--------------------------------------------------------------------------------------------------
-------------------------------------------------------------------------------------------------

Bolt.Anchored = false
Bolt.Transparency = 0
Bolt.RotVelocity = Bolt.CFrame.lookVector * 8

InitialPosition = Bolt.Position

-- Make a sound at firing time
fireSound = Instance.new(&quot;Sound&quot;)
fireSound.SoundId = &quot;rbxasset://sounds\\Shoulder fired rocket.wav&quot;
fireSound.Parent = Bolt
fireSound.Volume = .5
fireSound.PlayOnRemove = false
fireSound:play()

Bolt.Velocity = Bolt.CFrame.lookVector * parmSpeed

connection = Bolt.Touched:connect(onTouch)

local r = game:service(&quot;RunService&quot;)
local TIME_MAX = 5
local INTERVALS_PER_SECOND = 30
local t = 0

local ROT_INCR = Vector3.new(.001,.001,.001)

local decay = false
local inert = false

while t &lt; TIME_MAX  do	

	local _,s = r.Stepped:wait()
	t = t + s

	-- Modify the .RotVelocity an undetectable amount in order to insure touch events occur.
	Bolt.RotVelocity = Bolt.RotVelocity + ROT_INCR

	local d = (Bolt.Position - InitialPosition).magnitude

	-- If we are beyond range then start to remove the lift provided by BodyForce

	if d &gt; parmRange then
		if not decay then

			decay = t
			Bolt.RotVelocity = Vector3.new(math.random(0,3),math.random(0,3),math.random(0,3))
		end

		if (not inert) and (t &gt; decay + .5) then

			inert = true

			Bolt.BrickColor = BrickColor.White()
			Bolt.CanCollide = true

			if connection then 
				connection:disconnect() 
				connection = nil
			end
		end

		local f = Bolt:FindFirstChild(&quot;BodyForce&quot;)
			
		if f then
			f.force= f.force * .9
		end
		Bolt.Velocity = Bolt.Velocity * .96
	end
end

if connection then 
	connection:disconnect() 
	connection = nil
end


if  Bolt.Parent then
	fireSound:Stop()
	Bolt:Remove()
end

