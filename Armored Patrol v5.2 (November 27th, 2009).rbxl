-- GenStationProto indexed by GenStation model and contains the original
-- parent of that GenStations
local GenStationProto = {}

-- GenStationCurrent indexed by GenStation maintains a list of currently active 
-- GenStations.
local GenStationCurrent = {}

-----------------------------------------------------------------------------------------
-- Get list of all genstations and their parents
local function findGenStations( m )

	for k,v in pairs(m:GetChildren()) do

		if v.className == &quot;Model&quot; then

			if v.Name == &quot;GenStation&quot; then

				GenStationProto[v] = v.Parent
				v.Parent = nil
			else
				findGenStations( v )
			end
		end
	end
end

-----------------------------------------------------------------------------------------
local function removeCurrentGenStations()

	for k,v in pairs(GenStationCurrent) do

		k:Remove()
		GenStationCurrent[k] = nil

	end
end

-----------------------------------------------------------------------------------------
local function genGenStations()

	for k,v in pairs(GenStationProto) do

		local g = k:Clone()
		
		-- Enable any script in the top level of the GenStation

		for k2,v2 in pairs(g:GetChildren()) do
			if v2.className == &quot;Script&quot; then
				v2.Disabled = false
			end
		end

		g.Parent = v

		GenStationCurrent[g] = true
	end
end


----------------------------------------------------------------------------------------
-- Recurse thru the specified model and replace any placeholder script (those
-- starting with &quot;ph-&quot;) with the same named one from the scriptList.  All other scripts 
-- found just mark as disabled.

local function replaceScripts(m)

	for k,v in ipairs(m:GetChildren()) do

		if (v.className == &quot;Script&quot;) and string.match(v.Name, &quot;%-ph&quot;) then

			local bn = string.gsub(v.Name, &quot;%-ph&quot;, &quot;&quot;)

			--print(&quot;Found placehold script &quot;, v.Name)
	
			if scriptList[bn] then

				--print(&quot;Replacing with &quot;, scriptList[bn].Name)
	
				local s = scriptList[bn]:Clone()
				s.Parent = v.Parent
				s.Disabled = true
				v.Parent = nil
			else
				v.Disabled = true
			end

		elseif v.className == &quot;Model&quot; or v.className == &quot;Part&quot; then
			replaceScripts(v)
		end
	end
end

