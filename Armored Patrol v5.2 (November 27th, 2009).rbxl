local function deleteRec( m )

	for k,v in ipairs(m:GetChildren()) do

		if v.className == &quot;Model&quot; then
			deleteRec( v )
			v:Remove()

		elseif v.className == &quot;Part&quot; then
			v:Remove()

		elseif v.className == &quot;Script&quot; then
			v:Remove()
		end
	end
end

function _G.TerrainRemove()

	deleteRec(Hills)

end











