function onUnequipped()

	if Vehicle and CntrGunner.Value == Player then
		CntrGunner.Value = nil
	end

	game.Workspace.Camera.CameraType = Enum.CameraType.Custom

	_G.clearHint()

	Vehicle = nil

end
