-- Find the point on the surface of p that an object going from source to target would
-- hit.  We compute the intersection of the path of the center of the object with p.
-- Of course the object has non-zero dimensions so the edge of it may have hit us
-- even though the center does not pass thru p.  To compensate for this we move the 
-- source and target .5 closer to the surface we are examining (this assumes a projectile
-- that is thickness of 1).  If there is still no direct hit return the 
-- hit to the extended plane of each surface that the center of the object passes closest
-- to p.  In this case we return as position the edges of p that is closest to the hit.
-- Return the following:
--
--	-- Boolean: true if there is direct hit, false if not
--	-- The point on or closest to the surface as defined above
--	-- The normal to this surface.


local function findTp(source,target, p) 

	--print(&quot;In findTp NORMAL_ZN: &quot;, NORMAL_ZN)

	local s = p.CFrame:pointToObjectSpace(source)
	local t = p.CFrame:pointToObjectSpace(target)
	local sx = s.x
	local sy = s.y
	local sz = s.z
	local tx = t.x
	local ty = t.y
	local tz = t.z
	local sizexh = p.Size.x/2
	local sizeyh = p.Size.y/2
	local sizezh = p.Size.z/2
	local x,y,z

--[[
print(string.format(&quot;source (world): %.1f,%.1f,%.1f&quot;, source.x, source.y,source.z))
print(string.format(&quot;source(object): %.1f,%.1f,%.1f&quot;, s.x, s.y,s.z))
print(string.format(&quot;target (world): %.1f,%.1f,%.1f&quot;, target.x, target.y,target.z))
print(string.format(&quot;target(object): %.1f,%.1f,%.1f&quot;, t.x, t.y,t.z))
print(&quot;sizexh :&quot;, sizexh, &quot; sizeyh: &quot;, sizeyh, &quot; sizezh&quot;, sizezh)
--]]
	local r1,r2, d1, d2,cx,cy,cz,n
	local sd = 1000 -- some impossibly big number

	if sz &lt; -sizezh then
		--print(&quot;z-&quot;)
		r1,x,d1 = inters(sx,tx, sizexh, sz,tz, -sizezh-.5)
		r2,y,d2 = inters(sy,ty, sizeyh, sz,tz, -sizezh-.5)

		if r1 and r2 then
			return true, 
				p.CFrame:pointToWorldSpace(Vector3.new(x,y, -sizezh)),
				p.CFrame:toWorldSpace(NORMAL_ZN)
		end

		if d1 + d2 &lt; sd then
			--print(&quot;z-&quot;)
			cx,cy,cz,n = x,y,-sizezh,NORMAL_ZN
			sd = d1 + d2
		end
	end 

	if sz &gt; sizezh then
		--print(&quot;z+&quot;)
		r1,x,d1 = inters(sx,tx, sizexh, sz,tz, sizezh+.5)
		r2,y,d2 = inters(sy,ty, sizeyh, sz,tz, sizezh+.5)

		if r1 and r2 then
			return true,
				p.CFrame:pointToWorldSpace(Vector3.new(x,y, sizezh)),
				p.CFrame:toWorldSpace(NORMAL_ZP)
					
		end

		if d1 + d2 &lt; sd then
			--print(&quot;z+&quot;)
			cx,cy,cz,n = x,y,sizezh,NORMAL_ZP
			sd = d1 + d2
		end
	end 

	if sx &lt; -sizexh then
		--print(&quot;x-&quot;)
		r1,y,d1 = inters(sy,ty, sizeyh, sx,tx, -sizexh-.5)
		r2,z,d2 = inters(sz,tz, sizezh, sx,tx, -sizexh-.5)

		if r1 and r2 then
			return true,
				p.CFrame:pointToWorldSpace(Vector3.new(-sizexh,y, z)),
				p.CFrame:toWorldSpace(NORMAL_XN)
		end

		if d1 + d2 &lt; sd then
			--print(&quot;x-&quot;)
			cx,cy,cz,n = -sizexh,y, z,NORMAL_XN
			sd = d1 + d2
		end
	end

	if sx &gt; sizexh then
		--print(&quot;x+&quot;)
		r1,y,d1 = inters(sy,ty, sizeyh, sx,tx, sizexh+.5)
		r2,z,d2 = inters(sz,tz, sizezh, sx,tx, sizexh+.5)

		if r1 and r2 then
			return true,
				p.CFrame:pointToWorldSpace(Vector3.new(sizexh,y, z)),
				p.CFrame:toWorldSpace(NORMAL_XP)
		end

		if d1 + d2 &lt; sd then
			--print(&quot;x+&quot;)
			cx,cy,cz,n = sizexh,y, z,NORMAL_XP
			sd = d1 + d2
		end
	end

	if sy &lt; -sizeyh then
		--print(&quot;y-&quot;)
		r1,x,d1 = inters(sx,tx, sizexh, sy,ty, -sizeyh-.5)
		r2,z,d2 = inters(sz,tz, sizezh, sy,ty, -sizeyh-.5)

		if r1 and r2 then
			return true,
				p.CFrame:pointToWorldSpace(Vector3.new(x,-sizeyh,z)),
				p.CFrame:toWorldSpace(NORMAL_YN)
		end

		if d1 + d2 &lt; sd then
			--print(&quot;y-&quot;)
			cx,cy,cz,n = x,-sizeyh,z,NORMAL_YN
			sd = d1 + d2
		end
	end 

	if sy &gt; sizeyh then
		--print(&quot;y+&quot;)

		r1,x,d1 = inters(sx,tx, sizexh, sy,ty, sizeyh+.5)
		r2,z,d2 = inters(sz,tz, sizezh, sy,ty, sizeyh+.5)

		if r1 and r2 then
			return true,
				p.CFrame:pointToWorldSpace(Vector3.new(x,sizeyh,z)),
				p.CFrame:toWorldSpace(NORMAL_YP)
		end

		if d1 + d2 &lt; sd then
			--print(&quot;y+&quot;)
			cx,cy,cz,n = x,sizeyh,z,NORMAL_YP
			sd = d1 + d2
		end
	end 

	-- No direct hit, return hit closest to part

	--print(&quot;no direct hit&quot;)

	return false,
		p.CFrame:pointToWorldSpace(Vector3.new(cx,cy,cz)),
		p.CFrame:toWorldSpace(n)

end

_G.glFindTp = findTp

