-- 2009/3/25 Initial version
--2009/4/7 Get force and blast radius in takeDamage handler
--2009/4/11 Do not damage Chassis, scan for dislodged parts
--2009/5/5 Ignore any contained model with Adjunct in its name, make any parts
--		  in model named Movable as relative to contained Mount

-- Provide the DamageHandler function to be called by something that 
-- creates an explosion, to assess damage on the vehicle.

-- If there is a contained Model with Adjunct in its name it is skipped.

-- If there is a contained Model with Movable in its name then its parts
--     are compared relative with a part named Mount

-- part.FrontParamA - initial damage allowance
-- part.FrontParamB - current damage allowance




if _G.DamageHandler == nil then
	_G.DamageHandler = {}
	local mt = {}
	--mt.__mode = &quot;k&quot; 
	setmetatable(_G.DamageHandler, mt)
end

local Vehicle = script.Parent
local Chassis = Vehicle.Chassis


-----------------------------------------------------------------------------------------------------------------------------------
local function takeDamage( part, distance, force, blastRadius )

	-- Scale damage from 0-force based on blastRadius

	if distance &gt; blastRadius then distance = blastRadius end

	local d = ((blastRadius - distance)/blastRadius) * force

	local orig = part.FrontParamB

	part.FrontParamB = part.FrontParamB - d

	local cur = part.FrontParamB
	local max = part.FrontParamA

	--print(&quot;Part &quot;, part.Name, &quot; damage: &quot;, d, &quot; assessed, parmA: &quot;, part.FrontParamA, &quot; parmB: &quot;, part.FrontParamB)

	-- If strength is gone, release the part

	if cur &lt; 0 then
		part:BreakJoints()
		part.CanCollide = false
		local v = Vector3.new(math.random(-30,30), math.random(30,90), math.random(-30,30))
		part.RotVelocity = Vector3.new(math.random() * 3, math.random() * 3, math.random()*3)
		part.Velocity = v


	-- If down to 1/3 original strength then change to inlets

	elseif (orig &gt; max/3) and (cur &lt;= max/3) then

		part.BackSurface = 4
		part.FrontSurface = 4
		part.LeftSurface = 4
		part.RightSurface = 4
		part.TopSurface = 4
		part.BottomSurface = 4


	-- If down to 2/3 original strength then change to weld

	elseif (orig &gt; max*2/3) and (cur &lt;= max*2/3) then

		part.BackSurface = 1
		part.FrontSurface = 1
		part.LeftSurface = 1
		part.RightSurface = 1
		part.TopSurface = 1
		part.BottomSurface = 1


	end
	

end


-------------------------------------------------------------------------------------------------------------------
-- Recursively find parts that are to have a damage handler defined.  Also record in the
-- specified list these parts and their current distance from their reference parts.  
-- For any contained model with &quot;Movable&quot; in its name, the reference part is a part named
-- Mount within that model.  If there is a contained model with &quot;Adjunct&quot; in its name it is 
-- skipped as this is an potentially independent unit with it own damage handler script.

local function findParts(model, list, refPart)

	for k,v in pairs(model:GetChildren()) do

		if v.className == &quot;Model&quot; then

			if string.match(v.Name, &quot;Movable&quot;) then
				findParts(v, list, v.Mount)
		
			elseif not string.match(v.Name, &quot;Adjunct&quot;) then
				findParts(v, list, refPart)
			end

		elseif v.className == &quot;Part&quot; and v.FrontParamA &gt; 0 then

			_G.DamageHandler[v] = takeDamage

			local dist = refPart.CFrame:pointToObjectSpace(v.Position).magnitude
			list[v] = {ref=refPart, dist=dist}	
		end
	end
end
---------------------------------------------------------------------------------------------------

local partRelDist = {}

findParts(Vehicle, partRelDist, Chassis)

while true do

	wait(10)

	-- Periodically go thru all remaining parts and see if their relative distance from
	-- their reference part has changed any.  If yes, then this probably means the 
	-- part has been released by all surrounding parts being destroyed.  Remove such parts.

	for part,d in pairs(partRelDist) do

		if part.Parent then

			-- Find current relative dist from the reference part
			local dist = d.ref.CFrame:pointToObjectSpace(part.Position).magnitude

			local tolerance = .002

			-- Give wheels a bigger tolerance since they move.
			if string.match(part.Name, &quot;Wheel&quot;) then
				tolerance = 5

			-- Give Barrel and BarrelShunt a much bigger tolerance since they move a lot
			elseif string.match(part.Name, &quot;Barrel&quot;) then
				tolerance = 20
			end

			if math.abs(dist - d.dist) &gt; tolerance then
				part:Remove()
				partRelDist[part]  = nil
			end

		else
			partRelDist[part]  = nil	
		end
	end
end




