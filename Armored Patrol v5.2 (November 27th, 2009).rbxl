					<string name="Source">---------------------------------------------------------------------------------------------------
--2009/8/22 Initial version

local RemGenModel = script.Parent

local mod

if _G.RemGenProtoModels == nil then
	_G.RemGenProtoModels = {}
end

for k,v in pairs(RemGenModel:GetChildren()) do

	if v.className == &quot;Model&quot; then
		mod = v
		break
	end
end

if mod == nil then
	print(&quot;ScriptRemGenLib cannot find proto model&quot;)
	return
end

