					<string name="Source">--2009/6/7 Initial version based on GrenadeScript-v1.2
--2009/6/12 Make grenade match team color
--2009/6/14 Make unarmed grenade match team color, move ScriptGrenade to Tool
--2009/7/11 Reposition launch point so we do not immediately touch arm or handle and deactivate
--2009/7/18 Change FormFactor to formFactor
--2009/11/10 v2.4 Use GUI hint

-- Start grenade arc from Player handle, ignore initial touches of player

local Tool = script.Parent

-- This runs while the script is still in game.Players/&lt;playername&gt;.
local Player = Tool.Parent.Parent

local Handle

local GrenadeScript

-----------------------------------------------------------------------------------------------

local state = &quot;idle&quot;
local armedSeqNum = 1

-- Package variable identifying our Player Torso
local Torso 
local Mouse

local arc = 1	-- default high arc

local EFFECTIVE_g = 100
local MAX_SPEED = 110

-----------------------------------------------------------------------------------------------
-- Constants
local RELOAD_TIME = 5
local FUSE_TIME = 3

local COLOR_UNARMED					-- Filled in when tool equipped based on player team color
local COLOR_ARMED					-- Filled in when tool equipped based on player team color
local COLOR_ARMED2 = BrickColor.White()


local angle = 45

-------------------------------------------------------------------------------------------------
-- Separate co-routine to mark as no longer reloading after a timeout
function reload(grenade, timeLeft)

	wait(RELOAD_TIME + timeLeft)

	if state == &quot;thrown&quot; then
		state = &quot;idle&quot;
		Handle.Transparency = 0
	end
end


--------------------------------------------------------------------------------------------------
local function computeLaunchSpeed(source, target, angle, gravity)

	local v = target - source
	local mxz = Vector3.new(v.x, 0, v.z).magnitude

	local r = (gravity * mxz * mxz) / ( 2 * math.cos(angle) * math.cos(angle) * ( (mxz * math.tan(angle)) - v.y ) )

	if r &lt; 0 then
		return nil
	end

	return math.sqrt(r)
end


------------------------------------------------------------------------------------------------

local function throw( sourcePos, targetPos, timeLeft )

	--print(&quot;throw src: &quot;, sourcePos, &quot; to &quot;, targetPos)

	local speed = computeLaunchSpeed(sourcePos, targetPos, math.rad(angle), EFFECTIVE_g)

	-- If we are not angled steeply enough to hit target then arbitrarity pick a speed
	if not speed then
		print(&quot;Cannot hit target with specified elevation&quot;)	
		speed = MAX_SPEED / 2
	end

	-- Adjustment empirically determined.   Do not know why this is needed.
	speed = speed *.992	

	if speed &gt; MAX_SPEED then
		speed = MAX_SPEED
	end

	-- v3 is the 3d world space launch vector
	local v = targetPos - sourcePos
	local v2 = Vector3.new(v.x, 0, v.z).unit * math.cos(math.rad(angle))
	local v3 = Vector3.new(v2.x, math.sin(math.rad(angle)), v2.z)

	local grenade = Instance.new(&quot;Part&quot;)
	grenade.Name = &quot;HandGrenade&quot;
	grenade.Size = Vector3.new(1,1,1)
	grenade.formFactor = 0
	grenade.Shape = 1
	grenade.CanCollide = true
	grenade.Anchored = true
	grenade.BrickColor = COLOR_ARMED
	grenade.Transparency = 1
	grenade.CFrame = CFrame.new(Vector3.new(0,0,0), Vector3.new(0,0,0) + v3)


	local m = Handle.Mesh:Clone()
	m.Parent = grenade

	grenade.FrontParamA = 8004			-- Hand grenade parameter set
	grenade.FrontParamB = speed		-- Launch speed
	grenade.RightParamA = timeLeft		-- Fuse time
	grenade.RightParamB = EFFECTIVE_g	-- Effective gravity

	-- Record the player throwing grenade to check for TKing and score KO points
	local ov = Instance.new(&quot;ObjectValue&quot;)
	ov.Name = &quot;Creator&quot;
	ov.Value = Player
	ov.Parent = grenade

	local GrenadeScript = GrenadeScript:Clone()
	GrenadeScript.Disabled = false
	GrenadeScript.Parent = grenade

	
	grenade.Parent = game.Workspace

	grenade.Position = sourcePos

	-- If grenade does not place where we attempted to put it 
	-- this means there is something there.  This probably
	-- means Player is attempting to throw &quot;thru&quot; a wall or 
	-- some obstacle.  Remove grenade in this case but still
	-- do reload wait.

	if grenade.Position.y &gt; sourcePos.y + .5 then
		grenade:Remove()
	end

	state = &quot;thrown&quot;

	local co = coroutine.create(reload)
	assert(coroutine.resume(co, grenade, timeLeft))

end


----------------------------------------------------------------------------------

function onButton1Up()

	if state == &quot;armed&quot; then

		local curTime = time()
		local timeLeft = FUSE_TIME - (curTime - armTime)

		if timeLeft &gt; FUSE_TIME then
			timeLeft = FUSE_TIME
		elseif timeLeft &lt; 0 then
			timeLeft = 0
		end

		throw(Handle.Position+Vector3.new(0,1.5,0), script.Parent.Parent.Humanoid.TargetPoint, timeLeft)

		Handle.Transparency = 1
		Handle.BrickColor = COLOR_UNARMED
	end
end




-------------------------------------------------------------------------------------------------
-- Separate co-routine to time the fuse burning phase
function fuseMonitor(seqNum)

	Handle.BrickColor = COLOR_ARMED2
	Handle.Transparency = 0

	--print(&quot;enter fuseMonitor&quot;)

	local INTERVAL_TIME = .5

	--print(&quot;fuseMonitor start&quot;)

	-- Wait for up to FUSE_TIME, flashing the Handle white and team color every .5 secs

	local t = FUSE_TIME

	while t &gt; 0 do

		local tw = t

		if t &gt; INTERVAL_TIME then
			tw = INTERVAL_TIME
		end


		wait(tw)

		t = t - tw



		-- If grenade was thrown then we are done
		if (seqNum ~= armedSeqNum) or (state ~= &quot;armed&quot;) then
			return
		end


		
		if Handle.BrickColor == COLOR_ARMED then
			Handle.BrickColor = COLOR_ARMED2
		else
			Handle.BrickColor = COLOR_ARMED
		end
	end

	--print(&quot;fused &quot;, Handle.BrickColor)

	Handle.BrickColor = COLOR_ARMED

	-- Fuse timed out, throw grenade with zero time left so it explodes near

	throw(Handle.Position, script.Parent.Parent.Humanoid.TargetPoint, 0)

	Handle.Transparency = 1
	Handle.BrickColor = COLOR_UNARMED

	--print(&quot;fuseMonitor end&quot;)
end

----------------------------------------------------------------------------------
function onButton1Down()

	-- If not still reloading from last throw

	if state == &quot;idle&quot; then

		state = &quot;armed&quot;
		armedSeqNum = armedSeqNum + 1

		local co = coroutine.create(fuseMonitor)
		assert(coroutine.resume(co, armedSeqNum))

		armTime = time()
	end

end


----------------------------------------------------------------------------------------------------
function onKeyDown(key)

	if key == &quot;b&quot; then
		_G.displayHint(&quot;Low arc selected&quot;)
		angle = 25

	elseif key == &quot;g&quot; then
		_G.displayHint(&quot;Optimum arc selected&quot;)
		angle = 45

	elseif key == &quot;t&quot; then
		_G.displayHint(&quot;High arc selected&quot;)
		angle = 70
	end


end


--------------------------------------------------------------------------------------------------
function onEquippedLocal(mouse)

	if mouse == nil then
		print(&quot;Mouse not found&quot;)
		return 
	end

	Torso = script.Parent.Parent.Torso

	if Torso.Position.y &gt; 300 then
		return
	end

	-- Find the Grenade script in the GlobalScripts model with root name starting with &quot;ScriptGrenade&quot;

	for i,v in ipairs(script.Parent:GetChildren()) do
		if v.className == &quot;Script&quot; and 
		   string.match(v.Name,&quot;^ScriptGrenade&quot;) then
			GrenadeScript = v
			break
		end
	end

	if not GrenadeScript then
		print(&quot;LScriptGrenade cannot find ScriptGrenade root name in Tool model&quot;)
		return
	end

	COLOR_ARMED = Player.TeamColor
	COLOR_UNARMED = COLOR_ARMED

	Handle = script.Parent.Handle

	Handle.BrickColor = COLOR_UNARMED

	Mouse = mouse

	mouse.Button1Down:connect(onButton1Down)
	mouse.Button1Up:connect(onButton1Up)
	mouse.KeyDown:connect(onKeyDown)

	Mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;

	_G.displayHint(&quot;Left mouse down to arm (3 sec fuse), up to throw, b=low arc, g=optimum arc (default), t=high arc&quot;,0)

	--mouse.Icon = &quot;rbxasset://textures\\ArrowCursor.png&quot;
end

----------------------------------------------------------------------------------------
function onUnequippedLocal()

	_G.clearHint()

end


-----------------------------------------------------------------------------------------
Tool.Equipped:connect(onEquippedLocal)
Tool.Unequipped:connect(onUnequippedLocal)

local count = 0

while true do

	wait(10)


end
