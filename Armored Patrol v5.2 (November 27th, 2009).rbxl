--2009/1/11 Initial version -fixed moving problem

-- Rotate Ramp about the X axis 

local Ramp = script.Parent

local angle = -math.atan(Ramp.Size.y/Ramp.Size.z)
local rcf = CFrame.fromAxisAngle(Vector3.new(1,0,0),-angle)

local z = Ramp.Size.z / 2
local y = Ramp.Size.y / 2

local zp = z * math.cos(angle) - y * math.sin(angle)
local yp = y * math.cos(angle) + z * math.sin(angle)

local transVec = Vector3.new(0, yp-y, zp-z)

--wait(1)

--print(&quot;transVec &quot;, transVec)

Ramp.CFrame = Ramp.CFrame * rcf + Ramp.CFrame:vectorToWorldSpace(transVec)
