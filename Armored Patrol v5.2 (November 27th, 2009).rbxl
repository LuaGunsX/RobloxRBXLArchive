-- Parameters:
--	cfDir - zero origined CFrame give direction ridge would face
--	aps - the 4 corners of the top in world space
--	distToEdge - positive distance from origin to edge we are checking against

local function buildRidge(ho, cfDir, wps, distToEdge, height)

	height = height + .05

	local cp
	local cpDir
	local p2
	local p3

	-- Find the 3 points closest to the edge
	for i = 1,4 do

		local rp = cfDir:pointToObjectSpace(wps[i].p) 

		if (cp == nil) or (cp.z &gt; rp.z) then
			p3 = p2
			p2 = cp
			cp = rp
			cpDir = wps[i].dir

		elseif (p2 == nil) or (p2.z &gt; rp.z) then
			p3 = p2
			p2 = rp

		elseif (p3 == nil) or (p3.z &gt; rp.z) then
			p3 = rp
		end
	end

	-- If center point not close enough to edge, no ridge
	if -cp.z &lt; distToEdge - T_GAP then
		return
	end

	-- Determine which of p2,p3 are left and right of center
	local rp, lp

	if p2.x &gt; p3.x then
		rp = p2
		lp = p3
	else
		rp = p3
		lp = p2
	end

	local rWidth = math.ceil((rp.x - lp.x)/2)
	local rCenter = (rp.x + lp.x)/2

	-- Mark the two sides that we are building the ridge from so as not
	-- to gen side inclines from them later.  Make the side angles steep
	-- so floor generation is controlled.  This is pretty nasty since we 
	-- already generated the side angle and other data along with it
	-- but it is quick and dirty for now.

	local d = string.sub(cpDir,1,1)
	ho[d].ridge = true
	ho[d].angle = math.rad(30)
	
	d = string.sub(cpDir,2,2)
	ho[d].ridge = true
	ho[d].angle = math.rad(30)


	-- Do left ridge

	local rLength = distToEdge + lp.z	-- remember lp.z is negative

	local rRidge = Instance.new(&quot;Part&quot;)
	rRidge.Name = &quot;RidgeTop&quot;
	rRidge.formFactor = 2
	rRidge.Size = Vector3.new(rWidth, THICKNESS_TOP, rLength)
	rRidge.FrontSurface = 2 -- Weld, just to mark it
	rRidge.Friction = 1
	rRidge.Elasticity = 0
	rRidge.TopSurface = 0
	rRidge.BrickColor = COLOR_TOP
	rRidge.Material = Enum.Material.Slate
	rRidge.Anchored = true

	local pos = cfDir:pointToWorldSpace(Vector3.new(rCenter - (rWidth/2), height , (-distToEdge + lp.z)/2))

	local rsAngle = math.rad(45)
	local rsLen = math.ceil(height/math.sin(rsAngle))

	rRidge.CFrame = cfDir + pos
	rRidge.Parent = ho.hill

	makeIncline(ho.hill, rRidge,cfw,rLength,0,rWidth/2,rsLen,rsAngle, &quot;LeftRidgeSide&quot;)

	makeRidgeFloor(ho.hill, rRidge, cfw, height, rsAngle)


	-- Do right ridge

	rLength = distToEdge + rp.z	-- remember rp.z is negative

	local lRidge = rRidge:Clone()
	lRidge.Material = Enum.Material.Slate
	lRidge.Anchored = true
	lRidge.Size = Vector3.new(rWidth, THICKNESS_TOP, rLength)

	pos = cfDir:pointToWorldSpace(Vector3.new(rCenter + (rWidth/2), height , (-distToEdge + rp.z)/2))

	lRidge.CFrame = cfDir + pos
	lRidge.Parent = ho.hill

	makeIncline(ho.hill, lRidge,cfe,rLength,0,rWidth/2,rsLen,rsAngle, &quot;RightRidgeSide&quot;)

	makeRidgeFloor(ho.hill, lRidge, cfe, height, rsAngle)

end


