							<string name="Name">ScriptControlM109-v1.1</string>
							<string name="Source">-------------------------------------------------------------------------------------------------------------------------------
-- 2009/11/16 Initial version based on ScriptControlTank-v2.2
--2009/11/24 Add 8th wheel
--2009/11/25 Max speed of 12

-- Features:

	-- For tank control.

	-- Receives commands from tool

	-- Initially wheels ParamB is set to true zero to keep moving part
	-- count down, after Player has touched controls ParamB is set to
	-- a very tiny value instead of true zero to simulate braking.

	-- Hints give current speed.

	-- Knows about SeatDriver.

	-- Continually steers toward last target.
	
	-- Auto increase wheel RPM beyond max if chassis velocity below current requested

	-- Auto turn-off when no CntrUser for threshold time.


local Vehicle=script.Parent
local Chassis = Vehicle.Chassis

local Torso

-- Control Values.  CntrDrive communicates accel, decel, brake commands.
-- CntrSteer communicates new steer target position.
-- CntrUser contains the Player object that has both welded to the seat
-- and selected the driving tool.

local CntrDrive = Vehicle.CntrDrive
local CntrSteer = Vehicle.CntrSteer
local CntrUser= Vehicle.CntrUser

local l1 = Vehicle.Wheels.LWheel1
local l2 = Vehicle.Wheels.LWheel2
local l3 = Vehicle.Wheels.LWheel3
local l4 = Vehicle.Wheels.LWheel4
local l5 = Vehicle.Wheels.LWheel5
local l6 = Vehicle.Wheels.LWheel6
local l7 = Vehicle.Wheels.LWheel7
local l8 = Vehicle.Wheels.LWheel8

local r1 = Vehicle.Wheels.RWheel1
local r2 = Vehicle.Wheels.RWheel2
local r3 = Vehicle.Wheels.RWheel3
local r4 = Vehicle.Wheels.RWheel4
local r5 = Vehicle.Wheels.RWheel5
local r6 = Vehicle.Wheels.RWheel6
local r7 = Vehicle.Wheels.RWheel7
local r8 = Vehicle.Wheels.RWheel8

local seat = Vehicle.SeatDriver

-- The ratio between the actual RPS of a motor surfaceType and the
-- value specified for the xxxParamB parameter.  The value below
-- is the value to multiply by the xxxParamB parameter to find out
-- the rotations per second it will make the wheel go.
local ParamB_TO_RPS_RATIO = 4.6 

-- The PB_RATIO is the ParamB change needed for each studs/sec speed change.
-- (Use LWheel2 for wheel size because wheel 1 is not representative of the ground wheels.)
local PB_RATIO =  1 /(l2.Size.x * 3.14159 * ParamB_TO_RPS_RATIO)



-- Each accel increases speed by SPEED_INCR studs/sec.
local SPEED_INCR = 4
local SPEED_MAX = 12

local SPEED_BRAKE = .000001			-- To simulate braking set wheels to very slow speed	
local SPEED_IDLE = 0					-- When vehicle abandoned set wheels to true stop 
									--   to reduce load on physics engine
local speedReq = 0
local angle = 0

local vehWid = 14
local vehLen = 33



local lastSteerTarget

-----------------------------------------------------------------------------------------------------------
-- Set the speed of the left and right wheels to the specified radians/sec values.

local function setSpeed(adjLeft, adjRight)

	l1.BottomParamB = adjLeft
	l2.BottomParamB = adjLeft
	l3.BottomParamB = adjLeft
	l4.BottomParamB = adjLeft
	l5.BottomParamB = adjLeft
	l6.BottomParamB = adjLeft
	l7.BottomParamB = adjLeft
	l8.BottomParamB = adjLeft

	r1.BottomParamB = adjRight
	r2.BottomParamB = adjRight
	r3.BottomParamB = adjRight
	r4.BottomParamB = adjRight
	r5.BottomParamB = adjRight
	r6.BottomParamB = adjRight
	r7.BottomParamB = adjRight
	r8.BottomParamB = adjRight
end

--------------------------------------------------------------------------------------------------------
-- Adjust the speed of the vehicle to the specified speed in studs/sec.  This is
-- translated into wheel angular rotation in radians/sec and further
-- adjusted to the current turn angle (i.e. inner wheels turn slower than outer).

-- ALgorithm insures that when a speed of zero is specified all wheels will be set
-- to exactly zero.

function adjSpeed( speed ) 

	local rps = speed * PB_RATIO

	--print(&quot;RPS RATIO &quot;, PB_RATIO, &quot; speed &quot;, speed, &quot; rps &quot;, rps)

	local s = (math.sin(math.abs( angle )) * (vehWid /2)  ) * 5 

	local r = (vehLen - s) / (vehLen + s)

	local adjLeft = 0
	local adjRight = 0

	--print(&quot;speed &quot;, s, &quot; r &quot;, r)

	-- Turning right, left rear goes full speed, right rear reduced 
	if angle &gt; 0 then
		adjLeft = rps
		adjRight =  -(rps * r) 

	-- Else turning left, right rear goes full speed, left rear reduced    
	else    
		adjLeft = rps * r
		adjRight =  -rps

	end

	setSpeed(adjLeft, adjRight)

end



------------------------------------------------------------------------------------------------------------

function handleDriveCmd()

	local key = string.lower(CntrDrive.Value)

	-- Accelerate
	if (key == &quot;y&quot;)  then

		if speedReq &lt; SPEED_MAX then
			speedReq = SPEED_MAX
			adjSpeed(speedReq)
		end

	elseif (key == &quot;h&quot;)  then

		if speedReq &lt; SPEED_MAX then
			speedReq = speedReq + SPEED_INCR
			adjSpeed(speedReq)
		end

	-- Decelerate
	elseif (key==&quot;j&quot;) then

		if speedReq &gt; -SPEED_MAX then
			speedReq = speedReq - SPEED_INCR
			adjSpeed(speedReq)
		end

	-- Stop all acceleration and deceleration
	elseif (key==&quot;u&quot;) or (key == &quot;x&quot;) then
		speedReq = SPEED_BRAKE
		adjSpeed(speedReq)

	end
end

-------------------------------------------------------------------------------------------------------------
function steerToward(targetPos)

         --Translate target to a unit vector from our position
	local target = (targetPos - seat.Position).unit

	-- get seat look vector
	local face = seat.CFrame.lookVector

	-- calc angle from seat to target on the xz plane
	angle = math.atan2(target.z,target.x) - math.atan2(face.z,face.x)

	if angle &lt; -3.14159 then
		angle = 6.28318 + angle
	elseif angle &gt; 3.14159 then
		angle = angle - 6.28318
	end


	-- Target angle now is with respect to the facing direction of the vehicle.
	-- Positive angles go to the right of vehicle direction, negative angles
	-- go to the left.  The angle is normallized to within pi to -pi (i.e +180 degrees to -180 degrees).
	-- We adjust angle to the angle we want to steer the wheels as follows:
	--
	--	From 0 to 2.34 steer to the right that angle up to max of .8.
	--	From 2.34 to pi, the target is behind the vehicle but still to the right of vehicle.  
	--		Assume we are driving backward and still steer to the right but make wheels
	--		straighter as we approach pi.

	--	From 0 to -2.34: steer to the left that angle up to min of  -.8.
	--	From -2.34 to -pi: the target is behind the vehicle but still to the left of vehicle.
	--		Assume we are driving backwards and still steer to the left but make wheels
	--		straighter as we approach -pi.

	if (angle &gt; .8) then
		if (angle &lt; 2.34) then
			angle = .8
		else
			angle = 3.14159 - angle
		end
	elseif (angle &lt; -.8) then
		if (angle &gt; -2.34) then
			angle = -.8
		else
			angle = -3.14159 - angle
		end
	end
end

------------------------------------------------------------------------------------------------------------
local function handleSteerCmd()

	--print(&quot;handleSteerCmd&quot;, CntrSteer.Value)

	steerToward(CntrSteer.Value)

         --Translate target to a unit vector from our position
	local targetVec = (CntrSteer.Value - seat.Position).unit

	-- Create a fake target very far away in the direction of received target.
	-- Until we get another steering command the main loop will continually
	-- steer toward this fake target. 
 
	lastSteerTarget = seat.Position + (targetVec * 100000)

end

--------------------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------------------

local BodyGyro = Instance.new(&quot;BodyGyro&quot;)
BodyGyro.D = 9000
BodyGyro.P = 9000
BodyGyro.maxTorque = Vector3.new(40,0,40)

local state = &quot;Normal&quot;

local prevPos = Chassis.Position
local upCF = CFrame.fromAxisAngle(Vector3.new(1,0,0), math.rad(90))
local toc = 0	-- Tipped over counter


-------------------------------------------------------------------------
-- Get Current angle the Vehicle makes from the vertical

local function getAngle()

	local lv = (Chassis.CFrame * upCF).lookVector

	local ang

	if lv.y &gt; 1 or lv.y &lt; -1 then
		ang = 180

	else
		ang = math.deg(math.acos(lv.y))
	end

	return ang
end

----------------------------------------------------------------------
local function maintainOrientation()

	if state == &quot;Normal&quot; then

		-- If we are not moving check to see if we are not right side up

		if (prevPos - Chassis.Position).magnitude &lt; 1 then
	
			if getAngle() &gt; 85 then

				toc = 0
				state = &quot;PossibleFlipped&quot;
			end
		end

	elseif state == &quot;PossibleFlipped&quot; then

		if ((prevPos - Chassis.Position).magnitude &gt; 1) or
			getAngle() &lt;= 40 then

			state = &quot;Normal&quot;

		else
			toc = toc + 1
		
			if toc &gt; 8 then
				print(&quot;Tipped over &quot;, ang)

				-- Set the BodyGyro to be aligned to the current xz axis of Chassis with
				-- the y axis rotated about 20 degrees toward the z axis.
				local lv = Chassis.CFrame.lookVector
				lv = Vector3.new(lv.x,0,lv.z)

				BodyGyro.cframe = CFrame.new(Chassis.Position, Chassis.Position+lv) * 
					CFrame.fromAxisAngle(Vector3.new(0,0,1), math.rad(20))
				
				BodyGyro.Parent = Chassis
				state = &quot;Righting&quot;
			end
		end

	elseif state == &quot;Righting&quot; then

		if ((prevPos - Chassis.Position).magnitude &gt; 10) or
			getAngle() &lt;= 40 then

			BodyGyro.Parent = nil
			state = &quot;Normal&quot;	
		end
	end

	prevPos = Chassis.Position

end
---------------------------------------------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------

CntrUser.Value = nil

CntrDrive.Changed:connect(handleDriveCmd)
CntrSteer.Changed:connect(handleSteerCmd)

local Hint = Instance.new(&quot;Hint&quot;)
local prevDriver

local CONTROL_INTERVAL = .33
local IDLE_TIME_THRESHOLD = 30

local enabledHints = false
local idleTime = 0

setSpeed(0,0)

local enabledHints = false

while (true) do

	wait(CONTROL_INTERVAL)

	if CntrUser.Value then

		local speed = Chassis.Velocity.magnitude

		-- Do not start displaying hints until vehicle has reached 5 studs/sec so
		-- we do not overwrite other hint info the tool may display.

		if speed &gt; 5  or enabledHints then
			--Hint.Text = string.format(&quot;Req Speed: %.1f, Cur Speed %.1f&quot;,speedReq, speed) 
			--Hint.Parent = CntrUser.Value

			prevDriver = CntrUser.Value
			enabledHints = true
		end

		idleTime = 0

	else
		Hint.Parent = nil
		enabledHints = false

		-- If we do not have a user for some time, then set wheel speed to true
		-- zero so the physics engine can mark this vehicle as not moving.
		-- Also this will auto-turnoff any runaway vehicles.
		idleTime = idleTime + CONTROL_INTERVAL

		if idleTime &gt;= IDLE_TIME_THRESHOLD then
			speedReq = SPEED_IDLE
			adjSpeed(speedReq)

			idleTime = 0
		end
	end



	-- Continually steer toward the distant fake target created from the last actual
	-- steer target that we received.  This way the player can pick a direction and
	-- the vehicle will continue in that direction.

	if lastSteerTarget then
		steerToward(lastSteerTarget)
	end

	-- Auto accelerate beyond max if we have requested max speed but are actually moving
	-- a lot less than this.  This is to help get up hills.  We set s to the velocity in the z direction
	-- of the Chassis object space (the facing direction).  Negative z values means we are moving
	-- forward.

	local s = -(Chassis.CFrame:vectorToObjectSpace(Chassis.Velocity).z)

	if (speedReq == SPEED_MAX) then
		if (s &lt; SPEED_MAX*.5) then
			adjSpeed(SPEED_MAX*2)
		else
			adjSpeed(speedReq)
		end

	elseif (speedReq == -SPEED_MAX) then
		if (s &gt; -SPEED_MAX*.5) then
			adjSpeed(-SPEED_MAX*2)
		else
			adjSpeed(speedReq)
		end

	-- If no auto acceleration then refresh speed to support steering changes
	else
		adjSpeed(speedReq)
	end


	-- Monitor to see if vehicle tipped over, and, if so, right it.
	maintainOrientation()

end

