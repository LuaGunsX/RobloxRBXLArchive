-- If both corners of a joint are not marked cornerOpening
-- then consider this joint eligible for a joint opening.  Select
-- 2 eligible joints (if there are than many) and mark the corresponding corners.

local function jointOpening(ho)

	local list = {}

	if (not ho.nne.cornerOpening) and (not ho.ene.cornerOpening) then
		table.insert(list, &quot;ne&quot;)
	end

	if (not ho.ese.cornerOpening) and (not ho.sse.cornerOpening) then
		table.insert(list, &quot;se&quot;)
	end

	if (not ho.ssw.cornerOpening) and (not ho.wsw.cornerOpening) then
		table.insert(list, &quot;sw&quot;)
	end

	if (not ho.wnw.cornerOpening) and (not ho.nnw.cornerOpening) then
		table.insert(list, &quot;nw&quot;)
	end

	-- Pick up to 2 (depending on how many are in the list) randomly and mark the 
	-- corresponding corner angles as joint opening.



	if #list &gt; 0 then

		local i = math.random(1,#list)


		local jo1 = list[i]
		table.remove(list,i)

		local c1 = string.sub(jo1,1,1) .. jo1
		local c2 = string.sub(jo1,2,2) .. jo1

		ho[c1].jointOpening = true
		ho[c2].jointOpening = true
		ho[c2].jointOpening2 = true

		if #list &gt; 0 then
			i = math.random(1,#list)


			local jo1 = list[i]

			local c1 = string.sub(jo1,1,1) .. jo1
			local c2 = string.sub(jo1,2,2) .. jo1

			ho[c1].jointOpening = true
			ho[c2].jointOpening = true
			ho[c2].jointOpening2 = true

		end
	end
end


--------------------------------------------------------------------------------
-- For the side incline off of a main incline whose angle is mainAng, 
-- calculate the shallowest angle that the side incline can be and still be
-- inside of the adj main incline whose angle is adjAng.  Plus 15 degrees
-- to make interrior more open.

-- The mainAng and adjAng are in radians, result in  radians.

local function calcSideAngle( mainAng, adjAng)

	if mainAng &gt;= 90 then
		return 90
	end

	return math.atan(math.tan(adjAng)/math.cos(mainAng)) + math.rad(15)

end


----------------------------------------------------------------------------------
-- Map the corner angle to the corner incline length stop height needed
-- for a joint opening.  All empirical numbers.

local cangToStop = { 
	{a= 30.5, h=28},
	{a=48.7 , h=29},
	{a= 55.5, h=30},
	{a= 62.6, h=31},
	{a= 69.7, h=32},
	{a= 76.7, h= 33},
}

local function mapAngToStop( ang, height )

	ang = math.deg(ang)
	local sh = nil

	for i = 1,#cangToStop do

		if cangToStop[i].a &gt; ang then
			sh =  cangToStop[i].h
			break
		end
	end

	if not sh then
		sh = cangToStop[#cangToStop].h
	end

	if height &gt; 40 then
		sh = sh + (height-40) * .2 
	end

	return sh
end



-------------------------------------------------------------------------------------
local function calcOneCornerAttr(ca, sideAngle, adjSideAngle, height)

	ca.angle = calcSideAngle(sideAngle, adjSideAngle)

	local ls = 0

	if height &gt; MIN_HEIGHT_FOR_INTERIOR then
		local ls = 10
	end

	local ws = 0

	if ca.cornerOpening then
		ws = 15

	elseif ca.jointOpening then
		if ca.jointOpening2 then
			ls = mapAngToStop( ca.angle, height )
		else
			ca.angle = ca.angle + math.rad(15)
			ls = 10
		end
	end

	ca.ws = ws
	ca.ls = ls
end

-----------------------------------------------------------------------------------

local function calcCornerAttr(ho)

	local height = ho.height

	calcOneCornerAttr(ho.nnw, ho.n.angle, ho.w.angle, height)
	calcOneCornerAttr(ho.nne, ho.n.angle, ho.e.angle, height)
	calcOneCornerAttr(ho.ene, ho.e.angle, ho.n.angle, height)
	calcOneCornerAttr(ho.ese, ho.e.angle, ho.s.angle, height)
	calcOneCornerAttr(ho.sse, ho.s.angle, ho.e.angle, height)
	calcOneCornerAttr(ho.ssw, ho.s.angle, ho.w.angle, height)
	calcOneCornerAttr(ho.wsw, ho.w.angle, ho.s.angle, height)
	calcOneCornerAttr(ho.wnw, ho.w.angle, ho.n.angle, height)
end

------------------------------------------------------------------------------------
local function placeOneCache( top, x, y, z)

	local pos = top.CFrame:pointToWorldSpace(Vector3.new(x,0,z))
	pos = Vector3.new(pos.x, y, pos.z)

	local cache = CacheProto:Clone()
	cache.Name=&quot;Cache&quot;
	cache.Anchored = false

	-- If there is a script then enable it
	for k,v in pairs(cache:GetChildren()) do
		if v.className == &quot;Script&quot; then
			v.Disabled = false
			break
		end
	end

	
	cache.Parent = Hills --top.Parent
	cache.Position = pos

	if cache.Position.y &gt; pos.y + 3 then
		cache.Parent = nil
	end
end


-----------------------------------------------------------------------------------
-- Attempt to place a cache at each corner opening because this is 
-- a spot that would probably be hidden somewhat.  After placing the
-- cache by setting its Position attribute immediately check its 
-- position.  If is it higher than where Position was set to then this means
-- it overlapped a hill incline.  Only keep those caches that do not overlap
-- anything.

local function placeCache(ho)

	local top = ho.top
	local topxh = ho.top.Size.x/2
	local topzh = ho.top.Size.z/2
	local height = ho.height
	local ws = 15
	local indent = 8
	local indentw = 7
	local y = 6

	if ho.nnw.cornerOpening then
		placeOneCache(top, -topxh+indent, y, -topzh - ((height - ws) / math.tan(ho.n.angle)) + indentw)
	end

	if ho.nne.cornerOpening then
		placeOneCache(top, topxh-indent, y, -topzh - ((height - ws) / math.tan(ho.n.angle)) + indentw)
	end	

	if ho.ene.cornerOpening then
		placeOneCache(top, topxh + ((height - ws) / math.tan(ho.e.angle)) - indentw, y, -topzh+indent)
	end	

	if ho.ese.cornerOpening then
		placeOneCache(top, topxh + ((height - ws) / math.tan(ho.e.angle)) - indentw, y, topzh-indent)
	end	

	if ho.sse.cornerOpening then
		placeOneCache(top, topxh-indent, y, topzh + ((height - ws) / math.tan(ho.s.angle)) - indentw)
	end

	if ho.ssw.cornerOpening then
		placeOneCache(top, -topxh+indent, y, topzh + ((height - ws) / math.tan(ho.s.angle)) - indentw)
	end	

	if ho.wnw.cornerOpening then
		placeOneCache(top, -topxh - ((height - ws) / math.tan(ho.w.angle)) + indentw, y, -topzh+indent)
	end	

	if ho.wsw.cornerOpening then
		placeOneCache(top, -topxh - ((height - ws) / math.tan(ho.w.angle)) + indentw, y, topzh-indent)
	end	
	



end

-----------------------------------------------------------------------------------
-- Create the capture pad and place on top of hill Top.  Clone the capture script,
-- enable, and place in the hill model.

local function activateCtf( ho, sizeValue )



	local value = Instance.new(&quot;NumberValue&quot;)
	value.Value = sizeValue
	value.Parent = ho.Hill

	local s = ScriptCTH:Clone()
	s.Disabled = false
	s.Parent = ho.hill

end

-----------------------------------------------------------------------------------

local function createCaptureTheFlag( hillObjects )

	-- Make a list of all ranges, also find highest hill in each range

	local ranges = {}

	for ho in pairs(hillObjects) do

		if ho.height &gt; MIN_HEIGHT_FOR_INTERIOR then

			local r = ho.range

			if r then
				if (not r.highest) or (ho.height &gt; r.highest.height) then
					r.highest = ho
				end
			else
				r = {highest=ho, [ho]=true}
			end
			
			if r.sizeValue == nil then
				r.sizeValue = 0
			end

			-- Add the size value of this hill to the running sum for the range.
			-- The size value is simply the sum of the height and dimensions of the top
			-- plus the sizes of the 4 extents.

			r.sizeValue = r.sizeValue + ho.height + ho.top.Size.x + ho.top.Size.z + 
				ho.n.overlapExtent + ho.e.overlapExtent + ho.w.overlapExtent + ho.s.overlapExtent  

			ranges[r] = true
		end
	end

	-- Go thru all ranges and create a model for the 3 biggest ones, and move all the hills for that
	-- range into that range model. Create and activate the ctf pad and script for the 
	-- range on the highest hill in the range.

	for i = 1,MAX_FLAGS do

		local br

		for r in pairs(ranges) do

			if (br == nil) or (r.sizeValue &gt; br.sizeValue) then

				br = r
			end
		end

		ranges[br] = nil	-- Remove from ranges so we do not do again

		local Range = Instance.new(&quot;Model&quot;)
		Range.Name = &quot;Range&quot;

		Range.Parent = Hills

		for ho in pairs(br) do

			if type(ho) ~= &quot;string&quot; then
				ho.hill.Parent = Range
			end
		end

		activateCtf(br.highest, br.sizeValue)
	end
end

-------------------------------------------------------------------------------------------------
-- Parameters:
--	cfDir - zero origined CFrame give direction ridge would face
--	aps - the 4 corners of the top in world space
--	distToEdge - positive distance from origin to edge we are checking against

local function buildRidge(ho, cfDir, wps, distToEdge, height)

	height = height + .05

	local cp
	local cpDir
	local p2
	local p3

	-- Find the 3 points closest to the edge
	for i = 1,4 do

		local rp = cfDir:pointToObjectSpace(wps[i].p) 

		if (cp == nil) or (cp.z &gt; rp.z) then
			p3 = p2
			p2 = cp
			cp = rp
			cpDir = wps[i].dir

		elseif (p2 == nil) or (p2.z &gt; rp.z) then
			p3 = p2
			p2 = rp

		elseif (p3 == nil) or (p3.z &gt; rp.z) then
			p3 = rp
		end
	end

	-- If center point not close enough to edge, no ridge
	if -cp.z &lt; distToEdge - T_GAP then
		return
	end

	-- Determine which of p2,p3 are left and right of center
	local rp, lp

	if p2.x &gt; p3.x then
		rp = p2
		lp = p3
	else
		rp = p3
		lp = p2
	end

	local rWidth = math.ceil((rp.x - lp.x)/2)
	local rCenter = (rp.x + lp.x)/2

	-- Mark the two sides that we are building the ridge from so as not
	-- to gen side inclines from them later.  Make the side angles steep
	-- so floor generation is controlled.  This is pretty nasty since we 
	-- already generated the side angle and other data along with it
	-- but it is quick and dirty for now.

	local d = string.sub(cpDir,1,1)
	ho[d].ridge = true
	ho[d].angle = math.rad(30)
	
	d = string.sub(cpDir,2,2)
	ho[d].ridge = true
	ho[d].angle = math.rad(30)


	-- Do left ridge

	local rLength = distToEdge + lp.z	-- remember lp.z is negative

	local rRidge = Instance.new(&quot;Part&quot;)
	rRidge.Name = &quot;RidgeTop&quot;
	rRidge.formFactor = 2
	rRidge.Size = Vector3.new(rWidth, THICKNESS_TOP, rLength)
	rRidge.FrontSurface = 2 -- Weld, just to mark it
	rRidge.Friction = 1
	rRidge.Elasticity = 0
	rRidge.TopSurface = 0
	rRidge.BrickColor = COLOR_TOP
	rRidge.Material = Enum.Material.Slate
	rRidge.Anchored = true

	local pos = cfDir:pointToWorldSpace(Vector3.new(rCenter - (rWidth/2), height , (-distToEdge + lp.z)/2))

	local rsAngle = math.rad(45)
	local rsLen = math.ceil(height/math.sin(rsAngle))

	rRidge.CFrame = cfDir + pos
	rRidge.Parent = ho.hill

	makeIncline(ho.hill, rRidge,cfw,rLength,0,rWidth/2,rsLen,rsAngle, &quot;LeftRidgeSide&quot;)


	-- Do right ridge

	rLength = distToEdge + rp.z	-- remember rp.z is negative

	local lRidge = rRidge:Clone()
	lRidge.Material = Enum.Material.Slate
	lRidge.Anchored = true
	lRidge.Size = Vector3.new(rWidth, THICKNESS_TOP, rLength)

	pos = cfDir:pointToWorldSpace(Vector3.new(rCenter + (rWidth/2), height , (-distToEdge + rp.z)/2))

	lRidge.CFrame = cfDir + pos
	lRidge.Parent = ho.hill

	makeIncline(ho.hill, lRidge,cfe,rLength,0,rWidth/2,rsLen,rsAngle, &quot;RightRidgeSide&quot;)

end


----------------------------------------------------------------------
local function placeRidges(ho)

	local top = ho.top

	--print(&quot;top.Position &quot;, top.Position)

	local wps = {
		{dir=&quot;se&quot;, p=top.CFrame:pointToWorldSpace(Vector3.new(top.Size.x/2,0,top.Size.z/2))},
		{dir=&quot;sw&quot;, p=top.CFrame:pointToWorldSpace(Vector3.new(-top.Size.x/2,0,top.Size.z/2))},
		{dir=&quot;ne&quot;, p=top.CFrame:pointToWorldSpace(Vector3.new(top.Size.x/2,0,-top.Size.z/2))},
		{dir=&quot;nw&quot;, p=top.CFrame:pointToWorldSpace(Vector3.new(-top.Size.x/2,0,-top.Size.z/2))}
	}

	buildRidge(ho, cfn, wps, HIGH_Z_EDGE, ho.height)
	buildRidge(ho, cfs, wps, HIGH_Z_EDGE, ho.height)
	buildRidge(ho, cfe, wps, HIGH_X_EDGE, ho.height)
	buildRidge(ho, cfw, wps, HIGH_X_EDGE, ho.height)


end

-----------------------------------------------------------------------------------

local function genAllHills()

	local hillObjects = {}

	for i=1,MAX_HILLS do
		hillObjects[genOneHillObject()] = true
	end

	-- Generate edge ridges.  We do this before generating
	-- side inclines because where ever we put a ridge we 
	-- mark that side so it will not generate a side angle plus we
	-- mark it to have a steep angle so floor generation is controlled.

	for ho in pairs(hillObjects) do
		placeRidges(ho)
	end

	-- Check for corner openings, also populate the ranges
	-- table which groups those hills that overlapping each other into 
	-- the same range.
	for ho in pairs(hillObjects) do
		if ho.height &gt; MIN_HEIGHT_FOR_INTERIOR then
			cornerOpening(hillObjects, ho)
		end
	end

	--[
	-- Check for joint openings
	for ho in pairs(hillObjects) do
		if ho.height &gt; MIN_HEIGHT_FOR_INTERIOR then
			jointOpening(ho)
		end
	end
	--]]

	-- Calculate sizing parameters for the corner-inclines
	for ho in pairs(hillObjects) do
		calcCornerAttr(ho)
	end

	-- Make hills
	for ho in pairs(hillObjects) do
		makeHill(ho)
	end

--[
	-- Place caches
	for ho in pairs(hillObjects) do
		if ho.height &gt; MIN_HEIGHT_FOR_INTERIOR then
			placeCache(ho)
		end
	end



	-- Create and activate a type of capture-the-flag mechanisms for the ranges
	-- collected above.

	createCaptureTheFlag( hillObjects )
--]]

	-- Empty hillObjects table to make sure we do not keep a ref to anything
	for ho in pairs(hillObjects) do
		ho.hill = nil
		ho.top = nil
		hillObjects[ho] = nil
	end
end


----------------------------------------------------------------------------------------------------------
local function genOnePos()

	while true do

		local x = math.random(MIN_X, MAX_X)
		local z = math.random(MIN_Z, MAX_Z)	

		local pos = Vector3.new(x,0,z)

		for k,zone in pairs(Zones:GetChildren()) do

			if (zone.TopParamB &gt; 0) and 
				(zone.Position - pos).magnitude &lt; zone.TopParamB then

				return pos
			end
		end
	end
end


-----------------------------------------------------------------------------------------------------------
local function genAllRocks(pos)

	for i = 1, MAX_ROCKS do

		local pos = genOnePos()

		local xs = math.random(MIN_DIM, MAX_DIM)
		local ys = math.random(MIN_DIM, MAX_DIM)
		local zs = math.random(MIN_DIM, MAX_DIM)

		local rock = Instance.new(&quot;Part&quot;)
		rock.Name = &quot;Rock&quot;
		rock.formFactor = 0
		rock.Size = Vector3.new(xs,ys,zs)
		rock.FrontSurface = 2
		rock.Friction = 1
		rock.Elasticity = 0
		rock.TopSurface = 0
		rock.BottomSurface = 0
		rock.Material = Enum.Material.Slate
		rock.BrickColor = COLOR_ROCK

		local cfx = CFrame.fromAxisAngle(Vector3.new(1,0,0), math.rad(math.random(0,180)))
		local cfy = CFrame.fromAxisAngle(Vector3.new(0,1,0), math.rad(math.random(0,180)))
		local cfz = CFrame.fromAxisAngle(Vector3.new(0,0,1), math.rad(math.random(0,180)))

		rock.CFrame = CFrame.new(pos) * cfx * cfy * cfz

		rock.Anchored = true
		rock.Parent = Hills
	end

end

------------------------------------------------------------------------------------------------------
local seed

function _G.TerrainBuild()

	if seed == nil then
		seed = tonumber(string.sub(tostring({}),8),16) %1000000
		math.randomseed(seed)
		print(&quot;randomseed: &quot;, seed)
	end

	--math.randomseed(101) -- look

	genAllHills()

	genAllRocks()
	
end

------------------------------------------------------------------------------------------------------

local function deleteRec( m )

	for k,v in ipairs(m:GetChildren()) do

		if v.className == &quot;Model&quot; then
			deleteRec( v )
			v:Remove()

		elseif v.className == &quot;Part&quot; then
			v:Remove()

		elseif v.className == &quot;Script&quot; then
			v:Remove()
		end
	end
end

function _G.TerrainRemove()

	deleteRec(Hills)

end











