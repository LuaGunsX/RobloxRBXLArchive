-- Return the distance from the center of top to the edge in the indicated direction

local function distToEdge( top, dir )

	if dir == &quot;n&quot; then
		return top.z/2

	elseif dir == &quot;e&quot; then
		return top.x/2

	elseif dir == &quot;s&quot; then
		return top.z/2

	elseif dir == &quot;w&quot; then
		return top.x/2
	end
end

----------------------------------------------------------------------------------------------------------------
-- The overlapExtent in the distance from center of top to the point in the current 
-- direction where the side incline would drop to the OVERLAP_HEIGHT.

local function genSideParms( dist, height )

	local a = math.rad(randNorm())
	local oe

	if height &gt;= MIN_HEIGHT_FOR_INTERIOR then
		oe = ((height - OVERLAP_HEIGHT) / math.tan(a)) + dist
	end

	return {angle = a, dist = dist, overlapExtent=oe}
end

