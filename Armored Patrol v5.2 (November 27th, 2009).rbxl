					<string name="Source">------------------------------------------------------------------------------------------------------------
--2009/8/20 Initial version

-- Manager Slabs created by the Build tool.  Slabs are detected when the Build tool adds the
-- SlabInProgress Part to our Model.  This script manages the meamorphosis of the 
-- SlabInProgress to a Slab, handles touch events, define the DamageHandler routine
-- for it, removes it when it is sufficiently damaged, and detects its removal by other
-- means (like the Build tool deleting it).

-- Since event handlers for all Slabs are kept in this script and these
-- Slabs are continually leaving the system, experimentation shows this
-- will cause a memory leak.  This appears to be because not everything is
-- cleaned up as long as this script is still running.  So the game control must
-- remove this script every so often.

local Fortifications = script.Parent

local BUILDING_TIME = 30

local TEAM_RED = BrickColor.new(&quot;Bright red&quot;)
local TEAM_BLUE = BrickColor.new(&quot;Cyan&quot;)

local VEH_RED = BrickColor.new(&quot;Sand red&quot;)
local VEH_BLUE = BrickColor.new(&quot;Sand blue&quot;)

-----------------------------------------------------------------------------------------------------------------------------------
-- This routine is registered in the _G.DamageHandler table and called when an explosion 
-- is near.  It assess damage to the slab (part) and blasts it when it strength is gone.
-- This will cause it to fall to negative y void and be removed automatically by the system.

local function takeDamage( part, distance, force, blastRadius )

	-- Scale damage from 0-force based on blastRadius

	if distance &gt; blastRadius then distance = blastRadius end

	local d = ((blastRadius - distance)/blastRadius) * force

	local orig = part.FrontParamB

	part.FrontParamB = part.FrontParamB - d

	local cur = part.FrontParamB
	local max = part.FrontParamA

	--print(&quot;Part &quot;, part.Name, &quot; damage: &quot;, d, &quot; assessed, parmA: &quot;, part.FrontParamA, &quot; parmB: &quot;, part.FrontParamB)

	-- If strength is gone, release the part

	if cur &lt; 0 then
		part.Anchored = false
		part:BreakJoints()
		part.CanCollide = false
		local v = Vector3.new(math.random(-30,30), math.random(30,90), math.random(-30,30))
		part.RotVelocity = Vector3.new(math.random() * 3, math.random() * 3, math.random()*3)
		part.Velocity = v

		if _G.DamageHandler then
			_G.DamageHandler[part] = nil
		end


	-- If down to 1/3 original strength then change to inlets

	elseif (orig &gt; max/3) and (cur &lt;= max/3) then

		part.BackSurface = 4
		part.FrontSurface = 4
		part.LeftSurface = 4
		part.RightSurface = 4
		part.TopSurface = 4
		part.BottomSurface = 4


	-- If down to 2/3 original strength then change to weld

	elseif (orig &gt; max*2/3) and (cur &lt;= max*2/3) then

		part.BackSurface = 1
		part.FrontSurface = 1
		part.LeftSurface = 1
		part.RightSurface = 1
		part.TopSurface = 1
		part.BottomSurface = 1

	end
end



--------------------------------------------------------------------------------------------------------------

local function onTouch( slab, part, teamColor )

	-- Defensive check, should not happen
	if slab.Name ~= &quot;SlabInProgress&quot; then
		return
	end

	--print(&quot;SlabInProgress touched by part &quot;, part.Name)

	local torso = part.Parent:FindFirstChild(&quot;Torso&quot;) or part.Parent.Parent:FindFirstChild(&quot;Torso&quot;)

	local ourTeam = true

	--print(&quot;torso &quot;, torso)

	if torso then
		if torso.BrickColor ~= teamColor then
			--print(&quot;torso.BrickColor &quot;, torso.BrickColor, &quot; TeamColor &quot;, TeamColor)
			ourTeam = false
		end
		
	elseif (part.BrickColor ~= teamColor) and (part.BackParamA ~= teamColor.Number) then
		--print(&quot;part.BrickColor &quot;, part.BrickColor, &quot; TeamColor &quot;, TeamColor)
		ourTeam = false
	end

	-- If the part is not from the our team then remove the touched Slab
	if not ourTeam then

		-- Remove this Slab

		if _G.DamageHandler then
			_G.DamageHandler[slab] = nil
		end

		slab:Remove()
	end

end

---------------------------------------------------------------------------------------------------------
-- When a SlabInProgress is added to the Fortifications Model this handler is called.
-- This co-routine then manages this slab for the rest of its life.

local function onSlabAdded( slab )

	-- Defensive check, should not happen
	if slab.Name ~= &quot;SlabInProgress&quot; then
		return
	end

	local teamColor = slab.BrickColor

	if teamColor == TEAM_RED then
		slab.BrickColor = VEH_RED

	elseif teamColor == TEAM_BLUE then
		slab.BrickColor = VEH_BLUE

	else
		slab.BrickColor = BrickColor.Gray()	--Defensive, should not happen
	end

	slab.Transparency = .5

	local pos = slab.Position

	local connection = slab.Touched:connect(
		function(part) onTouch(slab, part, teamColor) end)

	wait(BUILDING_TIME)

	connection:disconnect()

	if slab.Parent then

		slab.Transparency = 0
		slab.CanCollide = true

		slab.Name = &quot;Slab&quot;

		_G.DamageHandler[slab] = takeDamage

		while slab.Parent do

			if (slab.Position - pos).magnitude &gt; 1 then
				break
			end

			wait(10)

		end
	end

	if _G.DamageHandler then
		_G.DamageHandler[slab] = nil
	end

	slab:Remove()
end

----------------------------------------------------------------------------------------------------------

Fortifications.ChildAdded:connect(onSlabAdded)
