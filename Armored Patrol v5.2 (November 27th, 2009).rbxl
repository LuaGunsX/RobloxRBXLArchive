					<string name="Name">ScriptExtRpg-v3.0</string>
					<string name="Source">-------------------------------------------------------------------------------------------------
-- 2009/6/2  revert back to version that delives missiles on touch, max of 5 per player


local Cache = script.Parent

local touched = false

local MAX_MISSILES_PER_PLAYER = 5

local numMissilesInCache = 5

-----------------------------------------------------------------------------------------------------------------------------------
function onTouch(part)

	local torso = part.Parent:FindFirstChild(&quot;Torso&quot;) or part.Parent.Parent:FindFirstChild(&quot;Torso&quot;)

	if not torso then
		return
	end

	local character = torso.Parent

	if not character then
		return
	end

	local Player = game.Players:playerFromCharacter(character)

	if not Player then
		return
	end

	if touched then
		return
	end

	touched = true

	local ExtRpgCount = Player:FindFirstChild(&quot;ExtRpgCount&quot;)

	if not ExtRpgCount then
	
		ExtRpgCount = Instance.new(&quot;IntValue&quot;)
		ExtRpgCount.Name = &quot;ExtRpgCount&quot;
		ExtRpgCount.Value = 0
		ExtRpgCount.Parent = Player
	end

	if ExtRpgCount.Value &lt; MAX_MISSILES_PER_PLAYER then

		local add = numMissilesInCache

		if ExtRpgCount.Value + numMissilesInCache &gt; MAX_MISSILES_PER_PLAYER then
			add = MAX_MISSILES_PER_PLAYER - ExtRpgCount.Value
		end

		numMissilesInCache = numMissilesInCache - add

		ExtRpgCount.Value = ExtRpgCount.Value + add

		local Hint = Instance.new(&quot;Hint&quot;)
		Hint.Text = &quot;Extended Range RPG missiles: &quot;.. ExtRpgCount.Value
		Hint.Parent = Player

		if numMissilesInCache &lt;= 0 then
			Cache.Position = Vector3.new(2000,2000,2000)
			wait(5)
			Hint.Parent = nil
			Cache:Remove()
		else
			wait(5)
			Hint.Parent = nil
		end

	else

		local Hint = Instance.new(&quot;Hint&quot;)
		Hint.Text = &quot;Player already has &quot;.. ExtRpgCount.Value ..&quot; extended range missiles, currently not eligible for more&quot;
		Hint.Parent = Player

		wait(5)

		Hint.Parent = nil
	end

	touched = false
end




local connection = Cache.Touched:connect(onTouch)</string>
