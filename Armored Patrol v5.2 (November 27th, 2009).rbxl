					<string name="Source">----------------------------------------------------------------------------------------------------------
-- 2009/6/30 Initial version
--2009/8/23 Add CntrOwner NumberValue which contains BrickColor number of flag owner


local Hill = script.Parent
local Staff
local Flag
local FlagMesh
local Top = Hill.Top

local TEAM_RED = BrickColor.new(&quot;Bright red&quot;)
local TEAM_BLUE = BrickColor.new(&quot;Cyan&quot;)
local VEH_RED = BrickColor.new(&quot;Sand red&quot;)
local VEH_BLUE = BrickColor.new(&quot;Sand blue&quot;)

local FLAG_NEUTRAL = BrickColor.White()

-- owner values are: FLAG_NEUTRAL, TEAM_RED, TEAM_BLUE
local owner  = FLAG_NEUTRAL

-- claimer values are: nil, TEAM_RED, TEAM_BLUE
local claimer 

local claimTimer = 0

-- Used for determining player is on top.  The +5 is fudge factor
-- is for when player is in a vehicle that could be on top but the player
-- may be extending out a little beyond top edge.

local SIZEXH = Top.Size.x/2 + 10
local SIZEZH = Top.Size.z/2 + 10

local HqRedFlag = game.Workspace.HqRed.RedFlag
local HqBlueFlag = game.Workspace.HqBlue.BlueFlag

-- Create the Staff

Staff = Instance.new(&quot;Part&quot;)
Staff.Name = &quot;Staff&quot;
Staff.formFactor = 0	--Symmetric
Staff.Size = Vector3.new(17,1,17)
Staff.BrickColor = BrickColor.new(&quot;Dark stone grey&quot;)
Staff.Reflectance = .5
Staff.CanCollide = false
Staff.Anchored = true
Staff.CFrame = Top.CFrame + Vector3.new(0, Top.Size.y/2+Staff.Size.y/2, 0)

local mesh = Instance.new(&quot;CylinderMesh&quot;)
mesh.Scale = Vector3.new(.06,26,.06)
mesh.Offset = Vector3.new(0,14,0)
mesh.Parent = Staff

Staff.Parent = Hill

-- Create the flag

local FLAG_OFFSET_LOW = 10
local FLAG_OFFSET_HIGH =  22 --23

Flag = Instance.new(&quot;Part&quot;)
Flag.Name = &quot;Flag&quot;
Flag.formFactor = 0	--Symmetric
Flag.Size = Vector3.new(1,1,1)
Flag.BrickColor = BrickColor.White()
Flag.CanCollide = false
Flag.Anchored = true
Flag.Transparency = 0

local woff = Staff.CFrame:vectorToWorldSpace( Vector3.new(Staff.Size.x/2 + Flag.Size.x/2, 0, 0))
Flag.CFrame = Staff.CFrame + woff

FlagMesh = Instance.new(&quot;BlockMesh&quot;)
--FlagMesh.Scale = Vector3.new(18,10,.5)
FlagMesh.Scale = Vector3.new(14,8,.5)
--FlagMesh.Offset = Vector3.new(0,FLAG_OFFSET_HIGH,0)
FlagMesh.Offset = Vector3.new(-2,FLAG_OFFSET_HIGH,0)
FlagMesh.Parent = Flag

Flag.Parent = Hill

local CntrOwner = Instance.new(&quot;NumberValue&quot;)
CntrOwner.Name = &quot;CntrOwner&quot;
CntrOwner.Value = 0	-- Indicate no owner
CntrOwner.Parent = Flag

-- Compute hill value to both red and blue team

local MAX_DIST = (HqRedFlag.Position - HqBlueFlag.Position).magnitude
local MAX_VALUE = 50
local MIN_VALUE = 10

local function computeHillValue(hillPos, enemyHqPos)
	
	local d = (hillPos - enemyHqPos).magnitude 

	local v = ((1 - d/MAX_DIST) * (MAX_VALUE - MIN_VALUE)) + MIN_VALUE

	return v
end

local valueBlue = computeHillValue(Staff.Position, HqRedFlag.Position)
local valueRed = computeHillValue(Staff.Position, HqBlueFlag.Position)

local function hillValue( team )

	if team == TEAM_RED then
		return valueRed
	elseif team == TEAM_BLUE then
		return valueBlue
	else
		return 0
	end
end


------------------------------------------------------------------------------------------------

local function onTouched( part )

	--print(&quot;touched by &quot;, part.Name, &quot; color num &quot;, part.BrickColor.Number, &quot; BackParamA &quot;, part.BackParamA)
	
	local team

	local torso = part.Parent:FindFirstChild(&quot;Torso&quot;) or part.Parent.Parent:FindFirstChild(&quot;Torso&quot;)
		
	if torso then
		team = torso.BrickColor

	else

		if _G.DamageHandler and _G.DamageHandler[part]  then
	
			if part.BackParamA == TEAM_RED.Number then
				team = TEAM_RED
	
			elseif part.BackParamA == TEAM_BLUE.Number then
				team = TEAM_BLUE
			end
		end
	end

	-- If not touched by a player or a vehicle then ignore
	if not team then
		return
	end

	-- If touching team already owns hill then insure any claiming by other team is stopped
	if team == owner then
		claimer = nil
		claimTime = 0
		Flag.BrickColor = team
		FlagMesh.Offset = Vector3.new(-2,FLAG_OFFSET_HIGH,0)
		return
	end

	-- If touching team is already in the act of claiming then ignore

	if team == claimer then
		return
	end

	-- Toucher does not already own hill and is not currently claiming, start the claiming process

	claimer = team
	claimTimer = 0

end


---------------------------------------------------------------------------------------------------------
-- Does the position lie over the top

local function overlap( top, pos)

	local rpos = top.CFrame:pointToObjectSpace(pos)

	if pos.y &lt; Staff.Position.y then
		return false
	end

	if (rpos.x &gt; -SIZEXH) and (rpos.x &lt; SIZEXH) and (rpos.z &gt; -SIZEZH) and (rpos.z &lt; SIZEZH) then
		return true
	end

	return false
end


-------------------------------------------------------------------------------------------------------
-- Check to see if any player of the designated team is on top of the CTH hill

local function checkForPlayer( teamColor )

	for k,p in pairs(game.Players:GetChildren()) do

		if p.TeamColor == teamColor then

			local ch = p.Character

			if ch then
				print(&quot;character&quot;)

				local t = ch:FindFirstChild(&quot;Torso&quot;)

				if t then
					if overlap(Top, t.Position) then
						return true
					end
				end
			end
		end
	end

	return false
end



-----------------------------------------------------------------------------------------------------------
	

Staff.Touched:connect(onTouched)
Flag.Touched:connect(onTouched)


local CLAIM_INTERVAL = 60
local INTERVAL = .5


while true do

	wait(INTERVAL)

	if claimer then

		if checkForPlayer(claimer) then

			claimTimer = claimTimer + INTERVAL

			if claimTimer &lt; CLAIM_INTERVAL then	
				
				if Flag.BrickColor == FLAG_NEUTRAL then
					Flag.BrickColor = claimer
				else
					Flag.BrickColor = FLAG_NEUTRAL
				end

				local offset = ((claimTimer/CLAIM_INTERVAL) * (FLAG_OFFSET_HIGH-FLAG_OFFSET_LOW)) + FLAG_OFFSET_LOW
				FlagMesh.Offset = Vector3.new(-2, offset, 0)

			-- Else claim timer has expired, give hill to new owners
			else

				if _G.pegCapture then

					-- If there was a previous owner then deduct the hill value from them
					if owner ~= TEAM_NEUTRAL then
						_G.pegCapture(owner, -hillValue(owner))
					end

					-- Add the hill value to the new owners
					_G.pegCapture(claimer, hillValue(claimer))
				end

				owner = claimer
				claimer = nil
				claimTime = 0
				Flag.BrickColor = owner
				FlagMesh.Offset = Vector3.new(-2,FLAG_OFFSET_HIGH,0)

				-- Indicate current owner in the CntrOwner value
				CntrOwner.Value = owner.Number
			end

		-- Players team is not still on hill, stop claiming process, revert flag to original owner color
		else
			claimer = nil
			claimTime = 0
			Flag.BrickColor = owner
			FlagMesh.Offset = Vector3.new(-2,FLAG_OFFSET_HIGH,0)
		end
	end
end
