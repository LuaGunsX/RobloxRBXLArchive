	local count = 0

	if _G.DamageHandler ~= nil then

		for k,v in pairs(_G.DamageHandler) do

			if _G.DamageHandler[k] then
				count = count + 1
			end

			_G.DamageHandler[k] = nil
		end
	end

	if _G.Projectiles ~= nil then

		for k,v in pairs(_G.Projectiles) do

