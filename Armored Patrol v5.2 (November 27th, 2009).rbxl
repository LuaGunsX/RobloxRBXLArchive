local function placeOneCache( top, x, y, z)

	local pos = top.CFrame:pointToWorldSpace(Vector3.new(x,0,z))
	pos = Vector3.new(pos.x, y, pos.z)

	local cache = CacheProto:Clone()
	cache.Name=&quot;Cache&quot;
	cache.Anchored = false

	-- If there is a script then enable it
	for k,v in pairs(cache:GetChildren()) do
		if v.className == &quot;Script&quot; then
			v.Disabled = false
			break
		end
	end

	
	cache.Parent = Hills --top.Parent
	cache.Position = pos

	if cache.Position.y &gt; pos.y + 3 then
		cache.Parent = nil
	end
end


-----------------------------------------------------------------------------------
-- Attempt to place a cache at each corner opening because this is 
-- a spot that would probably be hidden somewhat.  After placing the
-- cache by setting its Position attribute immediately check its 
-- position.  If is it higher than where Position was set to then this means
-- it overlapped a hill incline.  Only keep those caches that do not overlap
-- anything.

local function placeCache(ho)

	local top = ho.top
	local topxh = ho.top.Size.x/2
	local topzh = ho.top.Size.z/2
	local height = ho.height
	local ws = 15
	local indent = 8
	local indentw = 7
	local y = 6

	if ho.nnw.cornerOpening then
		placeOneCache(top, -topxh+indent, y, -topzh - ((height - ws) / math.tan(ho.n.angle)) + indentw)
	end

	if ho.nne.cornerOpening then
		placeOneCache(top, topxh-indent, y, -topzh - ((height - ws) / math.tan(ho.n.angle)) + indentw)
	end	

	if ho.ene.cornerOpening then
		placeOneCache(top, topxh + ((height - ws) / math.tan(ho.e.angle)) - indentw, y, -topzh+indent)
	end	

	if ho.ese.cornerOpening then
		placeOneCache(top, topxh + ((height - ws) / math.tan(ho.e.angle)) - indentw, y, topzh-indent)
	end	

	if ho.sse.cornerOpening then
		placeOneCache(top, topxh-indent, y, topzh + ((height - ws) / math.tan(ho.s.angle)) - indentw)
	end

	if ho.ssw.cornerOpening then
		placeOneCache(top, -topxh+indent, y, topzh + ((height - ws) / math.tan(ho.s.angle)) - indentw)
	end	

	if ho.wnw.cornerOpening then
		placeOneCache(top, -topxh - ((height - ws) / math.tan(ho.w.angle)) + indentw, y, -topzh+indent)
	end	

	if ho.wsw.cornerOpening then
		placeOneCache(top, -topxh - ((height - ws) / math.tan(ho.w.angle)) + indentw, y, topzh-indent)
	end	
	



end

-----------------------------------------------------------------------------------
-- Create the capture pad and place on top of hill Top.  Clone the capture script,
-- enable, and place in the hill model.

local function activateCtf( ho, sizeValue )



	local value = Instance.new(&quot;NumberValue&quot;)
	value.Value = sizeValue
	value.Parent = ho.Hill

	local s = ScriptCTH:Clone()
	s.Disabled = false
	s.Parent = ho.hill

end

-----------------------------------------------------------------------------------

local function createCaptureTheFlag( hillObjects )

	-- Make a list of all ranges, also find highest hill in each range

	local ranges = {}

	for ho in pairs(hillObjects) do

		if ho.height &gt; MIN_HEIGHT_FOR_INTERIOR then

			local r = ho.range

			if r then
				if (not r.highest) or (ho.height &gt; r.highest.height) then
					r.highest = ho
				end
			else
				r = {highest=ho, [ho]=true}
			end
			
			if r.sizeValue == nil then
				r.sizeValue = 0
			end

			-- Add the size value of this hill to the running sum for the range.
			-- The size value is simply the sum of the height and dimensions of the top
			-- plus the sizes of the 4 extents.

			r.sizeValue = r.sizeValue + ho.height + ho.top.Size.x + ho.top.Size.z + 
				ho.n.overlapExtent + ho.e.overlapExtent + ho.w.overlapExtent + ho.s.overlapExtent  

			ranges[r] = true
		end
	end

	-- Go thru all ranges and create a model for the 3 biggest ones, and move all the hills for that
	-- range into that range model. Create and activate the ctf pad and script for the 
	-- range on the highest hill in the range.

	for i = 1,MAX_FLAGS do

		local br

		for r in pairs(ranges) do

			if (br == nil) or (r.sizeValue &gt; br.sizeValue) then

				br = r
			end
		end

		ranges[br] = nil	-- Remove from ranges so we do not do again

		local Range = Instance.new(&quot;Model&quot;)
		Range.Name = &quot;Range&quot;

		Range.Parent = Hills

		for ho in pairs(br) do

			if type(ho) ~= &quot;string&quot; then
				ho.hill.Parent = Range
			end
		end

		activateCtf(br.highest, br.sizeValue)
	end
end

