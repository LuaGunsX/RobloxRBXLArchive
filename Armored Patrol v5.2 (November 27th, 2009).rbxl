local function buildRidge(top, lx, hx, lz, hz)

	--print(&quot;buildRidge &quot;, lx, &quot; &quot;, hx, &quot; &quot;, lz, &quot; &quot;, hz)

	local h = math.floor(top.Position.y + top.Size.y/2)

	local r = Instance.new(&quot;Part&quot;)
	r.Size = Vector3.new(hx-lx, h, hz-lz)
	r.Anchored = true
	r.Material = Enum.Material.Slate
	r.BrickColor = COLOR_SIDE
	r.CFrame = CFrame.new((lx+hx)/2, h/2, (lz+hz)/2)
	r.Parent = game.Workspace
end

----------------------------------------------------------------------
local function placeRidges(ho)

	local top = ho.top

	--print(&quot;top.Position &quot;, top.Position)

	local ps = {
		top.CFrame:pointToWorldSpace(Vector3.new(top.Size.x/2,0,top.Size.z/2)),
		top.CFrame:pointToWorldSpace(Vector3.new(-top.Size.x/2,0,top.Size.z/2)),
		top.CFrame:pointToWorldSpace(Vector3.new(top.Size.x/2,0,-top.Size.z/2)),
		top.CFrame:pointToWorldSpace(Vector3.new(-top.Size.x/2,0,-top.Size.z/2))
	}

	local lowx, highx, lowz, highz
	local lz, hz, lx, hx

	for i=1,4 do
		if (lowx == nil) or (ps[i].x &lt; lowx) then
			lowx = ps[i].x
			lz = ps[i].z
		end

		if (highx == nil) or (ps[i].x &gt; highx) then
			highx = ps[i].x
			hz = ps[i].z
		end

		if (lowz == nil) or (ps[i].z &lt; lowz) then
			lowz = ps[i].z
			lx = ps[i].x
		end

		if (highz == nil) or (ps[i].z &gt; highz) then
			highz = ps[i].z
			hx = ps[i].x
		end
	end

	if lowx &lt; LOW_X_THRESH then
		--print(&quot;aaa &quot;, ps[1],&quot;  &quot;, ps[2],&quot;  &quot;, ps[3],&quot;  &quot;, ps[4])
		if lowz &lt; LOW_Z_THRESH then
			buildRidge(top, lx, highx, LOW_Z_EDGE, hz)
			lowz = LOW_Z_EDGE
		end
		buildRidge(top, LOW_X_EDGE, lx, lowz, lz)

		if highz &gt; HIGH_Z_THRESH then
			buildRidge(top, hx, highx, hz, HIGH_Z_EDGE)
			highz = HIGH_Z_EDGE
		end
		buildRidge(top, LOW_X_EDGE, hx, lz, highz)

	elseif highx &gt; HIGH_X_THRESH then
		--print(&quot;bbb &quot;, ps[1],&quot;  &quot;, ps[2],&quot;  &quot;, ps[3],&quot;  &quot;, ps[4])
		if lowz &lt; LOW_Z_THRESH then
			buildRidge(top, lowx, lx, LOW_Z_EDGE, lz)
			lowz = LOW_Z_EDGE
		end
		buildRidge(top, lx, HIGH_X_EDGE, lowz, hz)

		if highz &gt; HIGH_Z_THRESH then
			buildRidge(top, lowx, hx, lz, HIGH_Z_EDGE)
			highz = HIGH_Z_EDGE
		end
		buildRidge(top, hx, HIGH_X_EDGE, hz, highz)

	elseif lowz &lt; LOW_Z_THRESH then
		--print(&quot;ccc &quot;, ps[1],&quot;  &quot;, ps[2],&quot;  &quot;, ps[3],&quot;  &quot;, ps[4])
		buildRidge(top, lowx, lx, LOW_Z_EDGE, lz)
		buildRidge(top, lx, highx, LOW_Z_EDGE, hz)

	elseif highz &gt; HIGH_Z_THRESH then
		--print(&quot;ddd &quot;, ps[1],&quot;  &quot;, ps[2],&quot;  &quot;, ps[3],&quot;  &quot;, ps[4])
		buildRidge(top, lowx, hx, lz, HIGH_Z_EDGE)
		buildRidge(top, hx, highx, hz, HIGH_Z_EDGE)
	end
end

