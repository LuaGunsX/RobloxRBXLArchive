local MIN_HEIGHT = 10
local MAX_SIDE = 130
local MIN_SIDE = 10
local MIN_INCLINE = 15
local MAX_INCLINE = 90

local THICKNESS_TOP = 14.4
local THICKNESS_SIDE = 7.2

local MAX_ROCKS =  15
local MAX_DIM = 50
local MIN_DIM = 14

local MAX_FLAGS = 3

local BASE_HEIGHT = .4	-- Height of whatever hills are built on

