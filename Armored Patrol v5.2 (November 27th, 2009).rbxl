			-- If this model has a Torso then we consider it a player character
			elseif v:FindFirstChild(&quot;Torso&quot;) then
				deleteVehicles(v)
			end
		end
	end
end


-----------------------------------------------------------------------------------------------------
local function resetPlayers()

	for k,player in pairs(game.Players:GetChildren()) do 

		local ExtRpgCount = player:FindFirstChild(&quot;ExtRpgCount&quot;)	

		if ExtRpgCount then
			ExtRpgCount.Parent = nil
		end

		if (not player.Neutral) and player.TeamColor and (player.TeamColor ~= teamGreen) then

			local character = player.Character

			if character then
				local humanoid = character.Humanoid

				if humanoid then
					humanoid.Health = 0
				end
			end
		end
	end
end


----------------------------------------------------------------------------------------------------
local function restartGame()

	local message = Instance.new(&quot;Message&quot;)
	message.Parent = game.Workspace

	local capRed = 0
	local capBlue = 0

	if _G.getCapture then
		capRed,capBlue = _G.getCapture()
	end

	if capRed &gt; capBlue then
		message.Text= &quot;Red wins!  Regenerating hills for new game...&quot;
	
	elseif capBlue &gt; capRed then
		message.Text= &quot;Blue wins!  Regenerating hills for new game...&quot;

	else
		message.Text= &quot;Regenerating hills for new game...&quot;
	end
		

	wait(5)

	message.Parent = nil

	assert(pcall(emptyDamageHandler))

	assert(pcall(deleteVehicles,game.Workspace))

	assert(pcall(removeCurrentGenStations))

	wait(5)
	
	assert(pcall(deleteHills, Hills ))

	assert(pcall(deleteFortifications, Fortifications ))

	wait(5)

	if _G.resetCapture then
		_G.resetCapture()
	end

	collectgarbage(&quot;collect&quot;)

	assert(pcall(genGenStations))

	assert(pcall(resetPlayers))

end
