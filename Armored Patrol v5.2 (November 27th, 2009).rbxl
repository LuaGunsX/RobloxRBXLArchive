local function genOneHillObject(  )

	local ho = {}

	local hill = Instance.new(&quot;Model&quot;)
	hill.Name = &quot;Hill&quot;
	hill.Parent = Hills
	
	ho.hill = hill

	local top = genOneTop(hill)
	ho.top = top

	-- Height of top, including base thickness, from xz plane
	local height = top.Position.y
	ho.height = height

	ho.n = genSideParms(top.Size.z/2, height)
	ho.e = genSideParms(top.Size.x/2, height)
	ho.s = genSideParms(top.Size.z/2, height)
	ho.w = genSideParms(top.Size.x/2, height)

	ho.nnw = {}
	ho.nne = {}
	ho.ene = {}
	ho.ese = {}
	ho.sse = {}
	ho.ssw = {}
	ho.wsw = {}
	ho.wnw = {}


	return ho
end

---------------------------------------------------------------------
-- Put ho1 and ho2 into same range group.  If neither already in a range 
-- then create a new range and put them both in it.  If one in a 
-- range and the other is not, then put the latter into the formers range.
-- If they both already in different ranges then merge the second range into the first.

local function popRange(ho1, ho2)

	local r1 = ho1.range
	local r2 = ho2.range

	if r1 then
		if r2 then
			-- If already in same range, nothing to do
			if r1 == r2 then
				return
			end
			
			-- Else both already in different ranges, merge
			-- the 2nd range into the first, remove the 2nd

			for h in pairs(r2) do
				h.range = r1
				r1[h] = true
			end
			
		-- ho1 in a range but ho2 not, put ho2 into ho1s range
		else	
			
			r1[ho2] = true
			ho2.range = r1
		end

	else
		-- If ho2 in a range but ho1 not, put ho1 into ho2s range
		if r2 then
			
			r2[ho1] = true
			ho1.range = r2

		-- Else neither in a range, create a new range, add both
		else

			r1 = {}

			r1[ho1] = true
			ho1.range = r1

			r1[ho2] = true
			ho2.range = r1
		end
	end
end

---------------------------------------------------------------------
local function cornerOpening(hillObjs, ho)

	local cf = ho.top.CFrame

	-- Get all 8 corners of ho in world object space

	local nnwa = cf:pointToWorldSpace(Vector3.new(-ho.top.Size.x/2, 0, -ho.n.overlapExtent))
	local nnea = cf:pointToWorldSpace(Vector3.new(ho.top.Size.x/2, 0, -ho.n.overlapExtent))

	local enea = cf:pointToWorldSpace(Vector3.new(ho.e.overlapExtent, 0, -ho.top.Size.z/2))
	local esea = cf:pointToWorldSpace(Vector3.new(ho.e.overlapExtent, 0, ho.top.Size.z/2))

	--print(&quot;overlap check for &quot;, ho.top.Position)

	local sswa = cf:pointToWorldSpace(Vector3.new(-ho.top.Size.x/2, 0, ho.s.overlapExtent))
	local ssea = cf:pointToWorldSpace(Vector3.new(ho.top.Size.x/2, 0, ho.s.overlapExtent))

	--print(&quot;esea &quot;, esea, &quot; ssea &quot;, ssea)

	local wswa = cf:pointToWorldSpace(Vector3.new(-ho.w.overlapExtent, 0, ho.top.Size.z/2))
	local wnwa = cf:pointToWorldSpace(Vector3.new(-ho.w.overlapExtent, 0, -ho.top.Size.z/2))

	for h in pairs(hillObjs) do

		if (h ~= ho) and (h.height &gt; MIN_HEIGHT_FOR_INTERIOR) then

			--print(&quot;---Check against &quot;, h.top.Position)

			-- Map the 8 corners of ho into h object space
			local nnw = h.top.CFrame:pointToObjectSpace(nnwa)
			local nne= h.top.CFrame:pointToObjectSpace(nnea)
			local ene= h.top.CFrame:pointToObjectSpace(enea)
			local ese= h.top.CFrame:pointToObjectSpace(esea)
			local ssw= h.top.CFrame:pointToObjectSpace(sswa)
			local sse= h.top.CFrame:pointToObjectSpace(ssea)
			local wsw= h.top.CFrame:pointToObjectSpace(wswa)
			local wnw= h.top.CFrame:pointToObjectSpace(wnwa)

			--print(&quot;---h object coord ese &quot;, ese, &quot; sse &quot;, sse)

			-- The 4 overlap extents for h.  We check against the total overlap
			-- extent rectangle of h (though a more accurate check would be to
			-- check against the 4 overlap extents flaps of the side-inclines plus the top).
			local zn = -h.n.overlapExtent
			local zp = h.s.overlapExtent
			local xn = -h.w.overlapExtent
			local xp = h.e.overlapExtent

			local overlap = false

			--print(&quot;--- h extent xn/xp  zn/zp &quot;, xn, &quot;/&quot;, xp, &quot; &quot;, zn, &quot;/&quot;, zp)

			-- Check if ho nnw corner overlaps h
			if (nnw.x &gt; xn) and (nnw.x &lt; xp) and (nnw.z &gt; zn) and (nnw.z &lt; zp) then

				ho.nnw.cornerOpening = true
				overlap = true
			end

			-- Check if ho nne corner overlaps h
			if (nne.x &gt; xn) and (nne.x &lt; xp) and (nne.z &gt; zn) and (nne.z &lt; zp) then
				ho.nne.cornerOpening = true
				overlap = true
			end

			-- Check if ho ene corner overlaps h
			if (ene.x &gt; xn) and (ene.x &lt; xp) and (ene.z &gt; zn) and (ene.z &lt; zp) then
				ho.ene.cornerOpening = true
				overlap = true
			end

			-- Check if ho ese corner overlaps h
			if (ese.x &gt; xn) and (ese.x &lt; xp) and (ese.z &gt; zn) and (ese.z &lt; zp) then
				ho.ese.cornerOpening = true
				overlap = true
			end

			-- Check if ho ssw corner overlaps h
			if (ssw.x &gt; xn) and (ssw.x &lt; xp) and (ssw.z &gt; zn) and (ssw.z &lt; zp) then
				ho.ssw.cornerOpening = true
				overlap = true
			end

			-- Check if ho sse corner overlaps h
			if (sse.x &gt; xn) and (sse.x &lt; xp) and (sse.z &gt; zn) and (sse.z &lt; zp) then
				ho.sse.cornerOpening = true
				overlap = true
			end

			-- Check if ho wsw corner overlaps h
			if (wsw.x &gt; xn) and (wsw.x &lt; xp) and (wsw.z &gt; zn) and (wsw.z &lt; zp) then
				ho.wsw.cornerOpening = true
				overlap = true
			end

			-- Check if ho wnw corner overlaps h
			if (wnw.x &gt; xn) and (wnw.x &lt; xp) and (wnw.z &gt; zn) and (wnw.z &lt; zp) then
				ho.wnw.cornerOpening = true
				overlap = true
			end

			if overlap then
				popRange(ho, h)
			end
		end
	end
end


---------------------------------------------------------------------
-- If both corners of a joint are not marked cornerOpening
-- then consider this joint eligible for a joint opening.  Select
-- 2 eligible joints (if there are than many) and mark the corresponding corners.

local function jointOpening(ho)

	local list = {}

	if (not ho.nne.cornerOpening) and (not ho.ene.cornerOpening) then
		table.insert(list, &quot;ne&quot;)
	end

	if (not ho.ese.cornerOpening) and (not ho.sse.cornerOpening) then
		table.insert(list, &quot;se&quot;)
	end

	if (not ho.ssw.cornerOpening) and (not ho.wsw.cornerOpening) then
		table.insert(list, &quot;sw&quot;)
	end

	if (not ho.wnw.cornerOpening) and (not ho.nnw.cornerOpening) then
		table.insert(list, &quot;nw&quot;)
	end

	-- Pick up to 2 (depending on how many are in the list) randomly and mark the 
	-- corresponding corner angles as joint opening.



	if #list &gt; 0 then

		local i = math.random(1,#list)


		local jo1 = list[i]
		table.remove(list,i)

		local c1 = string.sub(jo1,1,1) .. jo1
		local c2 = string.sub(jo1,2,2) .. jo1

		ho[c1].jointOpening = true
		ho[c2].jointOpening = true
		ho[c2].jointOpening2 = true

		if #list &gt; 0 then
			i = math.random(1,#list)


			local jo1 = list[i]

			local c1 = string.sub(jo1,1,1) .. jo1
			local c2 = string.sub(jo1,2,2) .. jo1

			ho[c1].jointOpening = true
			ho[c2].jointOpening = true
			ho[c2].jointOpening2 = true

		end
	end
end


--------------------------------------------------------------------------------
-- For the side incline off of a main incline whose angle is mainAng, 
-- calculate the shallowest angle that the side incline can be and still be
-- inside of the adj main incline whose angle is adjAng.  Plus 15 degrees
-- to make interrior more open.

-- The mainAng and adjAng are in radians, result in  radians.

local function calcSideAngle( mainAng, adjAng)

	if mainAng &gt;= 90 then
		return 90
	end

	return math.atan(math.tan(adjAng)/math.cos(mainAng)) + math.rad(15)

end


----------------------------------------------------------------------------------
-- Map the corner angle to the corner incline length stop height needed
-- for a joint opening.  All empirical numbers.

local cangToStop = { 
	{a= 30.5, h=28},
	{a=48.7 , h=29},
	{a= 55.5, h=30},
	{a= 62.6, h=31},
	{a= 69.7, h=32},
	{a= 76.7, h= 33},
}

local function mapAngToStop( ang, height )

	ang = math.deg(ang)
	local sh = nil

	for i = 1,#cangToStop do

		if cangToStop[i].a &gt; ang then
			sh =  cangToStop[i].h
			break
		end
	end

	if not sh then
		sh = cangToStop[#cangToStop].h
	end

	if height &gt; 40 then
		sh = sh + (height-40) * .2 
	end

	return sh
end



-------------------------------------------------------------------------------------
local function calcOneCornerAttr(ca, sideAngle, adjSideAngle, height)

	ca.angle = calcSideAngle(sideAngle, adjSideAngle)

	local ls = 0

	if height &gt; MIN_HEIGHT_FOR_INTERIOR then
		local ls = 10
	end

	local ws = 0

	if ca.cornerOpening then
		ws = 15

	elseif ca.jointOpening then
		if ca.jointOpening2 then
			ls = mapAngToStop( ca.angle, height )
		else
			ca.angle = ca.angle + math.rad(15)
			ls = 10
		end
	end

	ca.ws = ws
	ca.ls = ls
end

-----------------------------------------------------------------------------------

local function calcCornerAttr(ho)

	local height = ho.height

	calcOneCornerAttr(ho.nnw, ho.n.angle, ho.w.angle, height)
	calcOneCornerAttr(ho.nne, ho.n.angle, ho.e.angle, height)
	calcOneCornerAttr(ho.ene, ho.e.angle, ho.n.angle, height)
	calcOneCornerAttr(ho.ese, ho.e.angle, ho.s.angle, height)
	calcOneCornerAttr(ho.sse, ho.s.angle, ho.e.angle, height)
	calcOneCornerAttr(ho.ssw, ho.s.angle, ho.w.angle, height)
	calcOneCornerAttr(ho.wsw, ho.w.angle, ho.s.angle, height)
	calcOneCornerAttr(ho.wnw, ho.w.angle, ho.n.angle, height)
end

