-- If both corners of a joint are not marked cornerOpening
-- then consider this joint eligible for a joint opening.  Select
-- 2 eligible joints (if there are than many) and mark the corresponding corners.

local function jointOpening(ho)

	local list = {}

	if (not ho.nne.cornerOpening) and (not ho.ene.cornerOpening) then
		table.insert(list, &quot;ne&quot;)
	end

	if (not ho.ese.cornerOpening) and (not ho.sse.cornerOpening) then
		table.insert(list, &quot;se&quot;)
	end

	if (not ho.ssw.cornerOpening) and (not ho.wsw.cornerOpening) then
		table.insert(list, &quot;sw&quot;)
	end

	if (not ho.wnw.cornerOpening) and (not ho.nnw.cornerOpening) then
		table.insert(list, &quot;nw&quot;)
	end

	-- Pick up to 2 (depending on how many are in the list) randomly and mark the 
	-- corresponding corner angles as joint opening.



	if #list &gt; 0 then

		local i = math.random(1,#list)


		local jo1 = list[i]
		table.remove(list,i)

		local c1 = string.sub(jo1,1,1) .. jo1
		local c2 = string.sub(jo1,2,2) .. jo1

		ho[c1].jointOpening = true
		ho[c2].jointOpening = true
		ho[c2].jointOpening2 = true

		if #list &gt; 0 then
			i = math.random(1,#list)


			local jo1 = list[i]

			local c1 = string.sub(jo1,1,1) .. jo1
			local c2 = string.sub(jo1,2,2) .. jo1

			ho[c1].jointOpening = true
			ho[c2].jointOpening = true
			ho[c2].jointOpening2 = true

		end
	end
end


--------------------------------------------------------------------------------
-- For the side incline off of a main incline whose angle is mainAng, 
-- calculate the shallowest angle that the side incline can be and still be
-- inside of the adj main incline whose angle is adjAng.  Plus 15 degrees
-- to make interrior more open.

-- The mainAng and adjAng are in radians, result in  radians.

local function calcSideAngle( mainAng, adjAng)

	if mainAng &gt;= 90 then
		return 90
	end

	return math.atan(math.tan(adjAng)/math.cos(mainAng)) + math.rad(15)

end


----------------------------------------------------------------------------------
-- Map the corner angle to the corner incline length stop height needed
-- for a joint opening.  All empirical numbers.

local cangToStop = { 
	{a= 30.5, h=28},
	{a=48.7 , h=29},
	{a= 55.5, h=30},
	{a= 62.6, h=31},
	{a= 69.7, h=32},
	{a= 76.7, h= 33},
}

local function mapAngToStop( ang, height )

	ang = math.deg(ang)
	local sh = nil

	for i = 1,#cangToStop do

		if cangToStop[i].a &gt; ang then
			sh =  cangToStop[i].h
			break
		end
	end

	if not sh then
		sh = cangToStop[#cangToStop].h
	end

	if height &gt; 40 then
		sh = sh + (height-40) * .2 
	end

	return sh
end



-------------------------------------------------------------------------------------
local function calcOneCornerAttr(ca, sideAngle, adjSideAngle, height)

	ca.angle = calcSideAngle(sideAngle, adjSideAngle)

	local ls = 0

	if height &gt; MIN_HEIGHT_FOR_INTERIOR then
		local ls = 10
	end

	local ws = 0

	if ca.cornerOpening then
		ws = 15

	elseif ca.jointOpening then
		if ca.jointOpening2 then
			ls = mapAngToStop( ca.angle, height )
		else
			ca.angle = ca.angle + math.rad(15)
			ls = 10
		end
	end

	ca.ws = ws
	ca.ls = ls
end

-----------------------------------------------------------------------------------

local function calcCornerAttr(ho)

	local height = ho.height

	calcOneCornerAttr(ho.nnw, ho.n.angle, ho.w.angle, height)
	calcOneCornerAttr(ho.nne, ho.n.angle, ho.e.angle, height)
	calcOneCornerAttr(ho.ene, ho.e.angle, ho.n.angle, height)
	calcOneCornerAttr(ho.ese, ho.e.angle, ho.s.angle, height)
	calcOneCornerAttr(ho.sse, ho.s.angle, ho.e.angle, height)
	calcOneCornerAttr(ho.ssw, ho.s.angle, ho.w.angle, height)
	calcOneCornerAttr(ho.wsw, ho.w.angle, ho.s.angle, height)
	calcOneCornerAttr(ho.wnw, ho.w.angle, ho.n.angle, height)
end

