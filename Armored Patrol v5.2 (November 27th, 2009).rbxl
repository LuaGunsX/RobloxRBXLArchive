local function genOnePos()

	while true do

		local x = math.random(MIN_X, MAX_X)
		local z = math.random(MIN_Z, MAX_Z)	

		local pos = Vector3.new(x,0,z)

		for k,zone in pairs(Zones:GetChildren()) do

			if (zone.TopParamB &gt; 0) and 
				(zone.Position - pos).magnitude &lt; zone.TopParamB then

				return pos
			end
		end
	end
end


-----------------------------------------------------------------------------------------------------------
local function genAllRocks(pos)

	for i = 1, MAX_ROCKS do

		local pos = genOnePos()

		local xs = math.random(MIN_DIM, MAX_DIM)
		local ys = math.random(MIN_DIM, MAX_DIM)
		local zs = math.random(MIN_DIM, MAX_DIM)

		local rock = Instance.new(&quot;Part&quot;)
		rock.Name = &quot;Rock&quot;
		rock.formFactor = 0
		rock.Size = Vector3.new(xs,ys,zs)
		rock.FrontSurface = 2
		rock.Friction = 1
		rock.Elasticity = 0
		rock.TopSurface = 0
		rock.BottomSurface = 0
		rock.Material = Enum.Material.Slate
		rock.BrickColor = COLOR_ROCK

		local cfx = CFrame.fromAxisAngle(Vector3.new(1,0,0), math.rad(math.random(0,180)))
		local cfy = CFrame.fromAxisAngle(Vector3.new(0,1,0), math.rad(math.random(0,180)))
		local cfz = CFrame.fromAxisAngle(Vector3.new(0,0,1), math.rad(math.random(0,180)))

		rock.CFrame = CFrame.new(pos) * cfx * cfy * cfz

		rock.Anchored = true
		rock.Parent = Hills
	end

end

------------------------------------------------------------------------------------------------------

function _G.TerrainBuild()

	genAllHills()

	genAllRocks()
	
end

