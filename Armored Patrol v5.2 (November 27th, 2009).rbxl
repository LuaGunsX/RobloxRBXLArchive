					<string name="Name">ScriptRemGen-v1.0</string>
					<string name="Source">---------------------------------------------------------------------------------------------------
-- 2009/8/22 Initial version

-- This script is cloned and put under a GenPad which is put under the main model.
-- The ProtoModel under the main model is accessed to get the ProtoModel which is
-- cloned and each part is positioned on the GenPad in the same orientation as 
-- the proto part is to the ProtoGenPad.  The GenPad is removed, the generated model
-- is moved to game.Workspace, and this script returns.

local GenPad = script.Parent
local MainModel = GenPad.Parent
local ProtoGenPad = MainModel.ProtoGenPad
local ProtoModel = _G.RemGenProtoModels[MainModel.Name]
local OwnerPlayer = GenPad.CntrOwner.Value

local gmodel = ProtoModel:Clone()

local TEAM_RED = BrickColor.new(&quot;Bright red&quot;)
local TEAM_BLUE = BrickColor.new(&quot;Cyan&quot;)
local VEH_RED = BrickColor.new(&quot;Sand red&quot;)
local VEH_BLUE = BrickColor.new(&quot;Sand blue&quot;)
local VEH_DEFAULT = BrickColor.new(&quot;Dark stone grey&quot;)

-------------------------------------------------------------------------------------------------
-- Derive the CFrame of the gmodel Chassis positioned on the GenPad in the 
-- same relative position and rotation as the ProtoModel Chassis is to the ProtoGenPad.

local gchassis = gmodel.Chassis
local pchassis = ProtoModel.Chassis

local dcf = ProtoGenPad.CFrame:toObjectSpace(pchassis.CFrame)
local targetCFrame = GenPad.CFrame * dcf

-- Position each part of gmodel so its relation to gcframe is the same as the 
-- current relation it has with gchassis.CFrame.

local sourceCFrame = gchassis.CFrame

local function rec( veh )

	for k,v  in pairs(veh:GetChildren()) do

		if v.className == &quot;Part&quot; or v.className == &quot;Seat&quot; then

			if v.Name ~= &quot;Chassis&quot; then

				local dcf = sourceCFrame:toObjectSpace(v.CFrame)
				v.CFrame = targetCFrame * dcf
			end

		elseif v.className == &quot;Model&quot; then
			rec(v)
		end
	end
end

rec(gmodel)

-- Finally position the gmodel Chassis

gchassis.CFrame = targetCFrame

----------------------------------------------------------------------------------------
-- Enable all the scripts in the specified model

local function enableScripts(m)

	for k,v in ipairs(m:GetChildren()) do

		if v.className == &quot;Script&quot; then
			v.Disabled = false

		elseif v.className == &quot;Model&quot; then
			enableScripts(v)
		end
	end
end

enableScripts(gmodel)

-------------------------------------------------------------------------------------------------------
-- Recursively go thru every part in model and mark it with the players team color number.
-- If the part is one of the vehicle colors then
-- paint it to the new color (i.e. do not paint the non team specific colored parts).

local function paintVehicle(m, newColor,teamColorNumber)

	for k,p in pairs(m:GetChildren()) do

		if p.className == &quot;Model&quot; then
			paintVehicle(p, newColor, teamColorNumber)

		elseif (p.className == &quot;Part&quot;)  then
		
			p.BackParamA = teamColorNumber

			if (p.BrickColor == VEH_RED) or 
				(p.BrickColor == VEH_BLUE) or 
				(p.BrickColor == VEH_DEFAULT) then

				p.BrickColor = newColor
			end
		end
	end
end

-----------------------------------------------------------------------------------------
-- Assign vehicle owner and sequence number

local CntrOwner = gmodel:FindFirstChild(&quot;CntrOwner&quot;)
if CntrOwner then
	CntrOwner.Value = OwnerPlayer

	-- Assign latest owner seq number to determine order of ownership
	if _G.assignOwnerSeq then
		_G.assignOwnerSeq(gmodel:FindFirstChild(&quot;CntrOwnerSeq&quot;))
	end

end

-- Paint vehicle

if OwnerPlayer.TeamColor == TEAM_RED then
	paintVehicle(gmodel, VEH_RED, OwnerPlayer.TeamColor.Number)

elseif OwnerPlayer.TeamColor == TEAM_BLUE then
	paintVehicle(gmodel, VEH_BLUE, OwnerPlayer.TeamColor.Number)
end


-- Put the model into the world.

gmodel.Parent = game.Workspace

gmodel:MakeJoints()

-- Remove GenPad (our parent)

GenPad:Remove()






