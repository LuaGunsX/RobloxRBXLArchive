	if (v.className == &quot;Script&quot;) and (string.match(v.Name, &quot;ScriptHillControl&quot;)) then
		HillScript = v
		break
	end
end

local scriptList = {}

-- Get the list of replacement scripts from the local model.  Derive the 
-- base name of the script by stripping out any -XXX suffice (presumably
-- version information).  The list is indexed by the script base name and
-- contains the script.
for i,v in ipairs(GameControl:GetChildren()) do

	if v.className == &quot;Script&quot; then

		--print(&quot;Found script &quot;, v.Name)

		local s = string.gsub(v.Name,&quot;%-.*&quot;, &quot;&quot;)

		if scriptList[s] then
			print(&quot;ScriptArmoredPatrolGame: Error more than one replacement script has same base name &quot;,  s)
		else

			--print(&quot;replacement script found, base name &quot;, s)
			scriptList[s] = v
		end
	end
end


-- Find or create the TeamSwitchList model

local TeamSwitchList = game.Workspace:FindFirstChild(&quot;TeamSwitchList&quot;)

if not TeamSwitchList then
	TeamSwitchList = Instance.new(&quot;Model&quot;)
	TeamSwitchList.Name = &quot;TeamSwitchList&quot;
	TeamSwitchList.Parent = game.Workspace	
end

local function emptySwitchList()

	for k,v in pairs(TeamSwitchList:GetChildren()) do

		if v.Name == &quot;TeamSwitcher&quot; then	
