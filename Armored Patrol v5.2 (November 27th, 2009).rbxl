							<string name="Name">ScriptControlVehA-v1</string>
							<string name="Source">-------------------------------------------------------------------------------------------------------------------------------
--2009/5/8 Initial version based on ControlJeepScript-v2.1
--2009/5/8 Clean up drive controls, enforce max speed, speed restrict with adjunct


-- Features:

	-- For Jeep control.

	-- Receives commands from tool

	-- Initially wheels ParamB is set to true zero to keep moving part
	-- count down, after Player has used controls ParamB is set to
	-- a very tiny value instead of true zero to simulate braking.

	-- Hints give current speed.

	-- Knows about SeatDriver.

	-- Continually steers toward last target.

	-- Defines SteerMotors here instead of separate wheel script

	-- Chassis.TopParamA - Max speed 
	-- Chassis.TopParamB - Max speed (if towing something)

	-- Monitor for when adjunct equipment gone and adjust max speed

	-- Auto turn-off when no CntrUser for threshold time.


local Vehicle=script.Parent
local Torso

-- Insure MakeJoints is done
Vehicle:MakeJoints()

-- Control Values.  CntrDrive communicates accel, decel, brake commands.
-- CntrSteer communicates new steer target position.
-- CntrUser contains the Player object that has both welded to the seat
-- and selected the driving tool (he is the primary (and only) user).

local CntrDrive = Vehicle.CntrDrive
local CntrSteer = Vehicle.CntrSteer
local CntrUser= Vehicle.CntrUser

local Chassis = Vehicle.Chassis
local DriveTrain = Vehicle.DriveTrain

-- Vehicle parts and parameters

local FLDriveMotor = DriveTrain.FLDriveMotor
local FRDriveMotor = DriveTrain.FRDriveMotor
local RLDriveMotor = DriveTrain.RLWheel	-- Motor in the Wheel
local RRDriveMotor = DriveTrain.RRWheel	-- Motor in the Wheel

local FLSteerMotor 
local FRSteerMotor 

-- Get the right rear wheel position in left front wheel object space
local rrpo = DriveTrain.FLWheel.CFrame:pointToObjectSpace(DriveTrain.RRWheel.Position)

local vehLen = math.abs(rrpo.z)
local vehWid = math.abs(rrpo.x)

local seat = Vehicle.SeatDriver

-- The ratio between the actual RPS of a motor surfaceType and the
-- value specified for the xxxParamB parameter.  The value below
-- is the value to multiply by the xxxParamB parameter to find out
-- the rotations per second it will make the wheel go.
local ParamB_TO_RPS_RATIO = 4.6 

-- The PB_RATIO is the ParamB change needed for each studs/sec vehicle speed change.
local PB_RATIO =  1 /(DriveTrain.FLWheel.Size.x * 3.14159 * ParamB_TO_RPS_RATIO)

-- The speedMax is fastest vehicle can go.  The speedMaxCur is the current upper limit
-- which is initialized to the maximum speed while towing adjunct equipment.
-- The speedReq is the current user requested speed.  It can range from 0 to
-- speedMax.

local speedMax = Chassis.TopParamA
local speedTowingMax = Chassis.TopParamB
local speedMaxCur = speedTowingMax
local speedIncr = 10					-- How to accelerate by for each user accel command
local speedReq = 0						-- Current user requested speed

local SPEED_BRAKE = .000001			-- To simulate braking set wheels to very slow speed	
local SPEED_IDLE = 0					-- When vehicle abandoned set wheels to true stop 
									--   to reduce load on physics engine

local MAX_STEER_VEL = .05 -- the fastest the wheels will steer
local MIN_STEER_VEL = .005

local lastSteerTarget

-- See if we are towing adjunct equipment

local AdjunctEquipment = nil

for k,v in pairs(Vehicle:GetChildren()) do
	if string.match(v.Name, &quot;Adjunct&quot;) then
		AdjunctEquipment = v
		break
	end
end

----------------------------------------------------------------------------------------------------------------------------------------------
local function mountSteerWheel( chassis, Wheel, DriveMotor, steerMotorName)

	--Create Motor object with caller specified name

	local SteerMotor = Instance.new(&quot;Motor&quot;)
	SteerMotor.Name = steerMotorName

	-- Connect DriveMotor to Chassis via Motor object

	-- Cframe from center of wheel (axis straight down in world view) in Wheel object space.	
	local cfwo = CFrame.new(0, 0, 0, 0, 0, -1, 0, 1, 0, 1, 0, 0) -- Rotate neg Z to neg X	

	--print(&quot;DriveMotor.CFrame (before SteerMotor) &quot; , DriveMotor.CFrame)

	-- The same cframe in chassis and DriveMotor object space
	local cfco = chassis.CFrame:toObjectSpace(Wheel.CFrame:toWorldSpace(cfwo))
	local cfdo = DriveMotor.CFrame:toObjectSpace(Wheel.CFrame:toWorldSpace(cfwo))

	SteerMotor.Parent = game.Workspace
	SteerMotor.Part0= chassis
	SteerMotor.Part1=DriveMotor
	SteerMotor.C0 = cfco
	SteerMotor.C1 = cfdo

	return SteerMotor
end


--------------------------------------------------------------------------------------------------------
-- Restrict speed to the speedMaxCur.

-- Adjust speed on each rear wheel to the specified speed compensating for
-- the fact that the front wheels may be turned and the inner rear wheel should turn
-- more slowly.

function adjSpeed( speed ) 

	if speed &gt; speedMaxCur then
		speed = speedMaxCur
	end

	local rpmFront = speed * PB_RATIO
	local rpmRear = speed * PB_RATIO

	local s = math.sin(math.abs( FLSteerMotor.CurrentAngle )) * (vehWid /2)   

	local r = (vehLen - s) / (vehLen + s)

	--print(&quot;speed &quot;, s, &quot; r &quot;, r)

	-- Turning right, left rear goes full speed, right rear reduced 
	if FLSteerMotor.CurrentAngle &gt; 0 then
		RLDriveMotor.BottomParamB = rpmRear
		RRDriveMotor.BottomParamB =  -(rpmRear * r) 

		FLDriveMotor.TopParamB = rpmFront
		FRDriveMotor.TopParamB =  -(rpmFront * r) 


	-- Else turning left, right rear goes full speed, left rear reduced    
	else    
		RLDriveMotor.BottomParamB = rpmRear * r
		RRDriveMotor.BottomParamB =  -rpmRear

		FLDriveMotor.TopParamB = rpmFront * r
		FRDriveMotor.TopParamB =  -rpmFront
	end
end
------------------------------------------------------------------------------------------------------------
local function noteAdjunctRemoved()

	speedMaxCur = speedMax
	adjSpeed(speedReq)

	AdjunctEquipment = nil
end

------------------------------------------------------------------------------------------------------------
local function releaseHitch()

	local hitchBar = Vehicle:FindFirstChild(&quot;HitchBar&quot;)

	if hitchBar then
		hitchBar.Parent = nil
	end

	noteAdjunctRemoved()

end
------------------------------------------------------------------------------------------------------------
-- To preserve responsiveness when first starting from idle we
-- immediately assign the first increment to the wheels at 
-- command handler time rather than wait for the main loop.

function handleDriveCmd()

	local key = string.lower(CntrDrive.Value)

	-- Accelerate
	if (key == &quot;y&quot;)  then

		if speedReq &lt; speedMax then
			speedReq = speedMax
			adjSpeed(speedReq)
		end

	-- Accelerate
	elseif (key == &quot;h&quot;)  then

		if speedReq &lt; speedMax then
			speedReq = speedReq + speedIncr
			adjSpeed(speedReq)
		end

	-- Decelerate
	elseif (key==&quot;j&quot;)   then

		if speedReq &gt; -speedMax then
			speedReq = speedReq - speedIncr
			adjSpeed(speedReq)
		end

	-- Stop all acceleration and deceleration
	elseif (key==&quot;u&quot;) or (key == &quot;x&quot;) then
		speedReq = SPEED_BRAKE
		adjSpeed(speedReq)

	
	elseif key == &quot;r&quot; then
		releaseHitch()

	end
end

-------------------------------------------------------------------------------------------------------------
function steerToward(dest)

         --Translate target to a unit vector from our position
	local target = (dest - seat.Position).unit

	-- get seat look vector
	local face = seat.CFrame.lookVector

	-- calc angle from seat to target on the xz plane
	local angle = math.atan2(target.z,target.x) - math.atan2(face.z,face.x)

	if angle &lt; -3.14159 then
		angle = 6.28318 + angle
	elseif angle &gt; 3.14159 then
		angle = angle - 6.28318
	end


	-- Target angle now is with respect to the facing direction of the vehicle.
	-- Positive angles go to the right of vehicle direction, negative angles
	-- go to the left.  The angle is normallized to within pi to -pi (i.e +180 degrees to -180 degrees).
	-- We adjust angle to the angle we want to steer the wheels as follows:
	--
	--	From 0 to 2.34 steer to the right that angle up to max of .8.
	--	From 2.34 to pi, the target is behind the vehicle but still to the right of vehicle.  
	--		Assume we are driving backward and still steer to the right but make wheels
	--		straighter as we approach pi.

	--	From 0 to -2.34: steer to the left that angle up to min of  -.8.
	--	From -2.34 to -pi: the target is behind the vehicle but still to the left of vehicle.
	--		Assume we are driving backwards and still steer to the left but make wheels
	--		straighter as we approach -pi.

	if (angle &gt; .8) then
		if (angle &lt; 2.34) then
			angle = .8
		else
			angle = 3.14159 - angle
		end
	elseif (angle &lt; -.8) then
		if (angle &gt; -2.34) then
			angle = -.8
		else
			angle = -3.14159 - angle
		end
	end


	--print(&quot;angle&quot;, angle)


	FLSteerMotor.DesiredAngle = angle
	FRSteerMotor.DesiredAngle = angle


	FLSteerMotor.MaxVelocity = MAX_STEER_VEL
	FRSteerMotor.MaxVelocity = MAX_STEER_VEL

              
	adjSpeed(speedReq)
end

---------------------------------------------------------------------------------------------------------
-- Compute a fake target very far away in the direction of the actual target so
-- we can continue to steer toward this target in the absence of any new steer
-- commands.
local function handleSteerCmd()

	local svec = (CntrSteer.Value - seat.Position).unit

	lastSteerTarget = seat.Position + (svec * 100000)

	steerToward(lastSteerTarget)
end

--------------------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------------------
-- Define a BodyGyro used to right a vehicle that has flipped over

local BodyGyro = Instance.new(&quot;BodyGyro&quot;)
BodyGyro.D = 9000
BodyGyro.P = 9000
BodyGyro.maxTorque = Vector3.new(40,0,40)

local state = &quot;Normal&quot;

local prevPos = Chassis.Position
local upCF = CFrame.fromAxisAngle(Vector3.new(1,0,0), math.rad(90))
local toc = 0	-- Tipped over counter


-------------------------------------------------------------------------
-- Get Current angle the Vehicle makes from the vertical

local function getAngle()

	local lv = (Chassis.CFrame * upCF).lookVector

	local ang

	if lv.y &gt; 1 or lv.y &lt; -1 then
		ang = 180

	else
		ang = math.deg(math.acos(lv.y))
	end

	return ang
end

----------------------------------------------------------------------
local function maintainOrientation()

	if state == &quot;Normal&quot; then

		-- If we are not moving check to see if we are not right side up

		if (prevPos - Chassis.Position).magnitude &lt; 1 then
	
			if getAngle() &gt; 85 then

				toc = 0
				state = &quot;PossibleFlipped&quot;
			end
		end

	elseif state == &quot;PossibleFlipped&quot; then

		if ((prevPos - Chassis.Position).magnitude &gt; 1) or
			getAngle() &lt;= 40 then

			state = &quot;Normal&quot;

		else
			toc = toc + 1
		
			if toc &gt; 8 then
				print(&quot;Tipped over &quot;, ang)

				-- Set the BodyGyro to be aligned to the current xz axis of Chassis with
				-- the y axis rotated about 20 degrees toward the z axis.
				local lv = Chassis.CFrame.lookVector
				lv = Vector3.new(lv.x,0,lv.z)

				BodyGyro.cframe = CFrame.new(Chassis.Position, Chassis.Position+lv) * 
					CFrame.fromAxisAngle(Vector3.new(0,0,1), math.rad(20))
				
				BodyGyro.Parent = Chassis
				state = &quot;Righting&quot;
			end
		end

	elseif state == &quot;Righting&quot; then

		if ((prevPos - Chassis.Position).magnitude &gt; 10) or
			getAngle() &lt;= 40 then

			BodyGyro.Parent = nil
			state = &quot;Normal&quot;	
		end
	end

	prevPos = Chassis.Position

end
---------------------------------------------------------------------------------------------------------

-- Create the Steer motors between each Drive Motor and the Chassis

FLSteerMotor = mountSteerWheel(Chassis, DriveTrain.FLWheel, DriveTrain.FLDriveMotor, &quot;FLSteerMotor&quot;)
FRSteerMotor = mountSteerWheel(Chassis, DriveTrain.FRWheel, DriveTrain.FRDriveMotor, &quot;FRSteerMotor&quot;)

CntrUser.Value = nil

CntrDrive.Changed:connect(handleDriveCmd)
CntrSteer.Changed:connect(handleSteerCmd)

local Hint = Instance.new(&quot;Hint&quot;)

local CONTROL_INTERVAL = .334
local IDLE_TIME_THRESHOLD = 30

local enabledHints = false
local idleTime = 0


while (true) do

	wait(CONTROL_INTERVAL)

	if CntrUser.Value then

		local s = seat.Velocity.magnitude

		if (s &gt; 5) or enabledHints then
			Hint.Text = string.format(&quot;Req Speed: %.1f Cur Speed: %.1f&quot;,speedReq, s) 
			Hint.Parent = CntrUser.Value

			enabledHints = true
		end

		idleTime = 0

	else
		Hint.Parent = nil
		enabledHints = false

		-- If we do not have a user for some time, then set wheel speed to true
		-- zero so the physics engine can mark this vehicle as not moving.
		-- Also this will auto-turnoff any runaway vehicles.
		idleTime = idleTime + CONTROL_INTERVAL

		if idleTime &gt;= IDLE_TIME_THRESHOLD then
			speedReq = SPEED_IDLE
			adjSpeed(speedReq)

			idleTime = 0
		end
	end


	if lastSteerTarget then
		steerToward(lastSteerTarget)
	end

	-- Monitor to see if vehicle tipped over, and, if so, right it.
	maintainOrientation()

	-- Monitor to see if we are still towing adjunct equipment.  If no then
	-- increase our speedMaxCur

	if AdjunctEquipment and (AdjunctEquipment.Parent ~= Vehicle) then
		noteAdjunctRemoved()
	end
			
end

