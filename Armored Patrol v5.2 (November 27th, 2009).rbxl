-- Status indicator:

	-- base color: not selected by user

	-- Green: ready to fire
	-- White: reloading
	-- Red: reloading, previous launch was out of range



local Howitzer = script.Parent

local BarrelAssm = Howitzer.MovableBarrelAssm
local Chassis = Howitzer.Chassis
local GlobalScripts = game.Workspace.GlobalScripts


local Mount = BarrelAssm.Mount
local Barrel = BarrelAssm.Barrel

local Hitch = Howitzer.Hitch

local CntrUser = Howitzer.CntrUser
local CntrParent = Howitzer.CntrParent

local CntrCmd = Instance.new(&quot;StringValue&quot;)	-- to receive commands from Tool
CntrCmd.Name = &quot;CntrCmd&quot;
CntrCmd.Parent = Howitzer

local CntrAim = Instance.new(&quot;Vector3Value&quot;)	-- to receive aiming vector from Tool
CntrAim.Name = &quot;CntrAim&quot;
CntrAim.Parent = Howitzer

-- Values that represent nil for the CntrCmd and CntrAim values (because
-- Lua nil is not allowed to be assigned).
local NIL_CMD = &quot;-&quot;
local NIL_AIM = Vector3.new(0,0,0)

-- Bolt parameter sets used for ASV shells

local BOLTG_CODE_30	= 8103	-- Howitzer shell (30 degree launch angle)
local BOLTG_CODE_45	= 8203	-- Howitzer shell (45 degree launch angle)
local BOLTG_CODE_70	= 8303	-- Howitzershell (70 degree launch angle)

local POST_GEN_DELAY = 15	-- Amount of time that must elapsed after a Howitzer is 
						-- gened before it may be used.

local ProtoMissile = game.Workspace.ProtoMissile
local ProtoShell = game.Workspace.ProtoShell

-- Insure all joints in BarrelAssm are made.
BarrelAssm:MakeJoints()

--Howitzer:MakeJoints()

-- Weld the left and right armor plates to Mount
local Lweld = Instance.new(&quot;Weld&quot;) 
local cf = Mount.CFrame:toObjectSpace(BarrelAssm.WeldArmorLeft.CFrame) 
Lweld.Name = &quot;Weld&quot;
Lweld.Parent = game.Workspace
Lweld.Part0 = Mount --&gt; part0 has to be the parent 
Lweld.Part1 = BarrelAssm.WeldArmorLeft
Lweld.C0 = cf

local Rweld = Instance.new(&quot;Weld&quot;) 
cf = Mount.CFrame:toObjectSpace(BarrelAssm.WeldArmorRight.CFrame) 
Rweld.Name = &quot;Weld&quot;
Rweld.Parent = game.Workspace
Rweld.Part0 = Mount --&gt; part0 has to be the parent 
Rweld.Part1 = BarrelAssm.WeldArmorRight
Rweld.C0 = cf


-- Weld the Mount to the Chassis

local Mweld = Instance.new(&quot;Weld&quot;) 
local mcf = Chassis.CFrame:toObjectSpace(Mount.CFrame) 
Mweld.Name = &quot;Weld&quot;
Mweld.Parent = game.Workspace
Mweld.Part0 = Chassis --&gt; part0 has to be the parent 
Mweld.Part1 = Mount 
Mweld.C0 = CFrame.new(0, 1, 1.5)
Mweld.C1 = CFrame.new(0,0,1.5)



-- Howitzer status variables

local hitched = true		-- Still connected to parent Jeep
local attached = nil 	-- Player character if attached
local reloading = false
local alarm = false

-- Howitzer status indicator

local Status = Howitzer.MovableBarrelAssm.Status
local STATUS_BASE_COLOR = Status.BrickColor
local STATUS_READY = BrickColor.new(&quot;Bright green&quot;)
local STATUS_RELOAD = BrickColor.new(&quot;White&quot;)
local STATUS_ALARM = BrickColor.new(&quot;Bright red&quot;)

-- Constants
local RELOAD_TIME = 5

-- Configured in the Howitzers Mount part:
-- Mount.TopParamA: max (positive)  degrees barrel can elevate up
-- Mount.BottomParamA: max (negative)  degrees barrel can elevate down
-- Mount.RightParamA: max (positive) degrees barrel can turn to right
-- Mount.LeftParamA: max (negative) degrees barrel can turn to left 

local MaxUpAngle = math.rad(Mount.TopParamA)
local MaxDownAngle = math.rad(Mount.BottomParamA)
local MaxLeftAngle = math.rad(Mount.LeftParamA)
local MaxRightAngle = math.rad(Mount.RightParamA)


-- If the model above us does not have Jeep in its name then we do not
-- have a Jeep parent
local Jeep = Howitzer.Parent

if string.match(Jeep.Name, &quot;Jeep&quot;) then
	CntrParent.Value = Jeep
else
	Jeep = nil
end



-------------------------------------------------------------------------------------------------
local function intact()

	if Barrel.Parent and Mount.Parent and Mweld.Parent  then
		return true
	else
		return false
	end
end

--------------------------------------------------------------------------------------------------
local function setStatus()

	if Status.Parent == nil then
		return
	end

	if CntrUser.Value == nil then
	
		Status.BrickColor = STATUS_BASE_COLOR

	else
		if reloading then
	
			if alarm then
				Status.BrickColor = STATUS_ALARM
			else
				Status.BrickColor = STATUS_RELOAD
			end
		else
			Status.BrickColor = STATUS_READY
		end
	end
end


-------------------------------------------------------------------------------------------------
local RRWheel = Howitzer.RRWheel
local RLWheel = Howitzer.RLWheel

local function wheelMotion( val )

	RRWheel.BottomParamB = -val
	RLWheel.BottomParamB = val

end
-------------------------------------------------------------------------------------------------
-- This routine is called after a Bolt is fired to manage the timeout
-- until the vehicle can be fired again.  The Bolt that was fired is passed
-- to this routine.  We wait for the RELOAD_TIME, then we check to see if the 
-- specified Bolt is still in the world.  If yes, we conclude the server
-- is lagging.  To prevent adding to server load by having multiple simultaneous Bolts 
-- from this vehicle, we wait for the Bolt to be removed from the world
-- (indicating it finally reached its target) or until a much higher timeout time
-- is reached (to guard against some unknown error).

local function reload( cball )

	wait(RELOAD_TIME)
	
	local waitTime = RELOAD_TIME * 10

	while cball.Parent and (waitTime &gt; 0) do

		wait(1)
		waitTime = waitTime - 1

	end

	if reloading then
		reloading = false
	end
	
	alarm = false

	setStatus()
end


------------------------------------------------------------------------------------------------
local function fire( sourcePos, dirCf )

	local bolt = ProtoMissile:Clone()
	bolt.Name = &quot;Bolt&quot;
	
	bolt.CFrame =  dirCf + sourcePos
	bolt.FrontParamA = 8003	-- ASV missile parameter set

	-- Enable any script in the Bolt
	for k,v in pairs(bolt:GetChildren()) do
		if v.className == &quot;Script&quot; then
			v.Disabled = false
		end
	end

	bolt.Creator.Value = CntrUser.Value
	bolt.BrickColor = CntrUser.Value.TeamColor

	-- Put the Bolt into the world
	bolt.Parent = game.Workspace

	reloading = true

	local co = coroutine.create(reload)
	assert(coroutine.resume(co, bolt))
end
