					<string name="Source">---------------------------------------------------------------------------------------------------
--2009/5/17 Initial
--2009/5/22 Use Zone markers to determine where not to place hills
--2009/5/27 Fixed floor not going all the way to edge
--2009/5/28 Add defensive pcalls
--2009/6/11 Fix intermittent non-placement of caches
--2009/6/29 Add flag capture
--2009/7/25 Make top green instead of using a grass decal
--2009/8/11 Define global _G.BuildTerrain and _G.RemoveTerrain
--2009/8/27 More and bigger rocks
--2009/8/28 Ridges to edges of base for those hills close enough to edge
--2009/9/6 Fix floor thickness, add floor for ridges
--2009/9/11 Increased hill size, decrease rock size


-- Features

	--Create corner opening for any corner that overlaps at OVERLAP_HEIGHT

	--Create joint opening for up to 2 randomly selected joints that have no
	--   corner openings.

	-- Create openings only if hill meet minimum height.

	-- Tilted floor tiles

	-- Place cache at corner openings

	-- Uses Zone markers to determine where not to put hills

	-- For each zone marker:

		-- .TopParamA - if nonzero no hill center can be closer than this
		-- .TopParamB - if nonzero a rock must be within this distance 

local Hills = game.Workspace.Hills
local CacheProto = game.Workspace.CacheProto
local Zones = game.Workspace:FindFirstChild(&quot;Zones&quot;)

if Zones then
	Zones.Parent = nil
end

local BSize = game.Workspace.Base.Size

local H_GAP = 200	-- Gap between edge and where hills may be gened
local T_GAP = 190	-- Distance a hill top corner must be from edge making hill top elligible for a ridge

local LOW_X_EDGE = -BSize.x/2
local HIGH_X_EDGE = BSize.x/2
local LOW_Z_EDGE = -BSize.z/2
local HIGH_Z_EDGE = BSize.z/2

local MIN_X = 	LOW_X_EDGE + H_GAP
local MAX_X = HIGH_X_EDGE - H_GAP
local MIN_Z = LOW_Z_EDGE + H_GAP
local MAX_Z = HIGH_Z_EDGE - H_GAP


local MAX_HILLS =  25
local MAX_HEIGHT = 60
local MIN_HEIGHT = 10
local MAX_SIDE = 160 --130
local MIN_SIDE = 50 --30	--10
local MIN_INCLINE = 15
local MAX_INCLINE = 90

local THICKNESS_TOP = 14.4
local THICKNESS_SIDE = 7.2

local MAX_ROCKS =  15
local MAX_DIM = 25
local MIN_DIM = 14

local MAX_FLAGS = 3

local BASE_HEIGHT = .4	-- Height of whatever hills are built on

-- How far the center of a hill must be from a Zone marker
local ZONE_BUFFER = 200


local COLOR_TOP = BrickColor.new(&quot;Earth green&quot;)
--local COLOR_TOP = BrickColor.new(&quot;Dark stone grey&quot;)
local COLOR_SIDE = BrickColor.new(&quot;Dark stone grey&quot;)
local COLOR_FLOOR = BrickColor.new(&quot;Dark stone grey&quot;)

local COLOR_ROCK = BrickColor.new(&quot;Dark stone grey&quot;)

-- This height determines how far into each side incline we have to go
-- before considering two hills to be overlapping
local OVERLAP_HEIGHT = 15

local MIN_HEIGHT_FOR_INTERIOR = 25

local MIN_CORNER_SIZE = 7


local CH = 13
local CHW = 15
local CHL = 10

-- Find the capture the hill script

local ScriptCTH

for k,v in pairs(script.Parent:GetChildren()) do

	if (v.className == &quot;Script&quot;) and (string.match(v.Name, &quot;ScriptCTH&quot;)) then
		ScriptCTH = v
		break
	end
end

---------------------------------------------------------------------------------------------------
-- Generate random integer number with skewed normal distribution.
-- Means is about 20.  Lowest value is 15, highest is 90.

local toggle = true
local x1,x2

local function randNorm()

    local x

    if toggle then
        x1 =  math.random()
        x2 =  math.random()

        x = math.sqrt( -2 * math.log(x1)) * math.cos( 2 * 3.14159 * x2)

        toggle = false

    else
        x = math.sqrt( -2 * math.log(x1)) * math.sin( 2 * 3.14159 * x2)

        toggle = true
    end
       
    if x &lt; 0 then
        x = x * 3
    else
        x = x * 15
    end

    local m = math.floor( x + 22.5)

    if m &lt; 15 then
        m = 15
    elseif m &gt; 90 then
        m = 90
    end

    return m
end




--[[--------------------------------------------------------------------------------------
	Make one incline relative to base.  
        
	base - part to build incline relative to
	fcf - rotation CFrame to pick side to build ramp off of (rel to base)
	width - width of side of ramp touching base
	wShift - amount to shift incline in the x axis rel to base (pos to right, neg to left)
	dist - distance from center of base to start ramp
	len - length of side of ramp extending from base
	angle = angle ramp make with base
--]]
function makeIncline(Hill, base,fcf,width,wShift,dist,len,angle, name)

	--print(&quot;makeIncline base &quot;,base, &quot; fcf &quot;,fcf,&quot; width &quot;,width,&quot; dist &quot;, dist,&quot; len &quot;,len,&quot; angle &quot;,angle)

	-- Amount to translate after tilting (in base cframe)
	local dlen = (len/2) * math.cos(angle)
	local dht = (len/2) * math.sin(angle)

	local incline = Instance.new(&quot;Part&quot;)
	incline.Name = name
	incline.formFactor = 2
	incline.Size = Vector3.new(width, THICKNESS_SIDE, len)
	incline.FrontSurface = 2 -- Weld, just to mark it
	incline.Friction = 1
	incline.Elasticity = 0
	incline.TopSurface = 0
--incline.Transparency = .5

	incline.Material = Enum.Material.Slate
	incline.BrickColor = COLOR_SIDE

	local t = Instance.new(&quot;Texture&quot;)
	--t.Texture = &quot;http://www.roblox.com/asset/?id=2060902&quot;
	t.Texture = &quot;http://www.roblox.com/asset/?id=2022921&quot;
	t.Face = 1
	t.StudsPerTileU = 50
	t.StudsPerTileV = 50
	--t.Parent = incline

	-- CFrame of base rotated to the direction of this incline
	local rcf = base.CFrame * fcf

	-- Rotated and tilted CFrame of the incline, centered on base. 
	local cf = rcf * CFrame.fromAxisAngle(Vector3.new(1,0,0), -angle)

	-- Offset vector from center of base to center of where incline should go.
	local v = rcf:vectorToWorldSpace(Vector3.new(wShift,-dht, -(dist+dlen)))

	cf = cf + v

	incline.CFrame = cf

	local t = Instance.new(&quot;Texture&quot;)
	--t.Texture = &quot;http://www.roblox.com/asset/?id=2060902&quot;
	t.Texture = &quot;http://www.roblox.com/asset/?id=2022921&quot;
	t.Face = 1
	t.StudsPerTileU = 50
	t.StudsPerTileV = 50
	--t.Parent = incline

	incline.Parent = Hill
	incline.Anchored = true

	return incline
end



--------------------------------------------------------------------------------
-- Orthogonal rotation CFrames

local cfn = CFrame.new(0,0,0,1,0,0,0,1,0,0,0,1) -- north (0 degrees rotation)_
local cfe = CFrame.new(0,0,0,0,0,-1,0,1,0,1,0,0) -- east (270 degrees rotation)
local cfs = CFrame.new(0,0,0,-1,0,0,0,1,0,0,0,-1) -- south (180 degrees rotation)
local cfw = CFrame.new(0,0,0,0,0,1,0,1,0,-1,0,0) -- west (90 degrees rotation)

-----------------------------------------------------------------------------------
-- Make one hill side consisting of (optionally) a main incline and 
-- (optionally) a left corner incline and (optionally) a right corner incline.
-- Caller specifies:

--	Hill: the hill these inclines are part of,
--	top: the hill top part the inclines descend from,
--	height: height of hill top,
--	width: the size of side incline along edge touching top (i.e x axis in top object space)
--	dist: distance from center of top to sideincline edge (i.e. offset in z axis in top object space)
--	mainAng: the angle of the side incline (in radians)
--
--	leftAng: the angle (relative to the side incline) of the left corner incline in radians (0 means do not build)
--	leftHeightW: how high from ground the left corner incline is to stop in the width dimension
--	leftHeightL: how high from ground the left corner incline is to stop in the length dimension

--	Similarly for the right corner incline.

-- 	If the calculated length or width of any corner-incline is less than a minimum then do not
--	create it at all.

local function makeSide(
	Hill,top, cfd, height, width, dist, mainAng, 
	leftAng, leftHeightW, leftHeightL, 
	rightAng, rightHeightW, rightHeightL)

	--print(&quot;MainAng &quot;, mainAng)

	if mainAng == 0 then
		return
	end

	local slen = math.ceil(height / math.sin(mainAng))

	local r1 = makeIncline(Hill, top, cfd, width, 0, dist, slen, mainAng, &quot;SideIncline&quot;)

	if leftAng &gt; 0 then

		-- The side-incline len is our width.  Then subtract off whatever
		-- is needed to make the corner incline stop at leftHeightW from ground.
		local w = slen - math.ceil(leftHeightW/math.sin(mainAng))
		local sw = -(slen - w)/2

		-- Ratio of height to hypotenuse
		local r = 1 / (math.sin(leftAng) * math.cos(mainAng))

		-- Length needed to reach from top to leftHeightL
		local clen = math.floor((height - leftHeightL) * r)

		--print(&quot;Left corner bef: &quot;, w, &quot; &quot;, sw, &quot; &quot;,width/2, &quot; &quot;, clen, &quot; &quot;,leftAng)

		if (w &gt; MIN_CORNER_SIZE) and (clen &gt; MIN_CORNER_SIZE) then 

			--print(&quot;Left corner aft: &quot;, w, &quot; &quot;, sw, &quot; &quot;,width/2, &quot; &quot;, clen, &quot; &quot;,leftAng)
		
			makeIncline(Hill, r1, cfw, w, sw, width/2, clen, leftAng, &quot;CornerIncline&quot;)
		end
	end

	if rightAng &gt; 0 then

		-- The side-incline len is our width.  Then subtract off whatever
		-- is needed to make the corner incline stop at rightHeightW from ground.
		local w = slen - math.ceil(rightHeightW/math.sin(mainAng))
		local sw = (slen - w)/2

		-- Ratio of height to hypotenuse
		local r = 1 / (math.sin(rightAng) * math.cos(mainAng))

		-- Length needed to reach from top to rightHeightL
		local clen = math.floor((height - rightHeightL) * r)

		--print(&quot;Right corner bef: &quot;, w, &quot; &quot;, sw, &quot; &quot;,width/2, &quot; &quot;, clen, &quot; &quot;,leftAng)
		
		if (w &gt; MIN_CORNER_SIZE) and (clen &gt; MIN_CORNER_SIZE) then 

			--print(&quot;Right corner aft: &quot;, w, &quot; &quot;, sw, &quot; &quot;,width/2, &quot; &quot;, clen, &quot; &quot;,leftAng)

			makeIncline(Hill, r1, cfe, w, sw, width/2, clen, rightAng, &quot;CornerIncline&quot;)
		end
	end	
end

----------------------------------------------------------------------------
-- All parts are created full thickness but positioned as if they 
-- have zero thickness.  This routine adjusts each part by positioning the
-- Top surface where the middle of the part is. This is done by moving the
-- part 1/2 its thickness in the negative Y direction (in the parts object space). 

local function adjustForThickness(Hill)

	for k,p in pairs(Hill:GetChildren()) do

		if p.className == &quot;Part&quot; then

			local offsetVec = p.CFrame:vectorToWorldSpace(Vector3.new(0, -p.Size.y/2, 0))
			p.CFrame = p.CFrame + offsetVec
		end
	end
end

-----------------------------------------------------------------------------------------------------------------------------------
-- Calculate the rotation about the x axis (a) then z-axis (b) needed such the two xz points (relative to
-- the upper left corner rotation point) would both be lowered in height by h.  (These formulas were
-- derived using Mathematica.)
local function calcAngles(x1,z1,x2,z2,h)

	local rn= math.sqrt(-(z1*z1*h*h) - (z2*z2*h*h) + (2*z1*z2*h*h) + (x2*x2*z1*z1) + (x1*x1*z2*z2) - (2*x1*x2*z1*z2))
	local rd = math.sqrt( (x2*x2*z1*z1) - (2*x1*x2*z2*z1) + (x1*x1*z2*z2))
	local r = rn / rd

	local b = -math.acos( r)

	local an = r * ( -(h*z2*x1*x1) + (h*x2*z1*x1) + (h*x2*z2*x1) - (h*x2*x2*z1))
	local ad = (z1*z1*h*h) + (z2*z2*h*h) - (2*z1*z2*h*h) -(x2*x2*z1*z1) - (x1*x1*z2*z2) + (2*x1*x2*z1*z2)

	local a = math.asin( -an /ad )

	return a,b
end

----------------------------------------------------------------------------
-- Create and position one floor tile.  The caller specifies the angle of the 2 side inclines
-- that this floor tile straddles.  Caller also passes which quadrant this tile is in wrt to the
-- Top CFrame. This is done with the xq and zq which are either -1 or 1.

local FLOOR_THICKNESS = 2
local FLOOR_RECEDE = 5	-- How far to recede floor from end of incline to avoid a bump protruding thru incline

local function 	makeFloorTile(Hill, Top, ew_angle, ns_angle, xq, zq  )

	local h = Top.Position.y

	local el = ((h - BASE_HEIGHT) / math.tan(ew_angle)) - FLOOR_RECEDE	
	local sl = ((h - BASE_HEIGHT) / math.tan(ns_angle)) - FLOOR_RECEDE

	-- Calc the x,z coords (in Top object space) of the two points on the tile that we
	-- want to end up at exactly BASE_HEIGHT after tipping the floor tile about its
	-- corner that coincides with the Tops axis origin.

	local x1 = (Top.Size.x/2 + el) * xq
	local z1 = (Top.Size.z/2) * zq

	local x2 = (Top.Size.x/2) * xq
	local z2 = (Top.Size.z/2 + sl) * zq

	-- CFrame of the Top translated to that corner of floor
	local cf = Top.CFrame + Vector3.new(0, -h+BASE_HEIGHT+FLOOR_THICKNESS, 0)

	-- Calculate the two tipping angles wrt that corner
	local a,b = calcAngles(x1,z1,x2,z2, -FLOOR_THICKNESS)

	-- In the negative x quadrants invert the b angle
	if xq &lt; 0 then
		b = -b
	end

	local floor = Instance.new(&quot;Part&quot;)
	floor.Name = &quot;FloorTile&quot;
	floor.formFactor = 0
	floor.Anchored = true
	floor.BrickColor = COLOR_FLOOR
	floor.Material = Enum.Material.Slate
	floor.Size = Vector3.new(math.abs(x1), FLOOR_THICKNESS, math.abs(z2))

	--print(&quot;Top.Size &quot;, Top.Size, &quot; el &quot;, el, &quot; sl &quot;, sl, &quot; floor.Size &quot;, floor.Size)

	-- Calculate where the y pos of center of part would end up wrt above corner (use the actual size of
	-- floor, which will be rounded in some way to an integer).
	-- (Actually we calc where center of top is and position part to there.  Seems like it should be were center
	-- of part it, not center of top.  Never figured out why this works.)

	local xc = (floor.Size.x/2) * xq
	local yc = 0
	local zc = (floor.Size.z/2) * zq

	local ycr = (yc*math.cos(a)*math.cos(-b) - zc*math.sin(a)*math.cos(-b) - xc*math.sin(-b)) 

	--print(&quot;cf &quot;, cf, &quot; xr,yr,zr:  &quot;, x2, &quot; &quot;, yr, &quot; &quot; , zr)

	local tw = cf:vectorToWorldSpace(Vector3.new(xc,ycr,zc))

	floor.CFrame = cf * CFrame.fromAxisAngle(Vector3.new(1,0,0), a) * CFrame.fromAxisAngle(Vector3.new(0,0,1), b) + tw 

	floor.Parent = Hill
end
