local function fireBullet( sourcePos, targetPos )

	local bullet = ProtoBullet:Clone()
	bullet.Name = &quot;Bullet&quot;
	
	local v = (targetPos - sourcePos).unit
	bullet.CFrame =  CFrame.new(sourcePos,  sourcePos + v) * cf90
	bullet.Velocity = Gun50Cal.Velocity	-- Initialize Bolt to be moving with the Turret

	-- Enable any script in the Bullet
	for k,v in pairs(bullet:GetChildren()) do
		if v.className == &quot;Script&quot; then
			v.Disabled = false
		end
	end

	if CntrGunner.Value then
		bullet.Creator.Value = CntrGunner.Value
		bullet.BrickColor = CntrGunner.Value.TeamColor
	else
		bullet.Creator.Value = CntrUser.Value
		bullet.BrickColor = CntrUser.Value.TeamColor	
	end

	bullet.AncestryChanged:connect(onAncestryChanged)

	-- Put the Bullet into the world
	bullet.Parent = game.Workspace

	return bullet
end


---------------------------------------------------------------------------------------------
-- Aim barrel and fire bullet

local function aimAndFireBullet( target )

	local v = target - Gun50Cal.Position
	v = Mount.CFrame:vectorToObjectSpace(v)

	Weld50Cal.C0 = Weld50CalC0 * CFrame.new(Vector3.new(0,0,0), v)

	if intact50Cal() and (outstandingBullets &lt; MAX_BULLETS) then

		outstandingBullets = outstandingBullets + 1	

		local launchPos = Gun50Cal.CFrame:pointToWorldSpace(Vector3.new(0, 3, 0))
		local bolt = fireBullet(launchPos, target)	
	end
end


---------------------------------------------------------------------------------------------
-- Handle changes to the CntrCmd and CntrAim value objects.  We wait for
-- a change to something non-nil in the CntrCmd and (if the command indicates
-- it has accompanying aim info) in the CntrAim.  Note that the cmd and aim info
-- can come in any order.  After processing the command the tool forces both CntrCmd and
-- CntrAim to nil in preparation for next usage (note, of course, this will
-- trigger another Changed event here but this is ignored since we only act on 
-- non-nil values).

local function handleCmdAndAim()

	-- If we have not gotten the command portion yet, do not handle yet.
	if (CntrCmd.Value == nil) or  (CntrCmd.Value == NIL_CMD) then
		return
	end

	--If the command portion indicates there is accompanying aim info and
	-- it has not arrived yet then do not handle.
	if string.match(CntrCmd.Value, &quot;+&quot;) and 
	   ((CntrAim.Value == nil) or (CntrAim.Value == NIL_AIM)) then
		return
	end

	-- Else we have received complete command.

	--print(&quot;received complete command&quot;)

	local cmd = string.sub(CntrCmd.Value, 1,1)	-- Strip out just command char

	-- If the fire bullet command
	if cmd == &quot;c&quot; then
		aimAndFireBullet( CntrAim.Value )

	-- If p then remove the gunner seat if there is one
	elseif cmd == &quot;p&quot; then

		local gs = Turret.Parent:FindFirstChild(&quot;SeatGunner&quot;)
		if gs then
			gs:Remove()
		end

	-- Else check for a fire shell command
	else
		local boltCode 

		if (cmd == &quot;b&quot;) then
			boltCode = BOLTG_CODE_30

		elseif (cmd == &quot;g&quot;)  then
			boltCode = BOLTG_CODE_45

		elseif (cmd == &quot;t&quot;) then
			boltCode = BOLTG_CODE_70

		end

		if boltCode then
			launchShell(CntrAim.Value, boltCode)
		end
	end

end

-----------------------------------------------------------------------------------------------------------

-- Give barrel an initial upward tilt

adjustBarrel(math.rad(20), math.rad(15))


--CntrCmd.Changed:connect(function() print(&quot;CntrCmd received: &quot;, CntrCmd.Value); handleCmdAndAim() end)
--CntrAim.Changed:connect(function() print(&quot;CntrAim received: &quot;, CntrAim.Value); handleCmdAndAim() end)
CntrCmd.Changed:connect(handleCmdAndAim)
CntrAim.Changed:connect(handleCmdAndAim)


-- Monitor for ourselves to stop being operational 

while true do

	wait(5)

	if not intact() then
		Turret:Remove()
		return
	end

end
		
