						<string name="Name">ScriptHowitzer-v2.4</string>
						<string name="Source">-----------------------------------------------------------------------------------------------------------
--2009/5/3 Initial version based on AsdTurrentScript-v3.
--2009/5/5 Rework
--2009/5/5 Make weld parent game.Workspace
--2009/5/6 Do not seem to need to force refresh to client so removed it.
--2009/5/7 Add arcing trajectory fire, status indicator
--2009/5/10 Assume tool will flush nil values to CntrCmd and CntrAim
--2009/5/20 Support CntrParent
--2009/5/27 Make traj angles to 30,45,70
--2009/5/28 Switched high and low traj commands
--2009/6/6 Make Bolts the color of the Player
--2009/6/16 Increase gravity, range, and power
--2009/7/19 Launch BoltG farther away from barrel to allow building a shield in front of Howitzer
--		    Change FormFactor to formFactor
--2009/7/20 Use LibBolt
--2009/7/24 Go back to original launch point since now we ignore Slabs in BoltG to allow shields
--2009/8/23 Remove ourselves if we are too damaged to function
--2009/9/5 Use bundled ProtoBullet, ProtoShell, and ProtoMissile
--2009/9/10 Newly gened Howitzer displays is not ready for firing, adjust source pos for shell and missile

-- Status indicator:

	-- base color: not selected by user

	-- Green: ready to fire
	-- White: reloading
	-- Red: reloading, previous launch was out of range



local Howitzer = script.Parent

local BarrelAssm = Howitzer.MovableBarrelAssm
local Chassis = Howitzer.Chassis
local GlobalScripts = game.Workspace.GlobalScripts


local Mount = BarrelAssm.Mount
local Barrel = BarrelAssm.Barrel

local Hitch = Howitzer.Hitch

local CntrUser = Howitzer.CntrUser
local CntrParent = Howitzer.CntrParent

local CntrCmd = Instance.new(&quot;StringValue&quot;)	-- to receive commands from Tool
CntrCmd.Name = &quot;CntrCmd&quot;
CntrCmd.Parent = Howitzer

local CntrAim = Instance.new(&quot;Vector3Value&quot;)	-- to receive aiming vector from Tool
CntrAim.Name = &quot;CntrAim&quot;
CntrAim.Parent = Howitzer

-- Values that represent nil for the CntrCmd and CntrAim values (because
-- Lua nil is not allowed to be assigned).
local NIL_CMD = &quot;-&quot;
local NIL_AIM = Vector3.new(0,0,0)

-- Bolt parameter sets used for ASV shells

local BOLTG_CODE_30	= 8103	-- Howitzer shell (30 degree launch angle)
local BOLTG_CODE_45	= 8203	-- Howitzer shell (45 degree launch angle)
local BOLTG_CODE_70	= 8303	-- Howitzershell (70 degree launch angle)

local POST_GEN_DELAY = 15	-- Amount of time that must elapsed after a Howitzer is 
						-- gened before it may be used.

local ProtoMissile = game.Workspace.ProtoMissile
local ProtoShell = game.Workspace.ProtoShell

-- Insure all joints in BarrelAssm are made.
BarrelAssm:MakeJoints()

--Howitzer:MakeJoints()

-- Weld the left and right armor plates to Mount
local Lweld = Instance.new(&quot;Weld&quot;) 
local cf = Mount.CFrame:toObjectSpace(BarrelAssm.WeldArmorLeft.CFrame) 
Lweld.Name = &quot;Weld&quot;
Lweld.Parent = game.Workspace
Lweld.Part0 = Mount --&gt; part0 has to be the parent 
Lweld.Part1 = BarrelAssm.WeldArmorLeft
Lweld.C0 = cf

local Rweld = Instance.new(&quot;Weld&quot;) 
cf = Mount.CFrame:toObjectSpace(BarrelAssm.WeldArmorRight.CFrame) 
Rweld.Name = &quot;Weld&quot;
Rweld.Parent = game.Workspace
Rweld.Part0 = Mount --&gt; part0 has to be the parent 
Rweld.Part1 = BarrelAssm.WeldArmorRight
Rweld.C0 = cf


-- Weld the Mount to the Chassis

local Mweld = Instance.new(&quot;Weld&quot;) 
local mcf = Chassis.CFrame:toObjectSpace(Mount.CFrame) 
Mweld.Name = &quot;Weld&quot;
Mweld.Parent = game.Workspace
Mweld.Part0 = Chassis --&gt; part0 has to be the parent 
Mweld.Part1 = Mount 
Mweld.C0 = CFrame.new(0, 1, 1.5)
Mweld.C1 = CFrame.new(0,0,1.5)



-- Howitzer status variables

local hitched = true		-- Still connected to parent Jeep
local attached = nil 	-- Player character if attached
local reloading = false
local alarm = false

-- Howitzer status indicator

local Status = Howitzer.MovableBarrelAssm.Status
local STATUS_BASE_COLOR = Status.BrickColor
local STATUS_READY = BrickColor.new(&quot;Bright green&quot;)
local STATUS_RELOAD = BrickColor.new(&quot;White&quot;)
local STATUS_ALARM = BrickColor.new(&quot;Bright red&quot;)

-- Constants
local RELOAD_TIME = 5

-- Configured in the Howitzers Mount part:
-- Mount.TopParamA: max (positive)  degrees barrel can elevate up
-- Mount.BottomParamA: max (negative)  degrees barrel can elevate down
-- Mount.RightParamA: max (positive) degrees barrel can turn to right
-- Mount.LeftParamA: max (negative) degrees barrel can turn to left 

local MaxUpAngle = math.rad(Mount.TopParamA)
local MaxDownAngle = math.rad(Mount.BottomParamA)
local MaxLeftAngle = math.rad(Mount.LeftParamA)
local MaxRightAngle = math.rad(Mount.RightParamA)


-- If the model above us does not have Jeep in its name then we do not
-- have a Jeep parent
local Jeep = Howitzer.Parent

if string.match(Jeep.Name, &quot;Jeep&quot;) then
	CntrParent.Value = Jeep
else
	Jeep = nil
end



-------------------------------------------------------------------------------------------------
local function intact()

	if Barrel.Parent and Mount.Parent and Mweld.Parent  then
		return true
	else
		return false
	end
end

--------------------------------------------------------------------------------------------------
local function setStatus()

	if Status.Parent == nil then
		return
	end

	if CntrUser.Value == nil then
	
		Status.BrickColor = STATUS_BASE_COLOR

	else
		if reloading then
	
			if alarm then
				Status.BrickColor = STATUS_ALARM
			else
				Status.BrickColor = STATUS_RELOAD
			end
		else
			Status.BrickColor = STATUS_READY
		end
	end
end


-------------------------------------------------------------------------------------------------
local RRWheel = Howitzer.RRWheel
local RLWheel = Howitzer.RLWheel

local function wheelMotion( val )

	RRWheel.BottomParamB = -val
	RLWheel.BottomParamB = val

end
-------------------------------------------------------------------------------------------------
-- This routine is called after a Bolt is fired to manage the timeout
-- until the vehicle can be fired again.  The Bolt that was fired is passed
-- to this routine.  We wait for the RELOAD_TIME, then we check to see if the 
-- specified Bolt is still in the world.  If yes, we conclude the server
-- is lagging.  To prevent adding to server load by having multiple simultaneous Bolts 
-- from this vehicle, we wait for the Bolt to be removed from the world
-- (indicating it finally reached its target) or until a much higher timeout time
-- is reached (to guard against some unknown error).

local function reload( cball )

	wait(RELOAD_TIME)
	
	local waitTime = RELOAD_TIME * 10

	while cball.Parent and (waitTime &gt; 0) do

		wait(1)
		waitTime = waitTime - 1

	end

	if reloading then
		reloading = false
	end
	
	alarm = false

	setStatus()
end


------------------------------------------------------------------------------------------------
local function fire( sourcePos, dirCf )

	local bolt = ProtoMissile:Clone()
	bolt.Name = &quot;Bolt&quot;
	
	bolt.CFrame =  dirCf + sourcePos
	bolt.FrontParamA = 8003	-- ASV missile parameter set

	-- Enable any script in the Bolt
	for k,v in pairs(bolt:GetChildren()) do
		if v.className == &quot;Script&quot; then
			v.Disabled = false
		end
	end

	bolt.Creator.Value = CntrUser.Value
	bolt.BrickColor = CntrUser.Value.TeamColor

	-- Put the Bolt into the world
	bolt.Parent = game.Workspace

	reloading = true

	local co = coroutine.create(reload)
	assert(coroutine.resume(co, bolt))
end
----------------------------------------------------------------------------------------------
-- Adjust barrel to specified direction and elevation relative to Chassis.
-- Check if requested angles are within range and reduce as needed.
-- Return true if barrel could be adjusted as requested and false if
-- at least one specification was outside of range.

local function adjustBarrel(da, ea)

	local ok = true

	if ea &gt; MaxUpAngle then
		ea = MaxUpAngle
		ok = false

	elseif ea &lt; MaxDownAngle then
		ea = MaxDownAngle
		ok = false
	end

	if da &gt; MaxRightAngle then
		da = MaxRightAngle
		ok = false

	elseif da &lt; MaxLeftAngle then
		da = MaxLeftAngle
		ok = false
	end

	local dircf = CFrame.fromAxisAngle(Vector3.new(0,1,0), da)
	local elevcf = CFrame.fromAxisAngle(Vector3.new(-1,0,0), ea)

	--refreshClient()

	--Mweld.C1 = CFrame.new(0, 0, 1.5) * dircf * elevcf

	Mweld.C1 = CFrame.new(0, 0, 1.5) * elevcf * dircf

	return ok

end


---------------------------------------------------------------------------------------------
local function launchBolt( target )

	alarm = false

	--refreshClient()

	-- Source pos near rear center of Chassis
	local sourcePos = Chassis.CFrame:pointToWorldSpace(Vector3.new(0,7,1.5))

	local vec = target - sourcePos

	--print(&quot;got aim&quot;, vec)

	local vru = Chassis.CFrame:vectorToObjectSpace(vec).unit 

	local ea = math.asin(vru.y)
	local da = math.atan2(vru.x,-vru.z)

	if not adjustBarrel(da, ea) then
		alarm = true
	end

	if (not reloading) and intact()then

		-- Launch pos is 5 &quot;above&quot; end of barrel with respect to its new aimed orientation.
		-- This is to insure the missile does not hit the Howitzer itself.

		local launchPos = Barrel.CFrame:pointToWorldSpace(Vector3.new( 0, -4.5,  5))

		-- Fire in direction Mount actually faces.  Thus if we are blasted enough 
		-- that manipulating the Mweld does not really point the barrel where we want we
		-- will fire innacurately.
		fire(launchPos, Mount.CFrame - Mount.Position)		
	end

	setStatus()

	-- Set wheels in (very slow) motion so the barrel aiming will be forced down to Clients
	wheelMotion(.00001)
end


--------------------------------------------------------------------------------------------------
local function computeLaunchSpeed(source, target, angle, gravity)

	local v = target - source
	local mxz = Vector3.new(v.x, 0, v.z).magnitude

	local r = (gravity * mxz * mxz) / ( 2 * math.cos(angle) * math.cos(angle) * ( (mxz * math.tan(angle)) - v.y ) )

	if r &lt; 0 then
		return nil
	end

	return math.sqrt(r)


end


-------------------------------------------------------------------------------------------------
local function fireShell(sourcePos, speed, launchVec, effectiveGrav, boltCode )

	local bolt = ProtoShell:Clone()
	bolt.Name = &quot;Shell&quot;

	bolt.FrontParamA = boltCode		-- ASV Shell parameter set
	bolt.FrontParamB = speed		-- Launch speed
	bolt.CFrame = CFrame.new(sourcePos, sourcePos + launchVec)

	-- Enable any script in the Shell
	for k,v in pairs(bolt:GetChildren()) do
		if v.className == &quot;Script&quot; then
			v.Disabled = false
		end
	end

	bolt.Creator.Value = CntrUser.Value
	bolt.BrickColor = CntrUser.Value.TeamColor

	bolt.Parent = game.Workspace

	reloading = true

	local co = coroutine.create(reload)
	assert(coroutine.resume(co, bolt))

	return bolt
end

---------------------------------------------------------------------------------------------
local function launchShell( target, boltCode )

	alarm = false

	-- Source pos near rear center of Chassis
	local sourcePos = Chassis.CFrame:pointToWorldSpace(Vector3.new(0,5,1.5))

	local angle = _G.glBoltParms[boltCode].parmAngle
	local maxSpeed = _G.glBoltParms[boltCode].parmSpeed
	local effectiveGrav = _G.glBoltParms[boltCode].parmGravity

	local speed = computeLaunchSpeed(sourcePos, target, math.rad(angle), effectiveGrav)

	--print(&quot;launch speed &quot;, speed)

	-- If we are not angled steeply enough to hit target then arbitrarity pick a speed
	if not speed then
		--print(&quot;Cannot hit target with specified elevation&quot;)	
		speed = maxSpeed/2
	end	

	if speed &gt; maxSpeed then
		speed = maxSpeed
	end

	-- v3 is the 3d world space launch vector
	local v = target - Chassis.Position
	local v2 = Vector3.new(v.x, 0, v.z).unit * math.cos(math.rad(angle))
	local v3 = Vector3.new(v2.x, math.sin(math.rad(angle)), v2.z)

	-- Direction in Chassis object space of launch vector
	local v3ou = Chassis.CFrame:vectorToObjectSpace(v3).unit 

	-- Direction angle in Chassis object space
	local da = math.atan2(v3ou.x,-v3ou.z)

	-- Elev angle in Chassis object space
	local ea = math.asin(v3ou.y)

	if not adjustBarrel(da, ea) then
		alarm = true
	end

	-- Use the actual vector Mount is facing as the launch vector
	-- since the computed vector might be beyond its limits.

	local vec = Mount.CFrame.lookVector

	if (not reloading) and intact() then

		-- Launch pos is 5 &quot;above&quot; end of barrel with respect to its new aimed orientation.
		-- This is to insure the missile does not hit the Howitzer itself.
		local launchPos = Barrel.CFrame:pointToWorldSpace(Vector3.new( 0, -4.5,  5))

		local shell = fireShell(launchPos, speed, vec, effectiveGrav, boltCode)
	end

	setStatus()

	-- Set wheels in (very slow) motion so the barrel aiming will be forced down to Clients
	wheelMotion(.00001)
end

---------------------------------------------------------------------------------------------
-- Handle changes to the CntrCmd and CntrAim value objects.  We wait for
-- a change to something non-nil in the CntrCmd and (if the command indicates
-- it has accompanying aim info) in the CntrAim.  Note that the cmd and aim info
-- can come in any order.  After processing the command the tool forces both CntrCmd and
-- CntrAim to nil in preparation for next usage (note, of course, this will
-- trigger another Changed event here but this is ignored since we only act on 
-- non-nil values).

function handleCmdAndAim()

	-- If we have not gotten the command portion yet, do not handle yet.
	if (CntrCmd.Value == nil) or  (CntrCmd.Value == NIL_CMD) then
		return
	end

	--If the command portion indicates there is accompanying aim info and
	-- it has not arrived yet then do not handle.
	if string.match(CntrCmd.Value, &quot;+&quot;) and 
	   ((CntrAim.Value == nil) or (CntrAim.Value == NIL_AIM)) then
		return
	end

	-- Else we have received complete command.

	--print(&quot;received complete command&quot;)

	local cmd = string.sub(CntrCmd.Value, 1,1)	-- Strip out just command char

	if cmd == &quot;f&quot; then
		launchBolt(CntrAim.Value)

	else
		local boltCode 

		if (cmd == &quot;b&quot;) then
			boltCode = BOLTG_CODE_30

		elseif (cmd == &quot;g&quot;)  then
			boltCode = BOLTG_CODE_45

		elseif (cmd == &quot;t&quot;) then
			boltCode = BOLTG_CODE_70

		end

		if boltCode then
			launchShell(CntrAim.Value, boltCode)
		end
	end

end


-----------------------------------------------------------------------------------------------------------
-- It has been determined we are no longer physically hitched to the parent Jeep.
-- At this time we move ourselves to game.Workspace to act as an independent unit.
-- Since we poll for unhitched status, there is a window between the time the 
-- Jeep unhitches and we detect it.  The Jeep could leave the game during this time.
-- However, we hold a reference to the Jeep established at init time that should 
-- continue to be valid.

local function unHitch()

	hitched = false

	Howitzer.Parent = game.Workspace

	local cntrOwner = Howitzer:FindFirstChild(&quot;CntrOwner&quot;)

	local jeepCntrOwner = Jeep:FindFirstChild(&quot;CntrOwner&quot;)

	-- Inherit the current Jeeps owner
	if cntrOwner and jeepCntrOwner then
		cntrOwner.Value = jeepCntrOwner.Value
		Howitzer.CntrOwnerSeq.Value = Jeep.CntrOwnerSeq.Value
	end
end

-----------------------------------------------------------------------------------------------------------
-- These routines monitor for when we are no longer hitched to Jeep.  Several checks are involved in this:

--	- If the parent Jeep is no longer in the world then we are unhitched.

--	- If Jeep.HitchBar is no longer in the world then we are unhitched

-- 	- If the Hitch part is no longer in the world then we are unhitched.

--	- If the Hitch part is no longer the same distance from the parent Jeep Chassis (compared to
--	  gen time) then we are unhitched
--

local origHitchDistance = 10000

if Jeep then
	origHitchDistance = (Jeep.Chassis.Position - Hitch.Position).magnitude

-- Else no Jeep part, we were gened outside of a parent Jeep.
else
	hitched = false
end

local function stillHitched()

	if not hitched then
		return false
	end

	if Hitch.Parent == nil then
		unHitch()
		return false
	end

	if (Jeep == nil) or (Jeep.Parent == nil) then
		unHitch()
		return false
	end

	if Jeep:FindFirstChild(&quot;HitchBar&quot;) == nil then
		unHitch()
		return false
	end

	local hd = (Jeep.Chassis.Position - Hitch.Position).magnitude 

	if math.abs(origHitchDistance - hd) &gt; 1 then
		unHitch()
		return false
	end

	return true


end
-----------------------------------------------------------------------------------------------------------

CntrUser.Changed:connect(setStatus)

--CntrCmd.Changed:connect(function() print(&quot;CntrCmd received: &quot;, CntrCmd.Value); handleCmdAndAim() end)
--CntrAim.Changed:connect(function() print(&quot;CntrAim received: &quot;, CntrAim.Value); handleCmdAndAim() end)
CntrCmd.Changed:connect(handleCmdAndAim)
CntrAim.Changed:connect(handleCmdAndAim)


if stillHitched() then

	-- Monitor to see when we are unhitchedfrom parent Jeep.

	while stillHitched() do
		wait(5)
	end

-- Else we must be gened outside of a Jeep.  Mark us as reloading during this time.
else

	reloading = true
	setStatus()

	wait(POST_GEN_DELAY)

	reloading = false
	setStatus()

end

-- Loop until we are no long intact

while intact() do

	wait(5)

	-- Periodically turn off wheel motion so unused Howitzer will not
	-- overly burden physics engine
	wheelMotion(0)

end

Howitzer:Remove()


