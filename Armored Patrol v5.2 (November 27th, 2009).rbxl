local function cornerOpening(hillObjs, ho)

	local cf = ho.top.CFrame

	-- Get all 8 corners of ho in world object space

	local nnwa = cf:pointToWorldSpace(Vector3.new(-ho.top.Size.x/2, 0, -ho.n.overlapExtent))
	local nnea = cf:pointToWorldSpace(Vector3.new(ho.top.Size.x/2, 0, -ho.n.overlapExtent))

	local enea = cf:pointToWorldSpace(Vector3.new(ho.e.overlapExtent, 0, -ho.top.Size.z/2))
	local esea = cf:pointToWorldSpace(Vector3.new(ho.e.overlapExtent, 0, ho.top.Size.z/2))

	--print(&quot;overlap check for &quot;, ho.top.Position)

	local sswa = cf:pointToWorldSpace(Vector3.new(-ho.top.Size.x/2, 0, ho.s.overlapExtent))
	local ssea = cf:pointToWorldSpace(Vector3.new(ho.top.Size.x/2, 0, ho.s.overlapExtent))

	--print(&quot;esea &quot;, esea, &quot; ssea &quot;, ssea)

	local wswa = cf:pointToWorldSpace(Vector3.new(-ho.w.overlapExtent, 0, ho.top.Size.z/2))
	local wnwa = cf:pointToWorldSpace(Vector3.new(-ho.w.overlapExtent, 0, -ho.top.Size.z/2))

	for h in pairs(hillObjs) do

		if (h ~= ho) and (h.height &gt; MIN_HEIGHT_FOR_INTERIOR) then

			--print(&quot;---Check against &quot;, h.top.Position)

			-- Map the 8 corners of ho into h object space
			local nnw = h.top.CFrame:pointToObjectSpace(nnwa)
			local nne= h.top.CFrame:pointToObjectSpace(nnea)
			local ene= h.top.CFrame:pointToObjectSpace(enea)
			local ese= h.top.CFrame:pointToObjectSpace(esea)
			local ssw= h.top.CFrame:pointToObjectSpace(sswa)
			local sse= h.top.CFrame:pointToObjectSpace(ssea)
			local wsw= h.top.CFrame:pointToObjectSpace(wswa)
			local wnw= h.top.CFrame:pointToObjectSpace(wnwa)

			--print(&quot;---h object coord ese &quot;, ese, &quot; sse &quot;, sse)

			-- The 4 overlap extents for h.  We check against the total overlap
			-- extent rectangle of h (though a more accurate check would be to
			-- check against the 4 overlap extents flaps of the side-inclines plus the top).
			local zn = -h.n.overlapExtent
			local zp = h.s.overlapExtent
			local xn = -h.w.overlapExtent
			local xp = h.e.overlapExtent

			local overlap = false

			--print(&quot;--- h extent xn/xp  zn/zp &quot;, xn, &quot;/&quot;, xp, &quot; &quot;, zn, &quot;/&quot;, zp)

			-- Check if ho nnw corner overlaps h
			if (nnw.x &gt; xn) and (nnw.x &lt; xp) and (nnw.z &gt; zn) and (nnw.z &lt; zp) then

				ho.nnw.cornerOpening = true
				overlap = true
			end

			-- Check if ho nne corner overlaps h
			if (nne.x &gt; xn) and (nne.x &lt; xp) and (nne.z &gt; zn) and (nne.z &lt; zp) then
				ho.nne.cornerOpening = true
				overlap = true
			end

			-- Check if ho ene corner overlaps h
			if (ene.x &gt; xn) and (ene.x &lt; xp) and (ene.z &gt; zn) and (ene.z &lt; zp) then
				ho.ene.cornerOpening = true
				overlap = true
			end

			-- Check if ho ese corner overlaps h
			if (ese.x &gt; xn) and (ese.x &lt; xp) and (ese.z &gt; zn) and (ese.z &lt; zp) then
				ho.ese.cornerOpening = true
				overlap = true
			end

			-- Check if ho ssw corner overlaps h
			if (ssw.x &gt; xn) and (ssw.x &lt; xp) and (ssw.z &gt; zn) and (ssw.z &lt; zp) then
				ho.ssw.cornerOpening = true
				overlap = true
			end

			-- Check if ho sse corner overlaps h
			if (sse.x &gt; xn) and (sse.x &lt; xp) and (sse.z &gt; zn) and (sse.z &lt; zp) then
				ho.sse.cornerOpening = true
				overlap = true
			end

			-- Check if ho wsw corner overlaps h
			if (wsw.x &gt; xn) and (wsw.x &lt; xp) and (wsw.z &gt; zn) and (wsw.z &lt; zp) then
				ho.wsw.cornerOpening = true
				overlap = true
			end

			-- Check if ho wnw corner overlaps h
			if (wnw.x &gt; xn) and (wnw.x &lt; xp) and (wnw.z &gt; zn) and (wnw.z &lt; zp) then
				ho.wnw.cornerOpening = true
				overlap = true
			end

			if overlap then
				popRange(ho, h)
			end
		end
	end
end


