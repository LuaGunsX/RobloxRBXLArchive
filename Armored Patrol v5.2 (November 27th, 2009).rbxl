					<string name="Source">-------------------------------------------------------------------------------------------------------------
--2009/7/13 Initial version
--2009/7/17 Add concept of Anchored only for lowest blocks, welds to adjacents, delete key
--2009/7/17 Make selectable Slab shapes and anchor points
--2009/7/18 Put slab visualizer under camera so only player sees it, use Mouse.Target for delete
--2009/7/19 Do not allow building near bases
--2009/7/21 Add snap to closest Slab grid
--2009/7/21 Change Anchored decision based on what Mouse.Target is
--2009/7/24 Weld only those Slabs touching or adjacent to each other
--2009/7/26 Dont assume Fortifications only includes Slabs
--2009/8/20 Administer slabs from the centralized ScriptSlabManager instead of individual ScriptSlabs
--                  Increase Slab max to 30
--2009/8/25 Experiment with larger slabs, change CameraType to Watch
--2009/8/29 Even larger Slabs because still too hard to build a decent fort
--2009/9/7 CameraType to Track
--2009/11/10 v3.4 Use GUI hint


-- Build a small wall at the position pointed to by cursor


local Tool=script.Parent
local Torso
local Mouse

-- This runs while the script is still in game.Players/&lt;playername&gt;.
local Player = Tool.Parent.Parent

local MAX_TOOL_DISTANCE = 50
local MAX_SLABS = 30

local Shapes = {
	{sx=16,sy=8,sz=2,ox=0,oy=4, d=250},
	{sx=16,sy=8,sz=2,ox=8,oy=4, d=250},
	{sx=8,sy=16,sz=2,ox=0,oy=8, d=250},
	{sx=8,sy=16,sz=2,ox=4,oy=8, d=250},
	{sx=20,sy=4,sz=2,ox=0,oy=2, d=300},
	{sx=20,sy=4,sz=2,ox=10,oy=2, d=300},
	{sx=16,sy=16,sz=1,ox=0,oy=8, d=150},
	{sx=16,sy=16,sz=1,ox=8,oy=8, d=150},
}

local Cs
local CsNum = 1

local equipped = false
local curDir = 0
local curTilt = 0
local curSlabs = 0
local curSlabsNear = 0

local HqRedFlag = game.Workspace:FindFirstChild(&quot;HqRed&quot;) and game.Workspace.HqRed.RedFlag
local HqBlueFlag = game.Workspace:FindFirstChild(&quot;HqBlue&quot;) and game.Workspace.HqBlue.BlueFlag
local NO_BUILD_RADIUS = 300

local Fortifications 

-- Create the CursorSlab which will follow the cursor and be used to visualize Slab placement

local CursorSlab = Instance.new(&quot;Part&quot;)
CursorSlab.formFactor = 0
CursorSlab.Size = Vector3.new(1,1,1)
CursorSlab.Anchored = true
CursorSlab.CanCollide = false
CursorSlab.Transparency = .4

local Mesh = Instance.new(&quot;BlockMesh&quot;)
Mesh.Name = &quot;Mesh&quot;
Mesh.Parent = CursorSlab

local debounce = false


---------------------------------------------------------------------------------------------------------------
local function setCurShape( shapeNum )

	Cs = Shapes[shapeNum]

	Mesh.Scale = Vector3.new(Cs.sx,Cs.sy,Cs.sz)
	Mesh.Offset = Vector3.new(Cs.ox,Cs.oy +1,0)

end


---------------------------------------------------------------------------------------------------------------

local OwnerSeqMaster = game.Workspace.OwnerSeqMaster

local function assignOwnerSeq(CntrOwnerSeq)

	if OwnerSeqMaster.Value &gt; 1000000000 then
		OwnerSeqMaster.Value = 1
	end

	OwnerSeqMaster.Value = OwnerSeqMaster.Value + 1

	if CntrOwnerSeq then
		CntrOwnerSeq.Value = OwnerSeqMaster.Value
	end
end

-------------------------------------------------------------------------------------------------------
local function displayHint()

	_G.displayHint(&quot;Click nearby to build, rotate=r,e, tilt=t,y, shapes=b, delete=u, built &quot;..curSlabs..&quot; (&quot;..curSlabsNear..&quot; nearby) out of &quot;..MAX_SLABS,0)

end

------------------------------------------------------------------------------------------------------------
-- Count the number of slabs currently owned by this player.  If above the max then 
-- indiscriminately remove those above the max (this is a defensive check, should not happen).
-- Return the number of slabs owned and the oldest slab in case the caller wants to remove it.

local function countSlabs()

	local n = 0
	local near = 0
	local oldestSlabSeq
	local oldestSlab

	for k,s in pairs(Fortifications:GetChildren()) do

		if string.match(s.Name, &quot;^Slab&quot;) then

			if s.CntrOwner.Value == Player then

				-- Safety check, should never be more than MAX_SLABS
				if n &gt;= MAX_SLABS then
					s:Remove()

				else
					n = n + 1

					if (oldestSlabSeq == nil) or (oldestSlabSeq &gt; s.CntrOwnerSeq.Value) then
						oldestSlabSeq = s.CntrOwnerSeq.Value
						oldestSlab = s
					end

					if (Torso.Position - s.Position).magnitude &lt; MAX_TOOL_DISTANCE then
						near = near + 1
					end
				end
			end
		end
	end

	return n, near, oldestSlab
end
---------------------------------------------------------------------------------------------------------
-- See if the 8 vertices of p2 all fall on one side of any of the the 6 sides of p1.  
-- Oversize p1 a little so even if p2 is aligned and adjacent to p1 it will count that as not separate.

local function separate(p1,p2)

	local cf = p1.CFrame
	local s1h = p1.Size:lerp(Vector3.new(0,0,0),.49)
	local s2h = p2.Size:lerp(Vector3.new(0,0,0),.5)

	local cf2 = cf:toObjectSpace(p2.CFrame)

	local p1 = cf2:pointToWorldSpace(Vector3.new(s2h.x,s2h.y,s2h.z))
	local p2 = cf2:pointToWorldSpace(Vector3.new(s2h.x,s2h.y,-s2h.z))
	local p3 = cf2:pointToWorldSpace(Vector3.new(s2h.x,-s2h.y,s2h.z))
	local p4 = cf2:pointToWorldSpace(Vector3.new(s2h.x,-s2h.y,-s2h.z))
	local p5 = cf2:pointToWorldSpace(Vector3.new(-s2h.x,s2h.y,s2h.z))
	local p6 = cf2:pointToWorldSpace(Vector3.new(-s2h.x,s2h.y,-s2h.z))
	local p7 = cf2:pointToWorldSpace(Vector3.new(-s2h.x,-s2h.y,s2h.z))
	local p8 = cf2:pointToWorldSpace(Vector3.new(-s2h.x,-s2h.y,-s2h.z))


	local function beyondp( sh, d )

		--print(string.format(&quot;d:%s, sh: %.2f px: %.2f,%.2f,%.2f,%.2f,%.2f,%.2f,%.2f,%.2f&quot;,
		--	d, sh, p1[d],p2[d],p3[d],p4[d],p5[d],p6[d],p7[d],p8[d]))

		if p1[d] &lt; sh then return false end
		if p2[d] &lt; sh then return false end
		if p3[d] &lt; sh then return false end
		if p4[d] &lt; sh then return false end
		if p5[d] &lt; sh then return false end
		if p6[d] &lt; sh then return false end
		if p7[d] &lt; sh then return false end
		if p8[d] &lt; sh then return false end

		--print(&quot;true&quot;)
		return true
	end

	local function beyondm( sh, d )

		--print(string.format(&quot;d:%s, sh: %.2f px: %.2f,%.2f,%.2f,%.2f,%.2f,%.2f,%.2f,%.2f&quot;,
		--	d, sh, p1[d],p2[d],p3[d],p4[d],p5[d],p6[d],p7[d],p8[d]))

		if p1[d] &gt; sh then return false end
		if p2[d] &gt; sh then return false end
		if p3[d] &gt; sh then return false end
		if p4[d] &gt; sh then return false end
		if p5[d] &gt; sh then return false end
		if p6[d] &gt; sh then return false end
		if p7[d] &gt; sh then return false end
		if p8[d] &gt; sh then return false end

		--print(&quot;true&quot;)
		return true
	end

	if beyondp(s1h.x, &quot;x&quot;) or beyondm(-s1h.x, &quot;x&quot;) or
		beyondp(s1h.y, &quot;y&quot;) or beyondm(-s1h.y, &quot;y&quot;) or
		beyondp(s1h.z, &quot;z&quot;) or beyondm(-s1h.z, &quot;z&quot;) then

		return true
        end

	return false
end

---------------------------------------------------------------------------
-- Determine if p1 intersects with p2.  Care is taken such that
-- aligned, adjacent parts are counted as intersecting.  Use separation by axis but only
-- do for the 6 side.  This results in a small chance of false positives.

local function intersect(p1,p2)

	if (not separate(p1,p2)) and (not separate(p2,p1)) then
		return true

	else
		return false
	end
end


------------------------------------------------------------------------------------------------------------
-- Go thru all other Slabs in Fortifications and weld our Slab to any that intersect or
-- are adjacent.

local function weldAdjacents( slab )

	local under = false
	local sp = slab.Position

	for k,s in pairs(Fortifications:GetChildren()) do

		if string.match(s.Name, &quot;^Slab&quot;) then

			if s ~= slab then

				--print(&quot;Checking for inters&quot;)

				if ((sp - s.Position).magnitude &lt; MAX_TOOL_DISTANCE) and
					intersect(slab, s) then

					--print(&quot;Intersecting&quot;)

					local weld = Instance.new(&quot;Weld&quot;)
					local cf = slab.CFrame:toObjectSpace(s.CFrame)
					weld.Part0 = slab
					weld.Part1 = s
					weld.C0 = cf
					weld.Parent = game.Workspace
				end
			end
		end
	end
end

------------------------------------------------------------------------------------------------------------
-- If the specified object is a slab belonging to the player then delete it.

local function delete( s )

	if string.match(s.Name, &quot;^Slab&quot;) then

		if s.CntrOwner.Value == Player then

			s:Remove()

			curSlabs = curSlabs - 1
			displayHint()
		end
	end
end

-----------------------------------------------------------------------------------------------------------
function toggleCamera()

	if game.Workspace.Camera.CameraType == Enum.CameraType.Custom then
		game.Workspace.Camera.CameraType = Enum.CameraType.Track

	else
		game.Workspace.Camera.CameraType = Enum.CameraType.Custom
	end

	--print(&quot;Camera &quot;, game.Workspace.Camera.CameraType)

end


------------------------------------------------------------------------------------------------------------
local function onButton1Down(mouse)

	if debounce then
		return
	end

	if Torso.Velocity.magnitude &gt; 1 then
		_G.displayHint(&quot;Cannot build while moving&quot;)
		return
	end

	if not CursorSlab.Parent then
		return
	end

	if (HqRedFlag and ((CursorSlab.Position-HqRedFlag.Position).magnitude &lt; NO_BUILD_RADIUS)) or
		(HqBlueFlag and ((CursorSlab.Position-HqBlueFlag.Position).magnitude &lt; NO_BUILD_RADIUS)) then

		_G.displayHint(&quot;Cannot build close to either teams Base&quot;)
		return
	end

	local removed
	curSlabs,curSlabsNear,oldest = countSlabs()

	if curSlabs &gt;= MAX_SLABS then	-- It at max then remove the oldest one
		oldest:Remove()
		curSlabs = curSlabs - 1
	end

	curSlabs = curSlabs + 1		-- Account for the Slab we are about to build
	curSlabsNear = curSlabsNear + 1

	local cf = CursorSlab.CFrame

	local slab = Instance.new(&quot;Part&quot;)
	slab.Name = &quot;SlabInProgress&quot;
	slab.formFactor = 0
	slab.Size =Vector3.new(Cs.sx,Cs.sy,Cs.sz)
	slab.CanCollide = false
	slab.BrickColor = Player.TeamColor
	slab.TopSurface = 0
	slab.BottomSurface = 0
	slab.Transparency = 1
	slab.Friction = 1
	slab.FrontParamA = Cs.d		-- defensive strength of Slab
	slab.FrontParamB = Cs.d

	-- Position in world space of where actual part should go
	local npos = cf:pointToWorldSpace(Vector3.new(Cs.ox,Cs.oy + 1,0))

	slab.CFrame = cf + (npos - cf.p)

	local iv = Instance.new(&quot;IntValue&quot;)
	iv.Name = &quot;CntrOwnerSeq&quot;
	assignOwnerSeq(iv)
	iv.Parent = slab

	local o = Instance.new(&quot;ObjectValue&quot;)
	o.Name = &quot;CntrOwner&quot;
	o.Value = Player
	o.Parent = slab

	-- If the cursor is touching another Slab then we assume the Slab we are building
	-- is on top of that one and so is not a &quot;foundation&quot; Slab.  We do not anchor it and instead
	-- allow the welds to its adjacents to keep it up. 
	-- 
	if string.match(Mouse.Target.Name, &quot;^Slab&quot;) then
		slab.Anchored = false
	else
		slab.Anchored = true
	end

	-- As soon as we add the Slab to Fortifications, then the SlabManager script will
	-- manage it.
	slab.Parent = Fortifications

	-- Weld to nearby slabs
	weldAdjacents(slab)

	displayHint()

	debounce = true

	wait(.6)

	debounce = false

end

------------------------------------------------------------------------------------------------------------
local function onButton1Up(mouse)
	-- print(&quot;up: &quot;, mouse)

end

------------------------------------------------------------------------------------------------------------
local function onKeyDown(key)

	if key == &quot;r&quot; then
		curDir = curDir + 45

	elseif key == &quot;e&quot; then
		curDir = curDir + 5

	elseif key == &quot;t&quot; then
		curTilt = curTilt + 45

	elseif key == &quot;y&quot; then
		curTilt = curTilt + 5

	elseif key == &quot;b&quot; then

		if CsNum &lt; #Shapes then
			CsNum = CsNum + 1
		else
			CsNum = 1
		end

		setCurShape(CsNum)
	
	elseif key == &quot;u&quot; then

		local t = delete(Mouse.Target)

	elseif key == &quot;v&quot; then
		toggleCamera()
	end
end

-------------------------------------------------------------------------------------------------------
-- Snap the specified CFrame to the grid relative to the closest of close existing Slabs 
-- whose CFrame is orthogonal to ours.  Return the snapped to CFrame if a close 
-- orthogonal Slab was found or the original CFrame if not.

local function snapToGrid( tcf )

	local cs
	local cd = 30	-- Ignore any Slab farther away than this

	for k,s in pairs(Fortifications:GetChildren()) do

		if string.match(s.Name, &quot;^Slab&quot;) then
        
			local d = (s.Position-tcf.p).magnitude

			if d &lt; cd then
		    
				local rlv = s.CFrame:toObjectSpace(tcf).lookVector
		    
				-- If we are oriented orthogonally to this slab
				if math.abs(rlv.x) &gt; .999 or
					math.abs(rlv.y) &gt; .999 or
					math.abs(rlv.z) &gt; .999 then

					cs = s
					cd = d
				end
			end
		end
	end

	-- If we found a close slab that we are oriented to then
	-- snap to the closest position in its grid.
	if cs then
		local rpos = cs.CFrame:pointToObjectSpace(tcf.p)

		local x = math.floor(rpos.x + .5)
		local y = math.floor(rpos.y + .5)
		local z = math.floor(rpos.z + .5)

		local pos = cs.CFrame:pointToWorldSpace(Vector3.new(x,y,z))
        
		return tcf + (pos - tcf.p)
	else
		return tcf
	end

end


--------------------------------------------------------------------------------------------------------
local function animate()

	while (Torso) do

		local pos = Mouse.Hit.p

		if (Torso.Position-pos).magnitude &lt; MAX_TOOL_DISTANCE then
			-- The CursorSlab is put under the CurrentCamera in order to make it
			-- a local brick.  This is only visible to the local Player and not other Players.
			-- Presumably this makes it low network overhead and so we can have 
			-- a pretty fast animation interval for it.
			CursorSlab.Parent = game.Workspace.CurrentCamera 
		else
			CursorSlab.Parent = nil
		end

		if CursorSlab.Parent then

			-- Origin based CFrame rotated and tilted per the current angles
			local cf = CFrame.fromAxisAngle(Vector3.new(0,1,0), math.rad(curDir)) *
				CFrame.fromAxisAngle(Vector3.new(1,0,0), math.rad(curTilt))

			-- Relative offset of the actual part
			local pr = cf:pointToWorldSpace(Vector3.new(0,-1, 0))

			-- Rotate the actual part then translate to mouse pos  plus the offset above
			CursorSlab.CFrame =  snapToGrid(cf + pos + pr)
		end

		wait(.1)
	end
			
	CursorSlab.Parent = nil

end


-------------------------------------------------------------------------------------------------------
local function onEquipped(mouse)

	Torso = script.Parent.Parent.Torso

	if Torso.Position.y &gt; 300 then
		return
	end

	game.Workspace.Camera.CameraType = Enum.CameraType.Track

	Mouse = mouse

        mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;

	Fortifications = game.Workspace.Fortifications

	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
        mouse.Button1Up:connect(function() onButton1Up(mouse) end)

	mouse.KeyDown:connect(onKeyDown)

	-- Refresh the curSlabs to the current number of slabs we own
	curSlabs,curSlabsNear = countSlabs()

	displayHint()

	debounce = false

	-- Let this thread animate the slab cursor until onUnequipped nils out
	-- Torso.  We do this here so if there is an error we do not kill off the 
	-- main thread and permanently break the tool.
	animate()
end

-------------------------------------------------------------------------------------------------------
local function onUnequipped()

	Torso = nil
	_G.clearHint()
	Fortifications = nil

	game.Workspace.Camera.CameraType = Enum.CameraType.Custom

end
---------------------------------------------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)

setCurShape(CsNum)

while true do

	wait(10)

	if Torso then

		curSlabs,curSlabsNear = countSlabs()

		displayHint()
	end
end



