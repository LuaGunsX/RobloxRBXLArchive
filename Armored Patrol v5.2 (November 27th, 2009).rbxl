--2009/9/5 Initial version
--2009/11/10 v4.1 Use GUI hint


local Tool = script.Parent

local Player = script.Parent.Parent.Parent
local Handle
local equipped = false
local ProtoBolt

local Torso 
local Mouse

local vip = false
local dispLine = 0

local RELOAD_TIME = 5



------------------------------------------------------------------------------------------------

local function fire( sourcePos, targetPos )

	local bolt = ProtoBolt:Clone()
	bolt.Name = &quot;Bolt&quot;
	
	local v = (targetPos - sourcePos).unit
	bolt.CFrame =  CFrame.new(sourcePos,  sourcePos + v)
	bolt.Velocity = Vector3.new(0, 0, 0)
	bolt.BrickColor = Player.TeamColor

	-- Put parameter code based on whether this player had extended missiles
	local ExtRpgCount = Player:FindFirstChild(&quot;ExtRpgCount&quot;)

	if ExtRpgCount and (ExtRpgCount.Value) &gt; 0 then

		ExtRpgCount.Value = ExtRpgCount.Value - 1
		
		bolt.FrontParamA = 8001	-- Like Tank
	else
		bolt.FrontParamA = 8000	-- Default (RPG) parameter set
	end

	-- Enable any script in the Bolt
	for k,v in pairs(bolt:GetChildren()) do
		if v.className == &quot;Script&quot; then
			v.Disabled = false
		end
	end

	bolt.Creator.Value = Player

	-- Put the Bolt into the world
	bolt.Parent = game.Workspace

	if ExtRpgCount then

		if ExtRpgCount.Value == 0 then
			ExtRpgCount.Parent = nil
		end
	end

	return bolt
end

------------------------------------------------------------------------------------------------
local function displayBoltStatus( )

	-- If tool not currently selected then do not change the hint area

	if not equipped then
		return
	end

	local ExtRpgCount = Player:FindFirstChild(&quot;ExtRpgCount&quot;)

	if ExtRpgCount and (ExtRpgCount.Value) &gt; 0 then
		_G.displayHint(&quot;(Extended range missiles left: &quot;.. ExtRpgCount.Value..&quot;)&quot;,0)

	else
		_G.clearHint()
	end
end

---------------------------------------------------------------------------------------------------------------
-- If not reloading then fire a Bolt.  Mark reloading as true while waiting reload time.

local reloading = false	

function onButton1Down()

	if equipped and (not reloading) then

		local source = script.Parent.Handle.Position + Vector3.new(0,3,0)
		local bolt = fire(source, Mouse.Hit.p)

		displayBoltStatus()

		reloading = true

		wait(RELOAD_TIME)

		for i = 1,20 do
			if bolt.Parent == nil then
				break
			end
			wait(1)
		end

		reloading = false
	end
end

------------------------------------------------------------------------------------------------------------
-- Display the next line in the global CaptureArray

local function dispCapture()

	local ca = game.Workspace.CntrDebugText

	if not ca then
	
		Message.Text = &quot;No capture buffer string value found&quot;
		Message.Parent = Player
		return
	end

	local s = ca.Value
	local i = 1

	for t in string.gmatch(s, &quot;([^#]*#)&quot;) do

		if i == dispLine then
			Message.Text = t
			Message.Parent = Player

			dispLine = dispLine + 1
			return
		end

		i = i + 1
	end

	Message.Text = &quot;No more in capture buffer, resetting to start&quot;
	Message.Parent = Player

	dispLine = 1
end

---------------------------------------------------------------------------------------------------------------------------
function onKeyDown(key)

	if not vip then	-- Should not be possible, key events are only enabled for vip
		return
	end

	if key == &quot;m&quot; then
		local oldDebug = game.Workspace.CntrDebug.Value
		local newDebug

		if oldDebug ~= 0 then
			newDebug = 0
		else
			newDebug = 1
		end

		game.Workspace.CntrDebug.Value = newDebug

		_G.displayHint(&quot;Debug changed from &quot;..oldDebug..&quot; to &quot;..newDebug)

	elseif key == &quot;n&quot; then

		if game.Workspace.CntrDebug.Value == 0 then
			Message.Text = &quot;Debug disabled&quot;
			Message.Parent = Player
		else
			dispCapture()
		end
	end
end


--------------------------------------------------------------------------------------------------
function onEquippedLocal(mouse)

	if mouse == nil then
		print(&quot;Mouse not found&quot;)
		return 
	end

	Torso = script.Parent.Parent.Torso

	if Torso.Position.y &gt; 300 then
		return
	end

	equipped = true

	Handle = script.Parent.Handle
	Handle.BrickColor =  Player.TeamColor

	ProtoBolt = game.Workspace.ProtoMissile

	Mouse = mouse

	mouse.Button1Down:connect(onButton1Down)

	-- Check for VIP players

	vip = false

	for k,v in pairs(game.Workspace.VIPs:GetChildren()) do
	
		if Player.Name == v.Name then
			vip = true
			dispLine = 1
			mouse.KeyDown:connect(onKeyDown)
			break
		end
	end

	Mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;

	displayBoltStatus()
end

----------------------------------------------------------------------------------------
function onUnequippedLocal()

	_G.clearHint()
	equipped = false

end


-----------------------------------------------------------------------------------------
Tool.Equipped:connect(onEquippedLocal)
Tool.Unequipped:connect(onUnequippedLocal)






