local function genAllHills()

	local hillObjects = {}

	for i=1,MAX_HILLS do
		hillObjects[genOneHillObject()] = true
	end

	-- Generate edge ridges.  We do this before generating
	-- side inclines because where ever we put a ridge we 
	-- mark that side so it will not generate a side angle plus we
	-- mark it to have a steep angle so floor generation is controlled.

	for ho in pairs(hillObjects) do
		placeRidges(ho)
	end

	-- Check for corner openings, also populate the ranges
	-- table which groups those hills that overlapping each other into 
	-- the same range.
	for ho in pairs(hillObjects) do
		if ho.height &gt; MIN_HEIGHT_FOR_INTERIOR then
			cornerOpening(hillObjects, ho)
		end
	end

	--[
	-- Check for joint openings
	for ho in pairs(hillObjects) do
		if ho.height &gt; MIN_HEIGHT_FOR_INTERIOR then
			jointOpening(ho)
		end
	end
	--]]

	-- Calculate sizing parameters for the corner-inclines
	for ho in pairs(hillObjects) do
		calcCornerAttr(ho)
	end

	-- Make hills
	for ho in pairs(hillObjects) do
		makeHill(ho)
	end

--[
	-- Place caches
	for ho in pairs(hillObjects) do
		if ho.height &gt; MIN_HEIGHT_FOR_INTERIOR then
			placeCache(ho)
		end
	end



	-- Create and activate a type of capture-the-flag mechanisms for the ranges
	-- collected above.

	createCaptureTheFlag( hillObjects )
--]]

	-- Empty hillObjects table to make sure we do not keep a ref to anything
	for ho in pairs(hillObjects) do
		ho.hill = nil
		ho.top = nil
		hillObjects[ho] = nil
	end
end


