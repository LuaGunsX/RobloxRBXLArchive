-- Generate random integer number with skewed normal distribution.
-- Means is about 20.  Lowest value is 15, highest is 90.

local toggle = true
local x1,x2

local function randNorm()

    local x

    if toggle then
        x1 =  math.random()
        x2 =  math.random()

        x = math.sqrt( -2 * math.log(x1)) * math.cos( 2 * 3.14159 * x2)

        toggle = false

    else
        x = math.sqrt( -2 * math.log(x1)) * math.sin( 2 * 3.14159 * x2)

        toggle = true
    end
       
    if x &lt; 0 then
        x = x * 3
    else
        x = x * 15
    end

    local m = math.floor( x + 22.5)

    if m &lt; 15 then
        m = 15
    elseif m &gt; 90 then
        m = 90
    end

    return m
end




--[[--------------------------------------------------------------------------------------
	Make one incline relative to base.  
        
	base - part to build incline relative to
	fcf - rotation CFrame to pick side to build ramp off of (rel to base)
	width - width of side of ramp touching base
	wShift - amount to shift incline in the x axis rel to base (pos to right, neg to left)
	dist - distance from center of base to start ramp
	len - length of side of ramp extending from base
	angle = angle ramp make with base
--]]
function makeIncline(Hill, base,fcf,width,wShift,dist,len,angle, name)

	--print(&quot;makeIncline base &quot;,base, &quot; fcf &quot;,fcf,&quot; width &quot;,width,&quot; dist &quot;, dist,&quot; len &quot;,len,&quot; angle &quot;,angle)

	-- Amount to translate after tilting (in base cframe)
	local dlen = (len/2) * math.cos(angle)
	local dht = (len/2) * math.sin(angle)

	local incline = Instance.new(&quot;Part&quot;)
	incline.Name = name
	incline.formFactor = 2
	incline.Size = Vector3.new(width, THICKNESS_SIDE, len)
	incline.FrontSurface = 2 -- Weld, just to mark it
	incline.Friction = 1
	incline.Elasticity = 0
	incline.TopSurface = 0
--incline.Transparency = .5

	incline.Material = Enum.Material.Slate
	incline.BrickColor = COLOR_SIDE

	local t = Instance.new(&quot;Texture&quot;)
	--t.Texture = &quot;http://www.roblox.com/asset/?id=2060902&quot;
	t.Texture = &quot;http://www.roblox.com/asset/?id=2022921&quot;
	t.Face = 1
	t.StudsPerTileU = 50
	t.StudsPerTileV = 50
	--t.Parent = incline

	-- CFrame of base rotated to the direction of this incline
	local rcf = base.CFrame * fcf

	-- Rotated and tilted CFrame of the incline, centered on base. 
	local cf = rcf * CFrame.fromAxisAngle(Vector3.new(1,0,0), -angle)

	-- Offset vector from center of base to center of where incline should go.
	local v = rcf:vectorToWorldSpace(Vector3.new(wShift,-dht, -(dist+dlen)))

	cf = cf + v

	incline.CFrame = cf

	local t = Instance.new(&quot;Texture&quot;)
	--t.Texture = &quot;http://www.roblox.com/asset/?id=2060902&quot;
	t.Texture = &quot;http://www.roblox.com/asset/?id=2022921&quot;
	t.Face = 1
	t.StudsPerTileU = 50
	t.StudsPerTileV = 50
	--t.Parent = incline

	incline.Parent = Hill
	incline.Anchored = true

	return incline
end



