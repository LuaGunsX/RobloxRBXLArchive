--2009/11/5 Initial version

-- Provides global functions for local scripts.

-- Make output justified to the left

local ScreenGui = script.Parent

local Hint

local hintOffsetX, hintOffsetY, hintHeight

-- Map each character to the length in MM of a line with 100 of such characters
local letLen = {a=150, b=150,c=136,d=150,e=150,f=73,g=150,h=150,i=54,j=63,k=135,l=54,m=227,
			n=150,o=150,p=150,q=150,r=87,s=135,t=73,u=150,v=140,w=181,x=130,y=130,z=130,
			A=180,B=180,C=195,D=195,E=180,F=165,G=208,H=194,I=73,J=136,K=180,L=150,M=218,
			N=194,O=209,P=180,Q=209,R=194,S=180,T=160,U=194,V=180,W=262,X=170,Y=170,Z=165,
			[&quot;0&quot;] = 150, [&quot;1&quot;] = 150, [&quot;2&quot;] = 150, [&quot;3&quot;] = 150, [&quot;4&quot;] = 150, 
			[&quot;5&quot;] = 150, [&quot;6&quot;] = 150, [&quot;7&quot;] = 150, [&quot;8&quot;] = 150, [&quot;9&quot;] = 150, 
			[&quot;.&quot;]=73, [&quot;,&quot;]=73, [&quot;:&quot;]=73,[&quot; &quot;]=73,[&quot;(&quot;]=87,[&quot;)&quot;]=87}

-- Factor to multiple the value in letLen to get actual pixel length
local LSCALE = 500 / (100 * 129)

local hintDuration = 0

----------------------------------------------------------------------------------------------------------
local function vwait()

	while hintDuration &gt; 0 do
		wait(1)
		hintDuration = hintDuration - 1
	end

	hintDuration = 0
	Hint.Visible = false
end


----------------------------------------------------------------------------------------------------------
local function displayHint(line, duration)

	if not Hint then
		return
	end

	local totPixels = Hint.Size.X.Offset

	local pixels = 0

	-- Count up length line in scaled value.
	for i =1,#line do

		local c = string.sub(line, i, i)
		local n = letLen[c]

		if not n then
			n = 150	-- Default value in case char not in our table.
		end

		pixels = pixels + n
	end

	-- Derive actual number of pixels in line
	pixels = pixels * LSCALE

	Hint.Size = UDim2.new(0,pixels+10, 0,hintHeight)
	--Hint.Position = UDim2.new(0, hintOffsetX, 0, hintOffsetY)
	Hint.Text = line

	if not duration then
		duration = 5

	elseif duration &lt; 0 then
		duration = 100

	elseif duration == 0 then
		duration = 100000
	end

	hintDuration = duration

	if not Hint.Visible then
		Hint.Visible = true
		local co = coroutine.create(vwait)
		coroutine.resume(co)
	end

end

_G.displayHint = displayHint

----------------------------------------------------------------------------------------------------------
local function clearHint(  )

	Hint.Visible = false

end

_G.clearHint = clearHint


