-- Probe with a plate part so we get the .4 resolution in length

local cf90 = CFrame.fromAxisAngle(Vector3.new(1,0,0), math.rad(90))

local function playerHit(torso, blastPos)


	local len = math.floor((torso.Position - blastPos).magnitude)

	--if len &lt; 2 then
	--	return true
	--end

	local tp,d = cp(blastPos, torso)

	local lp, ld
	local limb = torso.Parent:FindFirstChild(&quot;Left Arm&quot;)

	if limb then
		lp,ld= cp(blastPos, limb)
		if ld &lt; d then tp = lp; d = ld end
	end

	limb = torso.Parent:FindFirstChild(&quot;Right Arm&quot;)

	if limb then
		lp,ld= cp(blastPos, limb)
		if ld &lt; d then tp = lp; d = ld end
	end

	limb = torso.Parent:FindFirstChild(&quot;Head&quot;)

	if limb then
		lp,ld= cp(blastPos, limb)
		if ld &lt; d then tp = lp; d = ld end
	end

	limb = torso.Parent:FindFirstChild(&quot;Right Leg&quot;)

	if limb then
		lp,ld= cp(blastPos, limb)
		if ld &lt; d then tp = lp; d = ld end
	end

	limb = torso.Parent:FindFirstChild(&quot;Left Leg&quot;)

	if limb then
		lp,ld= cp(blastPos, limb)
		if ld &lt; d then tp = lp; d = ld end
	end

	-- reduce length to nearest .4 multiple
	len = (math.floor(d*2.5) - 1) /2.5 

	if len &lt; .4 then
		return true
	end

	--print(&quot;blastPos &quot;, blastPos, &quot; tp &quot;, tp, &quot; len &quot;, len)

	local p = tp:lerp(blastPos, .5)  

	--print(&quot; lerp p &quot;, p)

	-- Move probe toward blastPos so it starts exactly at blastPos.
	-- Any reduction in len caused by probe size resolution should
	-- come from the tp end.
	p = p - ((tp - blastPos).unit * ((d-len)/2))

	local probe = Instance.new(&quot;Part&quot;)
	probe.formFactor = 2	--Plate
	probe.Size = Vector3.new(1,len,1)
	probe.Anchored = true
	probe.CFrame = CFrame.new(tp,blastPos) * cf90

	-- Put the probe into the world then move it to the desired
	-- position.  If it will not stay there this means there must be something in between
	-- the blast pos and the player, so the player is protected. (I know,	
	-- extremely hokey ray trace.)

	probe.Parent = game.Workspace
	probe.Position = p

	if (probe.Position - p).magnitude &gt; .1 then
		probe.Parent = nil
		return false
	end
	
	probe.Parent = nil

	return true
end

