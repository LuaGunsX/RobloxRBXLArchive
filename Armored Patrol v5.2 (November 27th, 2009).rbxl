local function genHillPos()

	while true do

		local x = math.random(MIN_X,MAX_X)
		local z = math.random(MIN_Z,MAX_Z)

		local allowed = true

		if Zones then

			local v = Vector3.new(x,0,z)

			for k,zone in pairs(Zones:GetChildren()) do

				if (zone.TopParamA &gt; 0) and
					((v - zone.Position).magnitude &lt; zone.TopParamA) then

					allowed = false
					break
				end
			end
		end

		if allowed then
			return x,z
		end
	end
end

