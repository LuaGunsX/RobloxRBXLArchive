CntrUser.Value = nil

CntrDrive.Changed:connect(handleDriveCmd)
CntrSteer.Changed:connect(handleSteerCmd)

local Hint = Instance.new(&quot;Hint&quot;)
local prevDriver

local CONTROL_INTERVAL = .33
local IDLE_TIME_THRESHOLD = 30

local enabledHints = false
local idleTime = 0

setSpeed(0,0)

local enabledHints = false

while (true) do

	wait(CONTROL_INTERVAL)

	if CntrUser.Value then

		local speed = Chassis.Velocity.magnitude

		-- Do not start displaying hints until vehicle has reached 5 studs/sec so
		-- we do not overwrite other hint info the tool may display.

		if speed &gt; 5  or enabledHints then
			--Hint.Text = string.format(&quot;Req Speed: %.1f, Cur Speed %.1f&quot;,speedReq, speed) 
			--Hint.Parent = CntrUser.Value

			prevDriver = CntrUser.Value
			enabledHints = true
		end

		idleTime = 0

	else
		Hint.Parent = nil
		enabledHints = false

		-- If we do not have a user for some time, then set wheel speed to true
		-- zero so the physics engine can mark this vehicle as not moving.
		-- Also this will auto-turnoff any runaway vehicles.
		idleTime = idleTime + CONTROL_INTERVAL

		if idleTime &gt;= IDLE_TIME_THRESHOLD then
			speedReq = SPEED_IDLE
			adjSpeed(speedReq)

			idleTime = 0
		end
	end



	-- Continually steer toward the distant fake target created from the last actual
	-- steer target that we received.  This way the player can pick a direction and
	-- the vehicle will continue in that direction.

	if lastSteerTarget then
		steerToward(lastSteerTarget)
	end

	-- Auto accelerate beyond max if we have requested max speed but are actually moving
	-- a lot less than this.  This is to help get up hills.  We set s to the velocity in the z direction
	-- of the Chassis object space (the facing direction).  Negative z values means we are moving
	-- forward.

	local s = -(Chassis.CFrame:vectorToObjectSpace(Chassis.Velocity).z)

	if (speedReq == SPEED_MAX) then
		if (s &lt; SPEED_MAX*.5) then
			adjSpeed(SPEED_MAX*2)
		else
			adjSpeed(speedReq)
		end

	elseif (speedReq == -SPEED_MAX) then
		if (s &gt; -SPEED_MAX*.5) then
			adjSpeed(-SPEED_MAX*2)
		else
			adjSpeed(speedReq)
		end

	-- If no auto acceleration then refresh speed to support steering changes
	else
		adjSpeed(speedReq)
	end


	-- Monitor to see if vehicle tipped over, and, if so, right it.
	maintainOrientation()

end

