local function genOneHillObject(  )

	local ho = {}

	local hill = Instance.new(&quot;Model&quot;)
	hill.Name = &quot;Hill&quot;
	hill.Parent = Hills
	
	ho.hill = hill

	local top = genOneTop(hill)
	ho.top = top

	-- Height of top, including base thickness, from xz plane
	local height = top.Position.y
	ho.height = height

	ho.n = genSideParms(top.Size.z/2, height)
	ho.e = genSideParms(top.Size.x/2, height)
	ho.s = genSideParms(top.Size.z/2, height)
	ho.w = genSideParms(top.Size.x/2, height)

	ho.nnw = {}
	ho.nne = {}
	ho.ene = {}
	ho.ese = {}
	ho.sse = {}
	ho.ssw = {}
	ho.wsw = {}
	ho.wnw = {}


	return ho
end

---------------------------------------------------------------------
-- Put ho1 and ho2 into same range group.  If neither already in a range 
-- then create a new range and put them both in it.  If one in a 
-- range and the other is not, then put the latter into the formers range.
-- If they both already in different ranges then merge the second range into the first.

local function popRange(ho1, ho2)

	local r1 = ho1.range
	local r2 = ho2.range

	if r1 then
		if r2 then
			-- If already in same range, nothing to do
			if r1 == r2 then
				return
			end
			
			-- Else both already in different ranges, merge
			-- the 2nd range into the first, remove the 2nd

			for h in pairs(r2) do
				h.range = r1
				r1[h] = true
			end
			
		-- ho1 in a range but ho2 not, put ho2 into ho1s range
		else	
			
			r1[ho2] = true
			ho2.range = r1
		end

	else
		-- If ho2 in a range but ho1 not, put ho1 into ho2s range
		if r2 then
			
			r2[ho1] = true
			ho1.range = r2

		-- Else neither in a range, create a new range, add both
		else

			r1 = {}

			r1[ho1] = true
			ho1.range = r1

			r1[ho2] = true
			ho2.range = r1
		end
	end
end

