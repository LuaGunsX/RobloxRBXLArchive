					<string name="Source">--
local CardImg = { 
	c = {&quot;http://www.roblox.com/asset/?id=13283362&quot;, &quot;http://www.roblox.com/asset/?id=13283370&quot;,&quot;http://www.roblox.com/asset/?id=13283377&quot;,&quot;http://www.roblox.com/asset/?id=13283384&quot;,&quot;http://www.roblox.com/asset/?id=13283390&quot;,&quot;http://www.roblox.com/asset/?id=13283400&quot;,&quot;http://www.roblox.com/asset/?id=13283406&quot;,&quot;http://www.roblox.com/asset/?id=13283414&quot;,&quot;http://www.roblox.com/asset/?id=13283421&quot;,&quot;http://www.roblox.com/asset/?id=13283432&quot;,&quot;http://www.roblox.com/asset/?id=13283439&quot;,&quot;http://www.roblox.com/asset/?id=13283448&quot;,&quot;http://www.roblox.com/asset/?id=13283474&quot;}, 
	d = {&quot;http://www.roblox.com/asset/?id=13283484&quot;,&quot;http://www.roblox.com/asset/?id=13283491&quot;,&quot;http://www.roblox.com/asset/?id=13283499&quot;,&quot;http://www.roblox.com/asset/?id=13283502&quot;,&quot;http://www.roblox.com/asset/?id=13283508&quot;,&quot;http://www.roblox.com/asset/?id=13283513&quot;,&quot;http://www.roblox.com/asset/?id=13283517&quot;,&quot;http://www.roblox.com/asset/?id=13283313&quot;,&quot;http://www.roblox.com/asset/?id=13283543&quot;,&quot;http://www.roblox.com/asset/?id=13283552&quot;,&quot;http://www.roblox.com/asset/?id=13283565&quot;,&quot;http://www.roblox.com/asset/?id=13283569&quot;,&quot;http://www.roblox.com/asset/?id=13283575&quot;}, 
	h = {&quot;http://www.roblox.com/asset/?id=13283582&quot;,&quot;http://www.roblox.com/asset/?id=13283586&quot;,&quot;http://www.roblox.com/asset/?id=13283593&quot;,&quot;http://www.roblox.com/asset/?id=13283598&quot;,&quot;http://www.roblox.com/asset/?id=13283605&quot;,&quot;http://www.roblox.com/asset/?id=13283608&quot;,&quot;http://www.roblox.com/asset/?id=13283612&quot;,&quot;http://www.roblox.com/asset/?id=13283621&quot;,&quot;http://www.roblox.com/asset/?id=13283627&quot;,&quot;http://www.roblox.com/asset/?id=13283642&quot;,&quot;http://www.roblox.com/asset/?id=13283655&quot;,&quot;http://www.roblox.com/asset/?id=13283669&quot;,&quot;http://www.roblox.com/asset/?id=13283674&quot;}, 
	s = {&quot;http://www.roblox.com/asset/?id=13283720&quot;,&quot;http://www.roblox.com/asset/?id=13283735&quot;,&quot;http://www.roblox.com/asset/?id=13283740&quot;,&quot;http://www.roblox.com/asset/?id=13283744&quot;,&quot;http://www.roblox.com/asset/?id=13283752&quot;,&quot;http://www.roblox.com/asset/?id=13283758&quot;,&quot;http://www.roblox.com/asset/?id=13283761&quot;,&quot;http://www.roblox.com/asset/?id=13283768&quot;,&quot;http://www.roblox.com/asset/?id=13283772&quot;,&quot;http://www.roblox.com/asset/?id=13283781&quot;,&quot;http://www.roblox.com/asset/?id=13283789&quot;,&quot;http://www.roblox.com/asset/?id=13283800&quot;,&quot;http://www.roblox.com/asset/?id=13283810&quot;} 
}
--
local AllCards = {}
local Deck = {ID = &quot;Deck&quot;} --deck
local Draw = {ID = &quot;Draw&quot;} --3 or less drawn cards
--
local Heart = {ID = &quot;Dump&quot;} --put away cards
local Spade = {ID = &quot;Dump&quot;}
local Diamond = {ID = &quot;Dump&quot;}
local Club = {ID = &quot;Dump&quot;}
--
local Rows = {{ID = &quot;Row&quot;}, {ID = &quot;Row&quot;}, {ID = &quot;Row&quot;}, {ID = &quot;Row&quot;}, {ID = &quot;Row&quot;}, {ID = &quot;Row&quot;}, {ID = &quot;Row&quot;}}
--
local Selection = nil
local ISelection = nil
--
local IGameBoard = game.Lighting.GB:clone()
IGameBoard.Owner.Value = script.Parent.Parent.Parent.Name
local IFF = Instance.new(&quot;ForceField&quot;)
--
local RefRows = {IGameBoard.R1, IGameBoard.R2, IGameBoard.R3, IGameBoard.R4, IGameBoard.R5, IGameBoard.R6, IGameBoard.R7}
local RefDraws = {IGameBoard.Draw1, IGameBoard.Draw2, IGameBoard.Draw3}
--

function NewDeck()
	local ref = {CardImg.c, CardImg.d, CardImg.s, CardImg.h}
	for s = 0, 3 do
		for i = 1, 13 do
			local nw = {}
			nw.Part = script.Parent.Card:clone()
			nw.Img = ref[s+1][i]
			nw.Part.Decal.Texture = nw.Img
			nw.Part.CardID.Value = (s*13) + i
			nw.Suit = s+1
			nw.Color = (((s+1) % 2) == 0)
			nw.Value = i
			nw.FaceUp = false
			nw.Parent = Deck
			nw.GUID = (s*13) + i
			Deck[(s*13) + i] = nw
			AllCards[(s*13) + i] = nw
		end
	end
end

function ShuffleDeck()
	for i = 1, 100 do
		local ps = math.random(1, #Deck)
		local c = Deck[ps]
		table.remove(Deck, ps)
		table.insert(Deck, 1, c)
	end
	for i = 1, 100 do
		local ps = math.random(1, #Deck - 1)
		Deck[ps], Deck[ps+1] = Deck[ps+1], Deck[ps]
	end
	for i = 1, #Deck do
		print(&quot;&lt;&quot;, i, &quot;&gt;&quot;,Deck[i].GUID)
	end
end

function PopTop()
	local c = Deck[1]
	table.remove(Deck, 1)
	return c
end

function PushBottom(card)
	Deck[#Deck + 1] = card
end

function SetupTable()
	math.randomseed(game.Workspace.DistributedGameTime)
	IGameBoard.Parent = game.Workspace
	IGameBoard:MoveTo(Vector3.new(math.random(-5000, 5000), 0, game.Workspace.Games.Value*1000))
	IGameBoard.Draw1.Parent = nil
	IGameBoard.Draw2.Parent = nil
	IGameBoard.Draw3.Parent = nil
	for i = 1, 7 do
		for c = 1, i do
			local cr = PopTop()
			if c == i then cr.FaceUp = true end
			Rows[i][c] = cr
			cr.Parent = Rows[i]
		end
	end	
	game.Workspace.Games.Value = game.Workspace.Games.Value + 1
end

function ResetTable()
	for i = 1, 52 do
		AllCards[i].Parent = Deck
		AllCards[i].FaceUp = false
		Deck[i] = AllCards[i]
	end
	for i = 1, 7 do
		for j = 1, 20 do
			Rows[i][j] = nil
		end
	end
	for i = 1, 3 do
		Draw[i] = nil
	end
	Heart = {ID = &quot;Dump&quot;}
	Spade = {ID = &quot;Dump&quot;}
	Club = {ID = &quot;Dump&quot;}
	Diamond = {ID = &quot;Dump&quot;}
	IGameBoard.DumpC.Decal.Texture = &quot;http://www.roblox.com/asset/?id=13345338&quot;
	IGameBoard.DumpD.Decal.Texture = &quot;http://www.roblox.com/asset/?id=13345355&quot;
	IGameBoard.DumpH.Decal.Texture = &quot;http://www.roblox.com/asset/?id=13345362&quot;
	IGameBoard.DumpS.Decal.Texture = &quot;http://www.roblox.com/asset/?id=13345369&quot;
	-------------------------------------
	ShuffleDeck()
	for i = 1, 7 do
		for c = 1, i do
			local cr = PopTop()
			if c == i then cr.FaceUp = true end
			Rows[i][c] = cr
			cr.Parent = Rows[i]
		end
	end	
end

function DrawTable()
	--draw rows
	for rw = 1, 7 do
		local c = 1
		while Rows[rw][c] ~= nil do
			local cr = Rows[rw][c]
			if cr.FaceUp then
				cr.Part.Decal.Texture = cr.Img
			else
				cr.Part.Decal.Texture = &quot;http://www.roblox.com/asset/?id=13284205&quot;
			end
			cr.Part.Parent = nil
			cr.Part.Position = RefRows[rw].Position + Vector3.new(0, 0.1*(c), -2*(c-1))
			cr.Part.Parent = IGameBoard
			c = c + 1
		end
	end
	--draw draws
	for i = 1, 3 do
		if Draw[i] then
			Draw[i].Part.Parent = nil
			Draw[i].Part.Position = RefDraws[i].Position
			Draw[i].Part.Parent = IGameBoard
			Draw[i].Part.Decal.Texture = Draw[i].Img
		end
	end
	--draw dumps
	if #Heart &gt; 0 then
		IGameBoard.DumpH.Decal.Texture = Heart[#Heart].Img
	end
	if #Diamond &gt; 0 then
		IGameBoard.DumpD.Decal.Texture = Diamond[#Diamond].Img
	end
	if #Spade &gt; 0 then
		IGameBoard.DumpS.Decal.Texture = Spade[#Spade].Img
	end
	if #Club &gt; 0 then
		IGameBoard.DumpC.Decal.Texture = Club[#Club].Img
	end
	--hide deck
	for i = 1, #Deck do
		Deck[i].Part.Parent = nil
	end
	--draw deck
	if #Deck &gt; 0 then
		IGameBoard.Deck.Transparency = 0
		IGameBoard.Deck.Decal.Texture = &quot;http://www.roblox.com/asset/?id=13284205&quot;
		IGameBoard.Deck.Mesh.Scale = Vector3.new(1, #Deck/30*2, 1)
	else
		IGameBoard.Deck.Transparency = 1
		IGameBoard.Deck.Decal.Texture = &quot;&quot;
	end
end

function Click(trg) --clicked on trg
	print(&quot;Clicked&quot;)
	if trg == IGameBoard.Deck then
		--draw new cards
		--push bottom old cards
		for i = 1, 3 do
			if Draw[i] then
				PushBottom(Draw[i])
				Draw[i].Parent = Deck
				Draw[i] = nil
			end
		end
		--poptop new cards
		for i = 1, 3 do
			if #Deck &gt; 0 then
				Draw[i] = PopTop()
				Draw[i].FaceUp = true
				Draw[i].Parent = Draw
			end
		end
	elseif trg == IGameBoard.Reset then
		ResetTable()
	elseif trg:FindFirstChild(&quot;CardID&quot;) then
		local c = AllCards[trg.CardID.Value]
		if c.Parent.ID == &quot;Row&quot; then --in a row
			if c.Parent[#c.Parent] == c and not c.FaceUp then --show card
				c.FaceUp = true
			elseif c.FaceUp then --select
				Selection = c
				IFF.Parent = c.Part
			end
		elseif c.Parent.ID == &quot;Draw&quot; then
			if Draw[#Draw] == c then
				Selection = c
				IFF.Parent = c.Part
			end
		end
	end
	DrawTable()
end

function MatchRule(src, dst)
	return (src.Value + 1 == dst.Value and src.Color ~= dst.Color)
end

function DumpCard(dumptb, card)
	if #dumptb == 0 then
		if Selection.Value == 1 then --ace, first
			if Selection.Parent.ID == &quot;Draw&quot; then
				Draw[#Draw] = nil
				Selection.Parent = dumptb
				dumptb[Selection.Value] = Selection
			elseif Selection.Parent.ID == &quot;Row&quot; and Selection.Parent[#Selection.Parent] == Selection then
				Selection.Parent[#Selection.Parent] = nil
				Selection.Parent = dumptb
				dumptb[Selection.Value] = Selection
			end
			Selection.Part.Parent = nil
		end
	else
		if dumptb[Selection.Value - 1] then --can be placed
			if Selection.Parent.ID == &quot;Draw&quot; then
				Draw[#Draw] = nil
				Selection.Parent = dumptb
				dumptb[Selection.Value] = Selection
			elseif Selection.Parent.ID == &quot;Row&quot; and Selection.Parent[#Selection.Parent] == Selection then
				Selection.Parent[#Selection.Parent] = nil
				Selection.Parent = dumptb
				dumptb[Selection.Value] = Selection
			end
			Selection.Part.Parent = nil
		end
	end
end

function ClickTo(trg) --clicked on trg with a selection
	print(&quot;Clicked with selection: &quot;..Selection.GUID)
	if trg:FindFirstChild(&quot;CardID&quot;) then
		local c = AllCards[trg.CardID.Value]
		if c.Parent.ID == &quot;Row&quot; and c.Parent[#c.Parent] == c then --first card in row
			if Selection.Parent.ID == &quot;Row&quot; then --move a group of cards
				--check match
				if not MatchRule(Selection, c) then
					Selection = nil
					IFF.Parent = nil
					return
				end
				local gpos = 1
				local gtb = Selection.Parent
				--find start
				while Selection.Parent[gpos] ~= Selection do
					gpos = gpos + 1
				end
				--move cards
				while gtb[gpos] do
					gtb[gpos].Parent = c.Parent
					c.Parent[#c.Parent + 1] = gtb[gpos]
					gtb[gpos] = nil
					gpos = gpos + 1
				end
				Selection = nil
				IFF.Parent = nil
			elseif Selection.Parent.ID == &quot;Draw&quot; then --move one card onto row
				if not MatchRule(Selection, c) then
					Selection = nil
					IFF.Parent = nil
					return
				end
				Selection.Parent = c.Parent
				c.Parent[#c.Parent+1] = Selection
				Draw[#Draw] = nil
				Selection = nil
				IFF.Parent = nil
			end
		end
	elseif trg.Name:sub(1,1) == &quot;R&quot; then
		local rnum = tonumber(trg.Name:sub(2,2))
		if #Rows[rnum] == 0 then
			if Selection.Parent.ID == &quot;Row&quot; then
				local gpos = 1
				local gtb = Selection.Parent
				--find start
				while Selection.Parent[gpos] ~= Selection do
					gpos = gpos + 1
				end
				--move cards
				while gtb[gpos] do
					gtb[gpos].Parent = Rows[rnum]
					Rows[rnum][#Rows[rnum] + 1] = gtb[gpos]
					gtb[gpos] = nil
					gpos = gpos + 1
				end
				Selection = nil
				IFF.Parent = nil				
			elseif Selection.Parent.ID == &quot;Draw&quot; then
				Selection.Parent = Rows[rnum]
				Rows[rnum][1] = Selection
				Draw[#Draw] = nil
				Selection = nil
				IFF.Parent = nil
			end
		end
	elseif trg == IGameBoard.DumpH then --4
		if Selection.Suit == 4 then
			DumpCard(Heart, Selection)
		end
	elseif trg == IGameBoard.DumpD then --2
		if Selection.Suit == 2 then
			DumpCard(Diamond, Selection)
		end		
	elseif trg == IGameBoard.DumpC then --1
		if Selection.Suit == 1 then
			DumpCard(Club, Selection)
		end
	elseif trg == IGameBoard.DumpS then --3
		if Selection.Suit == 3 then
			DumpCard(Spade, Selection)
		end
	end
	Selection = nil
	IFF.Parent = nil
	DrawTable()
end

script.Parent.Selected:connect(function(m)
	math.randomseed(tick())
	NewDeck()
	ShuffleDeck()
	SetupTable()
	DrawTable()
	--
	game.Workspace.CurrentCamera.CameraSubject = IGameBoard.Part
	game.Workspace.CurrentCamera.CameraType = 1
	--
	m.Button1Down:connect(function()
		if Selection then
			if m.Target then
				ClickTo(m.Target)
			else
				Selection = nil
			end
		elseif m.Target then
			Click(m.Target)
		end
	end)
end)

wait(0.5)

script.Parent.Parent.Parent.Character:remove()

