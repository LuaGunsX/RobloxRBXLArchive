&#9;seg1.Parent = m;
&#9;seg1.Name = &quot;Segment_1&quot;;
&#9;local seg1w = Instance.new(&quot;Weld&quot;,seg1);
&#9;seg1w.Part0 = neckpiece;
&#9;seg1w.Part1 = seg1;
