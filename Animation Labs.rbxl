gui=script.Parent
panel=gui.Color
sliders={gui.Red,gui.Green,gui.Blue}
hairslider=gui:findFirstChild(&quot;Style&quot;)
hairs=game.Lighting.Hair:children()
hairid=math.random(0,#hairs-1)
PresentColors={
&#9;{25,20,20}; -- Black
&#9;{30,20,10}; -- Dark Brown
&#9;{90,45,0}; -- Brown
&#9;{125,40,10}; -- Red
&#9;{190,160,40}; -- Blonde
&#9;{210,180,110}; -- Light Blonde
&#9;{255,255,255}; -- White
&#9;{200,250,200}; -- Light grey
&#9;{100,100,100}; -- Dark grey
}
