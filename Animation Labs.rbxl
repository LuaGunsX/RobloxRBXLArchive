&#9;&#9;Gui:RemoveTools(Player)
&#9;&#9;local Character = Player.Character
&#9;&#9;if Character and Character:FindFirstChild(&quot;Torso&quot;) then
&#9;&#9;&#9;local NewDecal = Instance.new(&quot;Decal&quot;)
