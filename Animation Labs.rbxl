&#9;local function gettable(v,mirror)
&#9;&#9;local t=Vector3ToTable(v.Value,v:findFirstChild(&quot;Offset&quot;) and v.Offset.Value)
&#9;&#9;return mirror and MirrorTable(t) or t
&#9;end
&#9;local function AddAutoMirrorPose(p) -- Most convenient function EVAR!
&#9;&#9;Pose={}
&#9;&#9;local children=PoseData:findFirstChild(p.AutoMirror.Value,true):children()
&#9;&#9;for i=1,#children do
&#9;&#9;&#9;local j=children[i]
&#9;&#9;&#9;if j and j.ClassName==&quot;Vector3Value&quot; then
&#9;&#9;&#9;&#9;local id=j.Name:find(&quot;Right&quot;) and j.Name:gsub(&quot;Right&quot;,&quot;Left&quot;) or j.Name:gsub(&quot;Left&quot;,&quot;Right&quot;)
&#9;&#9;&#9;&#9;if p:findFirstChild(id) then
&#9;&#9;&#9;&#9;&#9;Pose[id]=gettable(p:findFirstChild(id),j:findFirstChild(&quot;Mirror&quot;)~=nil)
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;Pose[id]=gettable(j,j:findFirstChild(&quot;Mirror&quot;)==nil)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;Poses[p.Name]=Pose
&#9;&#9;return Pose
&#9;end
&#9;local function AddPose(container)
&#9;&#9;if container:findFirstChild(&quot;AutoMirror&quot;) then
&#9;&#9;&#9;return AddAutoMirrorPose(container)
&#9;&#9;end
&#9;&#9;local Pose={}
&#9;&#9;local autoleft=container:findFirstChild(&quot;AutoLeft&quot;)~=nil
&#9;&#9;local children=container:children()
&#9;&#9;for i=1,#children do
&#9;&#9;&#9;local j=children[i]
&#9;&#9;&#9;if j and j.ClassName==&quot;Vector3Value&quot; then
&#9;&#9;&#9;&#9;Pose[j.Name]=Pose[j.Name] or gettable(j,j:findFirstChild(&quot;Mirror&quot;)~=nil) -- Second argument is relatively useless in this case
&#9;&#9;&#9;&#9;if autoleft and j.Name:find(&quot;Right&quot;) then
&#9;&#9;&#9;&#9;&#9;local id=j.Name:gsub(&quot;Right&quot;,&quot;Left&quot;)
&#9;&#9;&#9;&#9;&#9;if not container:findFirstChild(id) then
&#9;&#9;&#9;&#9;&#9;&#9;Pose[id]=Pose[id] or gettable(j,j:findFirstChild(&quot;Mirror&quot;)==nil)
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;Poses[container.Name]=Pose
&#9;&#9;return Pose
&#9;end
&#9;local function AddPoses(group)
&#9;&#9;local children=group:children()
&#9;&#9;for i=1,#children do
