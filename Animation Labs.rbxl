&#9;local function GetC(v,mirror)
&#9;&#9;local off=mirror and v.Value*Vector3.new(-1,1,1) or v.Value
&#9;&#9;local rot=(v:findFirstChild(&quot;Rotate&quot;) and v.Rotate.Value*math.pi/180 or Vector3.new(0,0,0))*(mirror and Vector3.new(1,-1,-1) or 1)
&#9;&#9;return CFrame.Angles(rot.x,rot.y,rot.z)+off*BodyScale
&#9;end
&#9;local function AddJoint(data,mirror)
&#9;&#9;mirror=data:findFirstChild(&quot;Mirror&quot;) and not mirror or mirror
&#9;&#9;local id=data.Name
&#9;&#9;local Part0=Body:findFirstChild(data.Part0.Value)
&#9;&#9;local Part1=Body:findFirstChild(data.Part1.Value)
&#9;&#9;local C0=GetC(data.C0,mirror)
&#9;&#9;local C1=GetC(data.C1,mirror)
&#9;&#9;Joint.new(id,Part0,Part1,C0,C1,data:findFirstChild(&quot;Mode&quot;)~=nil)
&#9;end
&#9;local function AddLeftJoint(data,mirror)
&#9;&#9;mirror=data:findFirstChild(&quot;Mirror&quot;) and not mirror or mirror
&#9;&#9;local id=data.Name
&#9;&#9;if not id:find(&quot;Right&quot;) then return; end
&#9;&#9;id=id:gsub(&quot;Right&quot;,&quot;Left&quot;)
&#9;&#9;local Part0=Body:findFirstChild(data.Part0.Value:gsub(&quot;Right&quot;,&quot;Left&quot;))
&#9;&#9;local Part1=Body:findFirstChild(data.Part1.Value:gsub(&quot;Right&quot;,&quot;Left&quot;))
&#9;&#9;local C0=GetC(data.C0,not mirror)
&#9;&#9;local C1=GetC(data.C1,not mirror)
&#9;&#9;Joint.new(id,Part0,Part1,C0,C1,data:findFirstChild(&quot;Mode&quot;)~=nil)
&#9;end
&#9;local function AddJoints(model)
&#9;&#9;local autoleft=model:findFirstChild(&quot;AutoLeft&quot;)~=nil
&#9;&#9;local children=model:children()
&#9;&#9;for i=1,#children do
&#9;&#9;&#9;local p=children[i]
&#9;&#9;&#9;if p then
&#9;&#9;&#9;&#9;if p.ClassName==&quot;BoolValue&quot; then
&#9;&#9;&#9;&#9;&#9;AddJoint(children[i])
&#9;&#9;&#9;&#9;&#9;if autoleft then
&#9;&#9;&#9;&#9;&#9;&#9;AddLeftJoint(children[i])
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;elseif p.ClassName==&quot;Backpack&quot; then
&#9;&#9;&#9;&#9;&#9;AddJoints(p)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
