&#9;&#9;end
&#9;end)
&#9;
&#9;local function Destroy()
&#9;&#9;for k in pairs(Class) do
&#9;&#9;&#9;Class[k] = nil
&#9;&#9;end
&#9;&#9;con:disconnect()
&#9;end
&#9;Class.Destroy = Destroy
&#9;
&#9;Update()
&#9;
&#9;return Class,Label
end

