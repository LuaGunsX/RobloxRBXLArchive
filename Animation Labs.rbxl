&#9;seg2w.C0 = CFrame.new(0,0,.45);
&#9;seg2w.C1 = CFrame.new(0,0,-.45)*CFrame.Angles(math.rad(-5),0,0);
&#9;m.Parent = player;
&#9;local seg3 = seg2:Clone();
