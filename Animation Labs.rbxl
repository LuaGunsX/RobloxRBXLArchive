hair.Size=Vector3.new(.2,.2,.2)
hair.CanCollide=false
hair.Anchored=false
mesh=Instance.new(&quot;SpecialMesh&quot;,hair)

while not player.Character do wait() end
while not player.Character:findFirstChild(&quot;Body&quot;) do wait() end
while not player.Character.Body:findFirstChild(&quot;Head&quot;) do wait() end
head=player.Character.Body.Head
