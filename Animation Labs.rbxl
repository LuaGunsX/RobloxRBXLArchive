hair.Parent=player.Character.Body

function refresh()
&#9;for i=1,3 do
&#9;&#9;rgb[i]=math.max(0,math.min(255,rgb[i]))
&#9;&#9;sliders[i].Text=rgb[i]
&#9;end
&#9;color=Color3.new(rgb[1]/255,rgb[2]/255,rgb[3]/255)
&#9;panel.BackgroundColor3=color
&#9;mesh.VertexColor=Vector3.new(rgb[1]/255,rgb[2]/255,rgb[3]/255)
end
function refreshPresentColor()
&#9;PresentId=PresentId%#PresentColors
&#9;rgb={unpack(PresentColors[PresentId+1])}
&#9;refresh()
end
function refreshhair()
&#9;hairid=hairid%#hairs
&#9;local data=hairs[hairid+1]
&#9;hairslider.Text=data:findFirstChild(&quot;Text&quot;) and data.Text.Value or data.Name
&#9;mesh.MeshId=link..data.MeshId.Value
&#9;mesh.TextureId=link..data.TextureId.Value
&#9;mesh.Scale=data.Scale.Value
&#9;weld.C0=(CFrame.new(data.Offset.Value)*CFrame.Angles(data.Offset.Rotate.Value.x,data.Offset.Rotate.Value.y,data.Offset.Rotate.Value.z)):inverse()
end
function GiveClickHold(button,initial_time,repeat_time,method,repeat_method,touch_method)
&#9;if Game:GetService(&apos;UserInputService&apos;).MouseEnabled then
&#9;&#9;local active=false
&#9;&#9;local controller=nil
&#9;&#9;button.MouseButton1Down:connect(function()
&#9;&#9;&#9;if controller then return; end
&#9;&#9;&#9;local c={}
&#9;&#9;&#9;controller=c
&#9;&#9;&#9;method()
&#9;&#9;&#9;wait(initial_time)
&#9;&#9;&#9;if controller~=c then return; end
&#9;&#9;&#9;while controller==c do
&#9;&#9;&#9;&#9;(repeat_method or method)()
&#9;&#9;&#9;&#9;wait(repeat_time)
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;&#9;button.MouseButton1Up:connect(function()
&#9;&#9;&#9;controller=nil
&#9;&#9;end)
&#9;&#9;button.MouseLeave:connect(function()
&#9;&#9;&#9;controller=nil
&#9;&#9;end)
&#9;else
&#9;&#9;button.MouseButton1Click:connect(touch_method)
