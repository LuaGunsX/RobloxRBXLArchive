end

function Flex(cape,vals)
&#9;for i,v in pairs(vals) do
&#9;&#9;if(i ~= 1) then 
&#9;&#9;&#9;cape:GetChildren()[i].Weld.C1 = CFrame.new(0,0,-.45)*CFrame.Angles(math.rad(vals[i-1]),0,0);
