&#9;&#9;if div then
&#9;&#9;&#9;for i=1,#polygonX do
&#9;&#9;&#9;&#9;polygonX[i] = (polygonX[i])/div
&#9;&#9;&#9;end
&#9;&#9;&#9;for i=1,#polygonY do
&#9;&#9;&#9;&#9;polygonY[i] = (polygonY[i])/div
