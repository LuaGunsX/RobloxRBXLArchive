function LerpPoses(pose1,pose2,alpha,beta)
&#9;beta=beta or 1-alpha
&#9;local pose={}
&#9;for k,v in pairs(pose1) do
&#9;&#9;pose[k]=pose[k] or pose2[k] and LerpTables(v,pose2[k],alpha,beta) or v
&#9;end
&#9;for k,v in pairs(pose2) do
&#9;&#9;pose[k]=pose[k] or v
&#9;end
&#9;return pose
end

-- Setting Poses
function SetPose(pose)
&#9;priority=priority or 1
&#9;for k,v in pairs(pose) do
&#9;&#9;if Joints[k].Controller==nil then
&#9;&#9;&#9;Joints[k]:Set(v)
&#9;&#9;end
&#9;end
end
do -- Set pose to default
