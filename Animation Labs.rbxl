local min = vMinimum.Value
local range = max - min
local inc = vIncrement.Value
local spinning = tick()
local internal = {
&#9;[vIncrement]=false;
&#9;[vMinimum]=false;
&#9;[vMaximum]=false;
&#9;[vValue]=false;
}

event = {}
function disconnect(set)
&#9;for _,index in pairs(set) do
