				<string name="Name">BodyController</string>
				<ProtectedString name="Source">while not script.Parent.BodyMain:findFirstChild(&quot;Module&quot;) do wait(1/16) end
while not script.Parent:findFirstChild(&quot;Body&quot;) do wait(1/16) end
while not script.Parent.Body:findFirstChild(&quot;Base&quot;) do wait(1/16) end
Base=script.Parent.Body.Base
wait(1/16)
Body=script.Parent.BodyMain.Module:Invoke()
Poses=Body.Poses
ToPose=Body.ToPose
--[[
&#9;Data;
&#9;Body;
&#9;BodyScale;
&#9;Parts;
&#9;Joints;
&#9;Poses;
&#9;
&#9;Vector3ToTable(rot,off)
&#9;MirrorTable(t)
&#9;TablesAreEqual(a,b)
&#9;LerpTables(a,b,alpha,beta)
&#9;TableToCFrame(t,mode)
&#9;LerpPoses(pose1,pose2,alpha,beta)

&#9;SetPose(pose)
&#9;ToPose(pose,framecount,framelength,priority,controller)
&#9;SustainPose(pose,frequency,framecount,framelength,priority,controller)
]]
Human=script.Parent.Humanoid
pose=&quot;Idle&quot;
Aborts={
&#9;Idle={true,false};
&#9;Running={true,false};
&#9;Falling={true,false};
&#9;Climbing={true,false};
&#9;Sitting={true,false};
}
function SetAborts(p)
&#9;for k,v in pairs(Aborts) do
&#9;&#9;v[2]=false
&#9;&#9;if k==p then
&#9;&#9;&#9;v[1]=false
&#9;&#9;else
&#9;&#9;&#9;v[1]=true
&#9;&#9;end
&#9;end
end
function ChangePose(p)
&#9;if p==pose then return; end
&#9;pose=p
&#9;wait(.05)
&#9;if pose~=p and p~=&quot;Jump&quot; then return; end
&#9;pose=p
&#9;print(p)
&#9;SetAborts(p)
end
Speed=0
Human.Running:connect(function(speed)
&#9;Speed=speed
&#9;if speed&gt;0 then
&#9;&#9;ChangePose(&quot;Running&quot;)
&#9;else
&#9;&#9;ChangePose(&quot;Idle&quot;)
&#9;end
end)
Human.FreeFalling:connect(function()
&#9;ChangePose(&quot;Falling&quot;)
end)
Human.Climbing:connect(function()
&#9;ChangePose(&quot;Climbing&quot;)
end)
Human.Seated:connect(function()
&#9;ChangePose(&quot;Sitting&quot;)
end)

PoseMethods={
&#9;Idle=function()
&#9;&#9;ToPose(Poses.Default,8,nil,1,Aborts.Idle)
&#9;end;
&#9;Running=function()
&#9;&#9;while not Aborts.Running[1] do
&#9;&#9;&#9;for i=1,4 do
&#9;&#9;&#9;&#9;ToPose(Poses[&quot;Run_&quot;..i],5,nil,1,Aborts.Running)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end;
&#9;Falling=function()
&#9;&#9;while not Aborts.Falling[1] do
&#9;&#9;&#9;for i=1,2 do
&#9;&#9;&#9;&#9;ToPose(Poses[&quot;Fall_&quot;..i],10,nil,1,Aborts.Falling)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end;
&#9;Sitting=function()
&#9;&#9;ToPose(Poses.Sit_2,16,nil,1,Aborts.Sitting)
&#9;&#9;while not Aborts.Sitting[1] do
&#9;&#9;&#9;for i=1,2 do
&#9;&#9;&#9;&#9;ToPose(Poses[&quot;Sit_&quot;..i],48,nil,1,Aborts.Sitting)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end;
&#9;Climbing=function()
&#9;&#9;local pause={false}
&#9;&#9;coroutine.resume(coroutine.create(function()
&#9;&#9;&#9;while pose==&quot;Climbing&quot; or not Aborts.Climbing[1] do
&#9;&#9;&#9;&#9;Aborts.Climbing[2]=Base.Velocity.Magnitude&lt;1
&#9;&#9;&#9;&#9;wait(.125)
&#9;&#9;&#9;end
&#9;&#9;&#9;return
&#9;&#9;end))
&#9;&#9;while not Aborts.Climbing[1] do
&#9;&#9;&#9;for i=1,2 do
&#9;&#9;&#9;&#9;ToPose(Poses[&quot;Climb_&quot;..i],12,nil,1,Aborts.Climbing)
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end;&#9;
}

while true do
&#9;SetAborts(pose)
&#9;PoseMethods[pose]()
&#9;wait()
end</ProtectedString>
