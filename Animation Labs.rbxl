		<Item class="LocalScript" referent="RBX1249">
			<Properties>
				<bool name="Disabled">true</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">BodyMain</string>
				<ProtectedString name="Source">-- Copyright 2013 Tomarty

Body=Instance.new(&quot;Model&quot;)
Body.Name=&quot;Body&quot;

Data=Game.Lighting.Human
PartData=Data.BodyParts
JointData=Data.Joints
PoseData=Data.Poses
--AnimData=Data.Animations

BodyScale=1
BodyScale2=Instance.new(&quot;NumberValue&quot;)
BodyScale2.Name=&quot;BodyScale&quot;
BodyScale2.Value=BodyScale
BodyScale2.Parent=script.Parent
BodyScale=(PartData:findFirstChild(&quot;BodyScale&quot;) and PartData.BodyScale.Value or 1)*BodyScale

Parts={}

do -- LOAD PARTS
&#9;local function AddPart(example,name)
&#9;&#9;if not example then return; end
&#9;&#9;local p=example:clone()
&#9;&#9;if p:findFirstChild(&quot;Mesh&quot;) then
&#9;&#9;&#9;p.Mesh.Scale=p.Mesh.Scale*BodyScale
&#9;&#9;end
&#9;&#9;p.Name=name or p.Name
&#9;&#9;Parts[p.Name]=p
&#9;&#9;p.Anchored=false
&#9;&#9;p.CanCollide=false
&#9;&#9;p.Parent=Body
&#9;&#9;p.CFrame=CFrame.new(0,512,0)
&#9;end
&#9;local function AddParts(model)
&#9;&#9;local autoleft=model:findFirstChild(&quot;AutoLeft&quot;) and true or false
&#9;&#9;local children=model:children()
&#9;&#9;for i=1,#children do
&#9;&#9;&#9;local p=children[i]
&#9;&#9;&#9;if p then
&#9;&#9;&#9;&#9;if p:IsA(&quot;BasePart&quot;) then
&#9;&#9;&#9;&#9;&#9;AddPart(p)
&#9;&#9;&#9;&#9;&#9;if autoleft and p.Name:find(&quot;Right&quot;) then
&#9;&#9;&#9;&#9;&#9;&#9;local leftname=p.Name:gsub(&quot;Right&quot;,&quot;Left&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;if not Body:findFirstChild(leftname) and not model:findFirstChild(leftname) and not Parts[leftname] then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;AddPart(p,leftname)
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;AddParts(PartData)
end

-- Table methods
function Vector3ToTable(rot,off)
&#9;off,rot=off or Vector3.new(0,0,0),rot and rot*math.pi/180 or Vector3.new(0,0,0)
&#9;return {rot.x,rot.y,rot.z,off.x,off.y,off.z}
end
function MirrorTable(t)
&#9;return {t[1],-t[2],-t[3],-t[4],t[5],t[6],t[7]}
end
function TablesAreEqual(a,b)
&#9;return a[1]==b[1] and a[4]==b[4] and a[2]==b[2] and a[5]==b[5] and a[3]==b[3] and a[6]==b[6] -- staggered check more efficient on average?
end
function LerpTables(a,b,alpha,beta)
&#9;beta=beta or 1-alpha
&#9;return {a[1]*alpha+b[1]*beta,a[2]*alpha+b[2]*beta,a[3]*alpha+b[3]*beta,a[4]*alpha+b[4]*beta,a[5]*alpha+b[5]*beta,a[6]*alpha+b[6]*beta}
end
function TableToCFrame(t,mode)
&#9;return mode and CFrame.Angles(t[1],t[2],t[3])+Vector3.new(t[4],t[5],t[6])*BodyScale or CFrame.Angles(t[1],t[2],t[3])*CFrame.new(t[4]*BodyScale,t[5]*BodyScale,t[6]*BodyScale)
end

Joints={}
Joint={
&#9;new=function(id,part0,part1,c0,c1,mode)
&#9;&#9;if not id or not part0 or not part1 or not c0 or not c1 then return; end
&#9;&#9;local w=Instance.new(&quot;Weld&quot;,part0)
&#9;&#9;w.Name=id
&#9;&#9;w.Part0,w.Part1=part0,part1
&#9;&#9;w.C0,w.C1=c0:inverse(),c1
&#9;&#9;local joint={id,w,part0,part1,c0,c1,{0,0,0,0,0,0},nil,0,mode,Set=Joint.Set}
&#9;&#9;setmetatable(joint,Joint.mt)
&#9;&#9;Joints[id]=joint
&#9;end;
&#9;Set=function(joint,t)
&#9;&#9;joint.Weld.C0=(TableToCFrame(t,joint.Mode)*joint.C0):inverse()
&#9;&#9;joint.Table=t -- This should solve the multiverse
&#9;end;
&#9;indexkeys={Id=1,Weld=2,Part0=3,Part1=4,C0=5,C1=6,Table=7,Controller=8,Priority=9,Mode=10};
&#9;mt={
&#9;&#9;__index=function(joint,key) -- Case statements are for noobs
&#9;&#9;&#9;return rawget(joint,Joint.indexkeys[key])
&#9;&#9;end;&#9;
&#9;&#9;__setindex=function(joint,key,value)
&#9;&#9;&#9;rawset(joint,Joint.indexkeys[key],value or joint[key])
&#9;&#9;end
&#9;}
}

