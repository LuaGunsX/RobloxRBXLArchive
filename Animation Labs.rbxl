function ToPose(pose,framecount,framelength,priority,controller)
&#9;-- Defaults
&#9;local framecount=framecount
&#9;&#9;or 8
&#9;local framelength=framelength
&#9;&#9;or 1/28
&#9;local priority=priority
&#9;&#9;or 1
&#9;local controller=controller
&#9;&#9;or {false,false}
&#9;-- These will contain joints that need to be moved
&#9;local joints={}
&#9;local start_pose={}
&#9;do -- Finds joints that need to be moved
&#9;&#9;local total=0
&#9;&#9;for k,v in pairs(pose) do
&#9;&#9;&#9;if Joints[k].Priority&lt;=priority and not TablesAreEqual(Joints[k].Table,v) then
&#9;&#9;&#9;&#9;joints[k]=Joints[k]
&#9;&#9;&#9;&#9;joints[k].Controller,joints[k].Priority=controller,priority
&#9;&#9;&#9;&#9;start_pose[k]=joints[k].Table
&#9;&#9;&#9;&#9;total=total+1
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;if total==0 then return wait(1/32) and true; end -- Returns true if all joints are already in place
&#9;end
&#9;do -- Animation
&#9;&#9;for alpha=0,1,1/framecount do
&#9;&#9;&#9;-- Check for pause
&#9;&#9;&#9;while controller[2] and not controller[1] do wait(.0625); end
&#9;&#9;&#9;-- Check for abort
&#9;&#9;&#9;if controller[1] then
&#9;&#9;&#9;&#9;for k,j in pairs(joints) do
&#9;&#9;&#9;&#9;&#9;if j.Controller==controller then
&#9;&#9;&#9;&#9;&#9;&#9;j.Controller,j.Priority=nil,0
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;return
&#9;&#9;&#9;end
&#9;&#9;&#9;-- Set joints
&#9;&#9;&#9;local beta=1-alpha
&#9;&#9;&#9;for k,j in pairs(joints) do
&#9;&#9;&#9;&#9;if j.Priority==priority and j.Controller==controller or j.Priority&lt;priority then
&#9;&#9;&#9;&#9;&#9;j:Set(LerpTables(pose[k],start_pose[k],alpha,beta))
&#9;&#9;&#9;&#9;&#9;j.Controller,j.Priority=controller,priority
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;&#9;wait(framelength)
&#9;&#9;end
&#9;end
&#9;do -- Return joints to previous values
&#9;&#9;for k,j in pairs(joints) do
&#9;&#9;&#9;if j.Controller==controller then
&#9;&#9;&#9;&#9;j.Controller,j.Priority=nil,0
&#9;&#9;&#9;&#9;--j:Set(pose[k]) -- shouldn&apos;t be necessary
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end
&#9;return true
end

function SustainPose(pose,frequency,framecount,framelength,priority,controller)
&#9;-- Defaults
&#9;local controller=controller
&#9;&#9;or {false,false}
&#9;local frequency=frequency
&#9;&#9;or 1/16
&#9;-- Loop
&#9;while not controller[1] do
&#9;&#9;ToPose(pose,framecount,framelength,priority,controller)
&#9;&#9;wait(frequency)
&#9;end
end


